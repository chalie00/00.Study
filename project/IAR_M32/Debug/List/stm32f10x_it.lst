   1347          /*******************************************************************************
   1348          * Function Name  : Rx_Main_Comm_Handling
   1349          *******************************************************************************/
   1350          void Rx_Main_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   1351          {
   1352          	// 첫번째 바이트 체크 (Sync Byte Check)
   1353          	if(u_sync == Rx_SYNC_BYTE)	
   1354          	{
   1355          		stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1356          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1357          		stUSART1.RX_BUF_INDEX++;
   1358          		
   1359          		#if(ERROR_CHECK_BUF)
   1360          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1361          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1362          		#endif
   1363          	}
   1364          
   1365          	// 두번째 바이트 체크 (Sync & Byte Check)
   1366          	if(u_sync == Rx_SYNC_AND_BYTE)
   1367          	{
   1368          		#if(ERROR_CHECK_BUF)	
   1369          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1370          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1371          		#endif		
   1372          		
   1373          		// 두번째 명령어 체크 (Sync & Byte Check)
   1374          		if(stUSART1.RX_BUF_INDEX == 0x01)
   1375          		{
   1376          			switch(stUSART1.RX_ENABLE)
   1377          			{
   1378          				case TILT_SECOND_BYTE:
   1379          					Rx_Main_Second_Comm_Check(u_buf, TILT_SECOND_BYTE);
   1380          					break;
   1381          
   1382          				case IP_SONY_SECOND_BYTE:
   1383          					Rx_Main_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   1384          					break;
   1385          					
   1386          				case IP_TSM_SECOND_BYTE:
   1387          					Rx_Main_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   1388          					break;
   1389          
   1390          				default:
   1391          					break;
   1392          			}
   1393          		}	
   1394          	}
   1395          
   1396          	// 세번째 바이트 체크 (Data Length)
   1397          	if(u_sync == Rx_LENGTH_BYTE)
   1398          	{
   1399          		#if(ERROR_CHECK_BUF)	
   1400          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1401          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1402          		#endif
   1403          		
   1404          		if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1405          		{
   1406          			// PELCO-D Command 예외처리
   1407          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1408          			{
   1409          				stUSART1.RX_BUF_LENGTH = 0x02;
   1410          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1411          				
   1412          				stUSART1.RX_BUF[2] =  u_buf;
   1413          				
   1414          				stUSART1.RX_BUF_INDEX++;
   1415          			}					
   1416          		}
   1417          		else
   1418          		{
   1419          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   1420          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1421          			{
   1422          				stUSART1.RX_BUF_LENGTH = u_buf;
   1423          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1424          				
   1425          				stUSART1.RX_BUF[2] =  u_buf;
   1426          				
   1427          				stUSART1.RX_BUF_INDEX++;
   1428          			}
   1429          		}
   1430          	}	
   1431          
   1432          	// 데이터 누적
   1433          	if(u_sync == Rx_DATA_BYTE)
   1434          	{
   1435          		#if(ERROR_CHECK_BUF)
   1436          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1437          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1438          		#endif		
   1439          		
   1440          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1441          
   1442          		stUSART1.RX_BUF_INDEX++;	
   1443          		
   1444          		if(stUSART1.RX_BUF_INDEX == stUSART1.RX_BUF_MAX)
   1445          		{
   1446          			// PELCO-D Command 예외처리
   1447          			if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1448          			{
   1449          				Rx_Pelco_Data_Check();
   1450          			}
   1451          			else
   1452          			{
   1453          				if((stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   1454          				{
   1455          					if(stSYS.START == ACTIVE)
   1456          					{
   1457          						stUSART1.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1458          						stUSART1.RX_PARITY_CNT++;
   1459          						
   1460          						// DEBUG 데이터 체크 ======================================================/
   1461          						//stERROR.PARITY_CNT_CHECK++;
   1462          						//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   1463          						//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
   1464          						// DEBUG 데이터 체크 ===================================================END/
   1465          						
   1466          						//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   1467          					}
   1468          					else
   1469          					{
   1470          						Boot_Status_Check_Parser();
   1471          					}
   1472          
   1473          					stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1474          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   1475          				}
   1476          				else
   1477          				{
   1478          					stUSART1.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1479          					stUSART1.RX_ERROR_CNT++;
   1480          				}
   1481          			}
   1482          			
   1483          			stUSART1.RX_BUF_INDEX = 0x00;
   1484          			stUSART1.RX_BUF_LENGTH = 0x00;
   1485          			
   1486          			stUSART1.RX_ENABLE = 0x00;
   1487          		}
   1488          	}		
   1489          }
