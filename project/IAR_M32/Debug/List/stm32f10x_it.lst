   1728          /*******************************************************************************
   1729          * Function Name  : USART2_IRQHandler
   1730          * Description    : This function handles USART2 interrupt request.
   1731          * Input           : MDIN - PA3 USART2_RX
   1732          * Output         : MDIN - PA2 USART2_TX
   1733          *******************************************************************************/
   1734          void USART2_IRQHandler(void)
   1735          {
   1736          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1737          
   1738          	// =======================================
   1739          	// USART2 TX Process 
   1740          	// =======================================
   1741          	if (USART_GetITStatus(USART2, USART_IT_TC) != RESET)
   1742          	{
   1743          
   1744          		if(stUSART2.TX_CNT < stUSART2.TX_MAX)
   1745          		{
   1746          			Tx_Buf = stUSART2.TX_BUF[stUSART2.TX_CNT];
   1747          			stUSART2.TX_CNT++;				
   1748          
   1749          			USART_SendData(USART2, Tx_Buf);
   1750          		}
   1751          		else
   1752          		{
   1753          			stUSART2.TX_CNT = 0x00;
   1754          			stUSART2.TX_MAX = 0x00;
   1755          		}
   1756          
   1757          		USART_ClearITPendingBit(USART2, USART_IT_TC);
   1758          
   1759          	}
   1760          
   1761          	// =======================================
   1762          	// USART2 RX Process
   1763          	// =======================================
   1764          	if (USART_GetITStatus(USART2, USART_IT_RXNE) != RESET)
   1765          	{
   1766          		Rx_Buf = USART_ReceiveData(USART2);
   1767          	}     
   1768          }
