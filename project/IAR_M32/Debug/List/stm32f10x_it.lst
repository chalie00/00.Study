   1541          /*******************************************************************************
   1542          * Function Name  : USART1_IRQHandler
   1543          * Description    : This function handles USART1 interrupt request.
   1544          * Input           : Main - PA10 USART1_RX
   1545          * Output         : Main - PA9 USART1_TX
   1546          *******************************************************************************/
   1547          void USART1_IRQHandler(void)
   1548          {
   1549          	u8 Rx_Buf = 0x00;
   1550          	
   1551            	// =======================================
   1552            	// USART1 TX Process : TILT -> MAIN Board
   1553            	// =======================================
   1554          	if (USART_GetITStatus(USART1, USART_IT_TC) != RESET)
   1555          	{
   1556          		if (stUSART1.TX_CNT < stUSART1.TX_MAX) 
   1557          		{
   1558          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1559          		}
   1560          		else 
   1561          		{
   1562          			stUSART1.TX_MAX = 0;
   1563          			stUSART1.TX_CNT = 0;
   1564          		}
   1565          		
   1566          		USART_ClearITPendingBit(USART1, USART_IT_TC);
   1567          	}
   1568          
   1569            	// =======================================
   1570            	// USART1 RX Process : TILT <- MAIN Board
   1571            	// =======================================
   1572          	if (USART_GetITStatus(USART1, USART_IT_RXNE) != RESET)
   1573          	{
   1574          		Rx_Buf = USART_ReceiveData(USART1);
   1575          
   1576          		Main_Protocol_Data_Pasher(Rx_Buf);
   1577          		
   1578          		//Sony_Data_Pasher(Rx_Buf);
   1579          
   1580          		//if(stUSART_ERROR.RX_CNT == UART4_RX_BUF_MAX) stUSART_ERROR.RX_CNT = 0;
   1581          
   1582          		//stUSART_ERROR.RX_BUF[stUSART_ERROR.RX_CNT++] = Rx_Buf;
   1583          	}
   1584          }
