   1260          void Rx_Main_Second_Comm_Check(u8 u_buf, u8 u_andbyte)
   1261          {
   1262          	if(u_buf == u_andbyte)
   1263          	{
   1264          		stUSART1.RX_BUF[1] =  u_buf;
   1265          		stUSART1.RX_BUF_INDEX++;
   1266          		stUSART1.RX_BUF_LENGTH = 0x01;
   1267          	}
   1268          	else
   1269          	{
   1270          		stUSART1.RX_BUF_INDEX = 0x00;					// Index Clear
   1271          		stUSART1.RX_BUF[0] = 0x00;						// Buffer Clear
   1272          		stUSART1.RX_ENABLE = 0x00;						// Command Check Byte Clear
   1273          	}	
   1274          }
