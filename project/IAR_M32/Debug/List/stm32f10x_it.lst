   2423          /*******************************************************************************
   2424          * Function Name  : Rx_Usart_Comm_Handling
   2425          *******************************************************************************/
   2426          void Rx_Usart_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   2427          {
   2428          	// 첫번째 바이트 체크 (Sync Byte Check)
   2429          	if(u_sync == Rx_SYNC_BYTE)	
   2430          	{
   2431          		stUART5.RX_BUF_INDEX = 0x00;	// Index Init
   2432          		stUART5.RX_BUF[stUART5.RX_BUF_INDEX] = u_buf;
   2433          		stUART5.RX_BUF_INDEX++;
   2434          		
   2435          		#if(ERROR_CHECK_BUF)
   2436          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   2437          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   2438          		#endif
   2439          	}
   2440          
   2441          	// 두번째 바이트 체크 (Sync & Byte Check)
   2442          	if(u_sync == Rx_SYNC_AND_BYTE)
   2443          	{
   2444          		#if(ERROR_CHECK_BUF)	
   2445          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   2446          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   2447          		#endif		
   2448          		
   2449          		// 두번째 명령어 체크 (Sync & Byte Check)
   2450          		if(stUART5.RX_BUF_INDEX == 0x01)
   2451          		{
   2452          			switch(stUART5.RX_ENABLE)
   2453          			{
   2454          				case IP_SONY_SECOND_BYTE:
   2455          					Rx_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   2456          					break;
   2457          
   2458          				case IP_TSM_SECOND_BYTE:
   2459          					Rx_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   2460          					break;
   2461          
   2462          				case IP_DRS_SECOND_BYTE:
   2463          					Rx_Second_Comm_Check(u_buf, IP_DRS_SECOND_BYTE);
   2464          					break;
   2465          
   2466          				case IP_TCM_SECOND_BYTE:
   2467          					Rx_Second_Comm_Check(u_buf, IP_TCM_SECOND_BYTE);
   2468          					break;
   2469          					
   2470          				case IP_PTZ_SECOND_BYTE:
   2471          					Rx_Second_Comm_Check(u_buf, IP_PTZ_SECOND_BYTE);
   2472          					break;
   2473          
   2474          				case IP_DATA_SECOND_BYTE:
   2475          					Rx_Second_Comm_Check(u_buf, IP_DATA_SECOND_BYTE);
   2476          					break;	
   2477          
   2478          				case IP_OPTION_SECOND_BYTE:
   2479          					Rx_Second_Comm_Check(u_buf, IP_OPTION_SECOND_BYTE);
   2480          					break;		
   2481          
   2482          				case IP_PELCO_CHK_BYTE:
   2483          					Rx_Pelco_Address_Check(u_buf);
   2484          					break;
   2485          
   2486          				default:
   2487          					break;
   2488          			}
   2489          		}	
   2490          	}
   2491          
   2492          	// 세번째 바이트 체크 (Data Length)
   2493          	if(u_sync == Rx_LENGTH_BYTE)
   2494          	{
   2495          		#if(ERROR_CHECK_BUF)	
   2496          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   2497          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   2498          		#endif
   2499          		
   2500          		if(stUART5.RX_ENABLE == IP_PELCO_CHK_BYTE)
   2501          		{
   2502          			// PELCO-D Command 예외처리
   2503          			if(stUART5.RX_BUF_INDEX == 0x02)
   2504          			{
   2505          				stUART5.RX_BUF_LENGTH = 0x02;
   2506          				stUART5.RX_BUF_MAX = stUART5.RX_BUF_LENGTH + 5;
   2507          				
   2508          				stUART5.RX_BUF[2] =  u_buf;
   2509          				
   2510          				stUART5.RX_BUF_INDEX++;
   2511          			}					
   2512          		}
   2513          		else
   2514          		{
   2515          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   2516          			if(stUART5.RX_BUF_INDEX == 0x02)
   2517          			{
   2518          				stUART5.RX_BUF_LENGTH = u_buf;
   2519          				stUART5.RX_BUF_MAX = stUART5.RX_BUF_LENGTH + 5;
   2520          				
   2521          				stUART5.RX_BUF[2] =  u_buf;
   2522          				
   2523          				stUART5.RX_BUF_INDEX++;
   2524          			}
   2525          		}
   2526          	}	
   2527          
   2528          	// 데이터 누적
   2529          	if(u_sync == Rx_DATA_BYTE)
   2530          	{
   2531          		#if(ERROR_CHECK_BUF)
   2532          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   2533          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   2534          		#endif		
   2535          		
   2536          		stUART5.RX_BUF[stUART5.RX_BUF_INDEX] = u_buf;
   2537          
   2538          		stUART5.RX_BUF_INDEX++;	
   2539          		
   2540          		if(stUART5.RX_BUF_INDEX == stUART5.RX_BUF_MAX)
   2541          		{
   2542          			// PELCO-D Command 예외처리
   2543          			if(stUART5.RX_ENABLE == IP_PELCO_CHK_BYTE)
   2544          			{
   2545          				Rx_Pelco_Data_Check();
   2546          			}
   2547          			else
   2548          			{
   2549          				if((stUART5.RX_BUF[stUART5.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   2550          				{
   2551          					stUART5.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터)
   2552          					stUART5.RX_PARITY_CNT++;
   2553          
   2554          					// DEBUG 데이터 체크 ======================================================/
   2555          					//stERROR.PARITY_CNT_CHECK++;
   2556          					//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   2557          					//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
   2558          					// DEBUG 데이터 체크 ===================================================END/
   2559          					
   2560          					//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   2561          					
   2562          					stUART5.RX_BUF_INDEX = 0x00;	// Index Init
   2563          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   2564          				}
   2565          				else
   2566          				{
   2567          					stUART5.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터)
   2568          					stUART5.RX_ERROR_CNT++;
   2569          				}
   2570          			}
   2571          			
   2572          			stUART5.RX_BUF_INDEX = 0x00;
   2573          			stUART5.RX_BUF_LENGTH = 0x00;
   2574          			
   2575          			stUART5.RX_ENABLE = 0x00;
   2576          		}
   2577          	}		
   2578          }
