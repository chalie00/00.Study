   2268          // ============================================================================
   2269          // USART3 : UART4 Data Pasher (TSM)
   2270          // ============================================================================
   2271          void UART4_Data_Pasher(u8 u_Buf, u8 u_Index)
   2272          {
   2273          	if((u_Index == INDEX_NON) || (u_Index == INDEX_THERMAL))
   2274          	{
   2275          		TSM_Data_Pasher(u_Buf);
   2276          	}
   2277          
   2278          	if((u_Index == INDEX_NON) || (u_Index == INDEX_COLOR))
   2279          	{
   2280          		SONY_Data_Pasher(u_Buf);
   2281          	}
   2282          }
