   1732          /*******************************************************************************
   1733          * Function Name  : USART3_IRQHandler
   1734          * Description    : This function handles USART1 interrupt request.
   1735          * Input           : Camera - PB11 USART3_RX
   1736          * Output         : Camera - PB10 USART3_TX
   1737          *******************************************************************************/
   1738          void USART3_IRQHandler(void)
   1739          {
   1740          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1741          
   1742            	// =======================================
   1743            	// USART3 TX Process 
   1744            	// =======================================
   1745          	if (USART_GetITStatus(USART3, USART_IT_TC) != RESET)
   1746          	{ 
   1747          		USART_SendData(USART3, Tx_Buf);
   1748          		
   1749          		USART_ClearITPendingBit(USART3, USART_IT_TC);
   1750          	}
   1751          	
   1752            	// =======================================
   1753            	// USART3 RX Process
   1754            	// =======================================
   1755          	if (USART_GetITStatus(USART3, USART_IT_RXNE) != RESET)
   1756          	{
   1757          		Rx_Buf = USART_ReceiveData(USART3);
   1758          	}
   1759          
   1760          }
