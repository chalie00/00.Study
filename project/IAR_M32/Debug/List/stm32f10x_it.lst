   1890          								stUART4.RX_REQUEST_CMD = 0x00;
   1891          							}
   1892          
   1893          							break;
   1894          
   1895          						case TSM_CALL_OPTICAL_ZOOM_POSITION:
   1896          							break;
   1897          
   1898          						case TSM_CALL_TOTAL_ZOOM_POSITION:
