   1774          /*******************************************************************************
   1775          * Function Name  : TSM_Data_Pasher
   1776          *******************************************************************************/
   1777          void TSM_Data_Pasher(u8 u_Buf)
   1778          {
   1779          	u8 i = 0;
   1780          	u8 u_Data_P1 = 0x00, u_Data_P2 = 0x00;
   1781          	u16 u16_Convert_Data = 0x0000;
   1782          	u8 u_Convert_Data  = 0x00;	
   1783          
   1784          	if(stUART4.RX_ENABLE == 0x01)
   1785          	{
   1786          		stUART4.RX_BUF[stUART4.RX_CNT++] = u_Buf;
   1787          
   1788          		if(stUART4.RX_BUF_MAX == 0x00)
   1789          		{
   1790          			switch(stUART4.RX_BUF[1])
   1791          			{
   1792          				case TSM_CALL_ALL_TEMPERATURE:
   1793          					stUART4.RX_BUF_MAX = 0x23;
   1794          					break;
   1795          
   1796          				case TSM_CALL_FIRMWARE_VERSION:
   1797          					stUART4.RX_BUF_MAX = 0x08;
   1798          					break;
   1799          
   1800          				case TCM_COOLER_RUNTIME_INQ:
   1801          					if(stINFO.THERMAL_MODEL == TCM_640)
   1802          					{
   1803          						stUART4.RX_BUF_MAX = 0x07;
   1804          					}
   1805          					break;
   1806          
   1807          				default:
   1808          					stUART4.RX_BUF_MAX = 0x06;
   1809          					break;
   1810          			}
   1811          		}
   1812          
   1813          		// Rx Data Check & Initialization
   1814          		if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf == 0xAF))
   1815          		{
   1816          			switch(stUART4.RX_BUF_MAX)
   1817          			{
   1818          				case 0x06:
   1819          
   1820          					switch(stUART4.RX_REQUEST_CMD)
   1821          					{
   1822          						case TSM_CALL_MODULE_STATUS:
   1823          
   1824          							if(stUART4.RX_BUF[1] == TSM_CALL_MODULE_STATUS)
   1825          							{
   1826          								u_Data_P1 = stUART4.RX_BUF[2];
   1827          
   1828          								if(u_Data_P1) 
   1829          								{
   1830          									stTSM.MODULE_ENABLE = 0x01;
   1831          								}
   1832          								
   1833          								stTSM.MODULE_STATUS = u_Data_P1;
   1834          								stINFO.MODULE_TYPE = TSM_MODULE;
   1835          
   1836          								stUART4.RX_REQUEST_CMD = 0x00;
   1837          							}
   1838          								
   1839          							break;
   1840          							
   1841          						case TSM_CALL_DIGITAL_ZOOM_POSITION:
   1842          
   1843          							if(stUART4.RX_BUF[1] == TSM_CALL_DIGITAL_ZOOM_POSITION)
   1844          							{
   1845          								u_Data_P1 = stUART4.RX_BUF[4];
   1846          								u16_Convert_Data |= u_Data_P1;
   1847          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1848          
   1849          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1850          
   1851          								stUART4.RX_REQUEST_CMD = 0x00;
   1852          							}
   1853          
   1854          							break;
   1855          
   1856          						case TSM_CALL_OPTICAL_ZOOM_POSITION:
   1857          							break;
   1858          
   1859          						case TSM_CALL_TOTAL_ZOOM_POSITION:
   1860          							
   1861          							if(stUART4.RX_BUF[1] == TSM_CALL_TOTAL_ZOOM_POSITION)
   1862          							{
   1863          								u_Data_P1 = stUART4.RX_BUF[2];
   1864          								u_Data_P2 = stUART4.RX_BUF[3];
   1865          
   1866          								u16_Convert_Data = u_Data_P1 << 8;
   1867          								u16_Convert_Data |= u_Data_P2;
   1868          
   1869          								stTSM.ZOOM_CNT = u16_Convert_Data;
   1870          
   1871          								stUART4.RX_REQUEST_CMD = 0x00;
   1872          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_TOTAL_ZOOM_POSITION;
   1873          							}
   1874          							
   1875          							break;
   1876          
   1877          						case TSM_CALL_FOCUS_POSITION:
   1878          
   1879          							if(stUART4.RX_BUF[1] == TSM_CALL_FOCUS_POSITION)
   1880          							{
   1881          								u_Data_P1 = stUART4.RX_BUF[2];
   1882          								u_Data_P2 = stUART4.RX_BUF[3];
   1883          
   1884          								u16_Convert_Data = u_Data_P1 << 8;
   1885          								u16_Convert_Data |= u_Data_P2;
   1886          
   1887          								stTSM.FOCUS_CNT = u16_Convert_Data;
   1888          
   1889          								stUART4.RX_REQUEST_CMD = 0x00;
   1890          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_FOCUS_POSITION;								
   1891          							}
   1892          							
   1893          							break;
   1894          
   1895          						case TSM_CALL_PRODUCT_IDENTIFICATION:
   1896          
   1897          							if(stUART4.RX_BUF[1] == TSM_CALL_PRODUCT_IDENTIFICATION)
   1898          							{
   1899          								// Camera Information Data Check							
   1900          								stINFO.THERMAL_MODEL = stUART4.RX_BUF[2];
   1901          								stINFO.THERMAL_LENS = stUART4.RX_BUF[4];
   1902          								
   1903          								stUART4.RX_REQUEST_CMD = 0x00;
   1904          							}
   1905          							
   1906          							break;
   1907          
   1908          						case TSM_CALL_CAMERA_INFORMATION:
   1909          
   1910          							if(stUART4.RX_BUF[1] == TSM_CALL_CAMERA_INFORMATION)
   1911          							{
   1912          								// Camera Information Data Check
   1913          								stINFO.THERMAL_VER_HIGH = stUART4.RX_BUF[3];
   1914          								stINFO.THERMAL_VER_LOW = stUART4.RX_BUF[4];
   1915          								
   1916          								stUART4.RX_REQUEST_CMD = 0x00;
   1917          							}
   1918          							
   1919          							break;
   1920          
   1921          						case TSM_CALL_FPGA_VERSION:
   1922          
   1923          							if(stUART4.RX_BUF[1] == TSM_CALL_FPGA_VERSION)
   1924          							{
   1925          								stINFO.FPGA_VERSION_HIGH = stUART4.RX_BUF[2];
   1926          								stINFO.FPGA_VERSION_LOW = stUART4.RX_BUF[3];
   1927          
   1928          								stUART4.RX_REQUEST_CMD = 0x00;
   1929          							}
   1930          							
   1931          							break;
   1932          							
   1933          						case TSM_CALL_TEMPERATURE_MODE:
   1934          
   1935          							if(stUART4.RX_BUF[1] == TSM_CALL_TEMPERATURE_MODE)
   1936          							{
   1937          								// Camera Information Data Check
   1938          								stINFO.THERMAL_TEMP_MODE = stUART4.RX_BUF[2];
   1939          								
   1940          								stUART4.RX_REQUEST_CMD = 0x00;
   1941          							}
   1942          							
   1943          							break;
   1944          
   1945          						case TCM_TOTAL_ZOOM_POSITION_INQ:
   1946          
   1947          							if(stUART4.RX_BUF[1] == TCM_TOTAL_ZOOM_POSITION_INQ)
   1948          							{
   1949          								u_Data_P1 = stUART4.RX_BUF[2];
   1950          								u_Data_P2 = stUART4.RX_BUF[3];
   1951          
   1952          								u16_Convert_Data = u_Data_P1 << 8;
   1953          								u16_Convert_Data |= u_Data_P2;
   1954          
   1955          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1956          
   1957          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1958          
   1959          								stUART4.RX_REQUEST_CMD = 0x00;
   1960          							}
   1961          							
   1962          							break;
   1963          
   1964          						case TCM_GET_FW_UPDATE_INQ:
   1965          
   1966          							if(stUART4.RX_BUF[1] == TCM_GET_FW_UPDATE_INQ)
   1967          							{
   1968          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[2];
   1969          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[3];
   1970          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   1971          									
   1972          								stUART4.RX_REQUEST_CMD = 0x00;
   1973          							}
   1974          							
   1975          							break;
   1976          
   1977          						case TCM_GET_ENGINE_VERSION:
   1978          
   1979          							if(stUART4.RX_BUF[1] == TCM_GET_ENGINE_VERSION)
   1980          							{
   1981          								stINFO.THERMAL_ENGINE_VER_HW = stUART4.RX_BUF[2];
   1982          								stINFO.THERMAL_ENGINE_VER_FW = stUART4.RX_BUF[3];
   1983          
   1984          								stUART4.RX_REQUEST_CMD = 0x00;
   1985          							}
   1986          							
   1987          							break;		
   1988          
   1989          						case TCM_FOCUS_POSITION_INQ:
   1990          
   1991          							if(stUART4.RX_BUF[1] == TCM_FOCUS_POSITION_INQ)
   1992          							{
   1993          								u_Data_P1 = stUART4.RX_BUF[2];
   1994          								u_Data_P2 = stUART4.RX_BUF[3];
   1995          
   1996          								u16_Convert_Data = u_Data_P1 << 8;
   1997          								u16_Convert_Data |= u_Data_P2;
   1998          
   1999          								stTSM.FOCUS_CNT = u16_Convert_Data;
   2000          
   2001          								stUART4.RX_REQUEST_CMD = 0x00;
   2002          							}
   2003          
   2004          							break;	
   2005          
   2006          						default:
   2007          							break;
   2008          					}
   2009          						
   2010          					break;
   2011          
   2012          				case 0x07:
   2013          					switch(stUART4.RX_REQUEST_CMD)
   2014          					{
   2015          						case TCM_COOLER_RUNTIME_INQ:
   2016          
   2017          								stINFO.THERMAL_ENGINE_TIME_DAY= 0x0000;
   2018          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[2]<<8);
   2019          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[3]&0xFF);
   2020          
   2021          								stINFO.THERMAL_ENGINE_TIME_HOUR = 0x0000;
   2022          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[4]<<8);
   2023          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[5]&0xFF);
   2024          
   2025          								stINFO.THERMAL_ENGINE_DAY_HIGH = stUART4.RX_BUF[2];
   2026          								stINFO.THERMAL_ENGINE_DAY_LOW = stUART4.RX_BUF[3];
   2027          								stINFO.THERMAL_ENGINE_HOUR_HIGH = stUART4.RX_BUF[4];
   2028          								stINFO.THERMAL_ENGINE_HOUR_LOW = stUART4.RX_BUF[5];								
   2029          								
   2030          								stINFO.TCM640_ENGINE_RUNTIME_COMPLETE = TRUE;
   2031          								
   2032          								stUART4.RX_REQUEST_CMD = 0x00;	
   2033          
   2034          							break;
   2035          					}
   2036          					break;
   2037          					
   2038          				case 0x08:
   2039          					
   2040          					switch(stUART4.RX_REQUEST_CMD)
   2041          					{
   2042          						case TSM_CALL_FIRMWARE_VERSION:
   2043          
   2044          							if(stUART4.RX_BUF[1] == TSM_CALL_FIRMWARE_VERSION)
   2045          							{
   2046          								// Camera Information Data Check
   2047          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   2048          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[5];
   2049          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[6];
   2050          								
   2051          								stUART4.RX_REQUEST_CMD = 0x00;
   2052          							}
   2053          							
   2054          							break;
   2055          
   2056          						default:
   2057          							break;
   2058          					}
   2059          							
   2060          					break;
   2061          					
   2062          				case 0x23:
   2063          					
   2064          					//stTEMP.TEMPERATURE_READ_COMPLETE = 0x01;	// 온도 체크 데이터 수신 완료.
   2065          					
   2066          					break;
   2067          
   2068          				default:
   2069          					break;
   2070          			}
   2071          		
   2072          			for(i=0;i<stUART4.RX_BUF_MAX;i++)
   2073          			{
   2074          				//stTEMP.RX_BUF[i] = stUART4.RX_BUF[i];
   2075          				stUART4.RX_BUF[i] = 0x00;
   2076          			}
   2077          
   2078          			stUART4.RX_ENABLE = 0x00;
   2079          			stUART4.RX_CNT = 0x00;
   2080          			stUART4.RX_BUF_MAX = 0x00;
   2081          			stUART4.RX_PARITY = 0x01;
   2082          			
   2083          		}
   2084          		else if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf != 0xAF))
   2085          		{
   2086          			for(i=0;i<stUART4.RX_CNT;i++)
   2087          			{
   2088          				stUART4.RX_BUF[i] = 0x00;
   2089          			}
   2090          
   2091          			stUART4.RX_ENABLE = 0x00;
   2092          			stUART4.RX_CNT = 0x00;
   2093          			stUART4.RX_BUF_MAX = 0x00;
   2094          			stUART4.RX_REQUEST_ERROR = 0x01;			
   2095          		}
   2096          	}
   2097          
   2098          	if((stUART4.RX_ENABLE == 0x00) && (u_Buf == 0xA0))
   2099          	{
   2100          		stUART4.RX_BUF[0] = u_Buf;
   2101          		stUART4.RX_CNT++;
   2102          		stUART4.RX_ENABLE = 0x01;
   2103          	}
   2104          }
