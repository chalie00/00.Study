   2000          							
   2001          							break;
   2002          
   2003          						case TCM_GET_FW_UPDATE_INQ:
   2004          
   2005          							if(stUART4.RX_BUF[1] == TCM_GET_FW_UPDATE_INQ)
   2006          							{
   2007          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[2];
