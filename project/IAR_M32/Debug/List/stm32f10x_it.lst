   2500          					//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   2501          					
   2502          					stUART5.RX_BUF_INDEX = 0x00;	// Index Init
   2503          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   2504          				}
   2505          				else
   2506          				{
   2507          					stUART5.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터)
   2508          					stUART5.RX_ERROR_CNT++;
   2509          				}
   2510          			}
   2511          			
   2512          			stUART5.RX_BUF_INDEX = 0x00;
   2513          			stUART5.RX_BUF_LENGTH = 0x00;
