   2107          /*******************************************************************************
   2108          * Function Name  : Rx_Sony_Inquiry_Command
   2109          *******************************************************************************/
   2110          void SONY_Data_Pasher(u8 u_buf)
   2111          {
   2112          	u16 u_CZoomData = 0x0000, u_CFocusData = 0x0000;
   2113          
   2114          	// Data Save
   2115          	if(stSONY.RX_CNT >= 0x02)
   2116          	{
   2117          		stSONY.RX_BUF[stSONY.RX_CNT] = u_buf;
   2118          		stSONY.RX_CNT++;
   2119          	}
   2120          
   2121          	if(stSONY.RX_CNT == 0x04)
   2122          	{
   2123          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[3] == SONY_BYTE_END))
   2124          		{
   2125          			switch(stUART4.RX_REQUEST_CMD)
   2126          			{
   2127          				case SONY_POWER_INQ:
   2128          
   2129          					stSONY.MODULE_STATUS = stSONY.RX_BUF[2];
   2130          
   2131          					switch(stSONY.MODULE_STATUS)
   2132          					{
   2133          						case 0x02:
   2134          							stSONY.MODULE_ENABLE = 1;
   2135          							break;
   2136          
   2137          						case 0x03:
   2138          							stSONY.MODULE_ENABLE = 0;
   2139          							break;
   2140          
   2141          						default:
   2142          							break;
   2143          					}
   2144          
   2145          					stINFO.MODULE_TYPE = SONY_MODULE;
   2146          					
   2147          					break;
   2148          
   2149          				default:
   2150          					break;
   2151          			}
   2152          
   2153          			SONY_Data_Clear();
   2154          			
   2155          			stSONY.RX_CNT = 0;
   2156          			stSONY.RX_END_FLAG = 0x01;	
   2157          		}
   2158          	}
   2159          
   2160          	// Data Compare Check
   2161          	if(stSONY.RX_CNT == 0x07)
   2162          	{
   2163          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[6] == SONY_BYTE_END))
   2164          		{
   2165          			switch(stUART4.RX_REQUEST_CMD)
   2166          			{
   2167          				case SONY_CALL_ZOOM_POSITION:
   2168          					
   2169          					u_CZoomData = stSONY.RX_BUF[2];
   2170          					stSONY.ZOOM_CNT = u_CZoomData << 12;
   2171          					u_CZoomData = stSONY.RX_BUF[3];
   2172          					stSONY.ZOOM_CNT |= u_CZoomData << 8;
   2173          					u_CZoomData = stSONY.RX_BUF[4];
   2174          					stSONY.ZOOM_CNT |= u_CZoomData << 4;
   2175          					u_CZoomData = stSONY.RX_BUF[5];
   2176          					stSONY.ZOOM_CNT |= u_CZoomData;
   2177          
   2178          					if(stSONY.RX_BUF[6] == 0x00) 
   2179          					{
   2180          						stUART4.RX_REQUEST_ERROR = 0x01;
   2181          					}
   2182          					else 
   2183          					{
   2184          						stUART4.RX_REQUEST_CMD = 0x00;
   2185          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_ZOOM_POSITION;
   2186          					}
   2187          					
   2188          					break;
   2189          
   2190          				case SONY_CALL_FOCUS_POSITION:
   2191          					
   2192          					u_CFocusData = stSONY.RX_BUF[2];
   2193          					stSONY.FOCUS_CNT = u_CFocusData << 12;
   2194          					u_CFocusData = stSONY.RX_BUF[3];
   2195          					stSONY.FOCUS_CNT |= u_CFocusData << 8;
   2196          					u_CFocusData = stSONY.RX_BUF[4];
   2197          					stSONY.FOCUS_CNT |= u_CFocusData << 4;
   2198          					u_CFocusData = stSONY.RX_BUF[5];
   2199          					stSONY.FOCUS_CNT |= u_CFocusData;
   2200          
   2201          					if(stSONY.RX_BUF[6] == 0x00) 
   2202          					{
   2203          						stUART4.RX_REQUEST_ERROR = 0x01;
   2204          					}
   2205          					else 
   2206          					{
   2207          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_FOCUS_POSITION;
   2208          						stUART4.RX_REQUEST_CMD = 0x00;
   2209          					}
   2210          
   2211          					break;
   2212          
   2213          				default:
   2214          
   2215          					stUART4.RX_ENABLE = 0x00;
   2216          					if(stUART4.RX_REQUEST_ERROR != 0x00) stUART4.RX_REQUEST_ERROR = 0x00;
   2217          					
   2218          					break;
   2219          			}
   2220          
   2221          			SONY_Data_Clear();
   2222          			
   2223          			stSONY.RX_CNT = 0;
   2224          			stSONY.RX_END_FLAG = 0x01;
   2225          
   2226          		}
   2227          	}
   2228          	
   2229          	if(stSONY.RX_END_FLAG == 0x00)
   2230          	{
   2231          		// Second Data Check
   2232          		if((u_buf == SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2233          		{
   2234          			stSONY.RX_BUF[1] = SONY_BYTE_2;
   2235          			stSONY.RX_CNT++;
   2236          		}
   2237          		else if((u_buf != SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2238          		{
   2239          			stSONY.RX_BUF[0] = 0x00;
   2240          			stSONY.RX_CNT = 0;
   2241          		}
   2242          
   2243          		// First Data Check
   2244          		if((u_buf == SONY_BYTE_1) && (stSONY.RX_CNT == 0))
   2245          		{
   2246          			stSONY.RX_BUF[0] = SONY_BYTE_1;
   2247          			stSONY.RX_CNT++;
   2248          		}
   2249          	}
   2250          	else
   2251          	{
   2252          		stSONY.RX_END_FLAG = 0x00;
   2253          	}
   2254          
   2255          	if(stSONY.RX_CNT >= SONY_RX_BUF_MAX)
   2256          	{
   2257          		stSONY.RX_CNT = 0;
   2258          		SONY_Data_Clear();
   2259          		
   2260          	}
   2261          }
