   2014          							break;
   2015          
   2016          						case TCM_GET_ENGINE_VERSION:
   2017          
   2018          							if(stUART4.RX_BUF[1] == TCM_GET_ENGINE_VERSION)
   2019          							{
   2020          								stINFO.THERMAL_ENGINE_VER_HW = stUART4.RX_BUF[2];
   2021          								stINFO.THERMAL_ENGINE_VER_FW = stUART4.RX_BUF[3];
