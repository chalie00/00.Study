   2097          					}
   2098          							
   2099          					break;
   2100          					
   2101          				case 0x23:
   2102          					
   2103          					//stTEMP.TEMPERATURE_READ_COMPLETE = 0x01;	// 온도 체크 데이터 수신 완료.
