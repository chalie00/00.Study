    901          /*******************************************************************************
    902          * Function Name  : SYSTEM_Data_Handling
    903          *******************************************************************************/
    904          void SYSTEM_Data_Buffer(void)
    905          {
    906          	u8 i = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    907          
    908          	if(stUSART1.RX_PARITY == 0x01)
    909          	{
    910          		u_buf = stUSART1.RX_BUF[0];
    911          
    912          		switch(u_buf)
    913          		{
    914          /*
    915          			case TILT_FIRST_BYTE:
    916          
    917          				u_length = stUSART1.RX_BUF[2];
    918          
    919          				u_max = u_length + 5;
    920          
    921          				stCOOR.RX_BUF[stCOOR.RX_INDEX][0] = stCOOR.RX_INDEX + 1;
    922          
    923          				for(i=0; i<u_max; i++)
    924          				{
    925          					stCOOR.RX_BUF[stCOOR.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    926          					stUSART1.RX_BUF[i] = 0x00;
    927          				}
    928          
    929          				stCOOR.RX_INDEX++;
    930          				
    931          				break;
    932          */				
    933          			case TILT_FIRST_BYTE:
    934          			case IP_SONY_FIRST_BYTE:
    935          			case IP_TSM_FIRST_BYTE:
    936          			case IP_DRS_FIRST_BYTE:
    937          			case IP_PTZ_FIRST_BYTE:
    938          			case IP_DATA_FIRST_BYTE:
    939          			case IP_OPTION_FIRST_BYTE:
    940          
    941          				u_length = stUSART1.RX_BUF[2];
    942          
    943          				u_max = u_length + 5;
    944          
    945          				stSYS.RX_BUF[stSYS.RX_INDEX][0] = stSYS.RX_INDEX + 1;
    946          
    947          				for(i=0; i<u_max; i++)
    948          				{
    949          					stSYS.RX_BUF[stSYS.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    950          					stUSART1.RX_BUF[i] = 0x00;
    951          				}
    952          
    953          				stSYS.RX_INDEX++;
    954          
    955          				break;
    956          
    957          			default:
    958          				break;
    959          		}
    960          
    961          		stUSART1.RX_PARITY = 0x00;
    962          
    963          	}
    964          }
