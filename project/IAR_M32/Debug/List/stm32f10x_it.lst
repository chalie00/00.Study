   2266          /*******************************************************************************
   2267          * Function Name  : UART4_IRQHandler
   2268          * Description    : This function handles UART4 interrupt request.
   2269          * Input           : - PC11 UART4_RX
   2270          * Output         : - PC10 UART4_TX
   2271          *******************************************************************************/
   2272          void UART4_IRQHandler(void)
   2273          {
   2274          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   2275          
   2276            	// =======================================
   2277            	// USART4 TX Process 
   2278            	// =======================================
   2279          	if (USART_GetITStatus(UART4, USART_IT_TC) != RESET)
   2280          	{
   2281          		if(stUART4.TX_CNT < stUART4.TX_MAX)
   2282          		{
   2283          			Tx_Buf = stUART4.TX_BUF[stUART4.TX_CNT];
   2284          			stUART4.TX_CNT++;				
   2285          
   2286          			USART_SendData(UART4, Tx_Buf);
   2287          		}
   2288          		else
   2289          		{
   2290          			stUART4.TX_CNT = 0x00;
   2291          			stUART4.TX_MAX = 0x00;
   2292          		}
   2293          
