   1519          /*******************************************************************************
   1520          * Function Name  : USART1_IRQHandler
   1521          * Description    : This function handles USART1 interrupt request.
   1522          * Input           : Main - PA10 USART1_RX
   1523          * Output         : Main - PA9 USART1_TX
   1524          *******************************************************************************/
   1525          void USART1_IRQHandler(void)
   1526          {
   1527          	u8 Rx_Buf = 0x00;
   1528          	
   1529            	// =======================================
   1530            	// USART1 TX Process : TILT -> MAIN Board
   1531            	// =======================================
   1532          	if (USART_GetITStatus(USART1, USART_IT_TC) != RESET)
   1533          	{
   1534          		if (stUSART1.TX_CNT < stUSART1.TX_MAX) 
   1535          		{
   1536          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1537          		}
   1538          		else 
   1539          		{
   1540          			stUSART1.TX_MAX = 0;
   1541          			stUSART1.TX_CNT = 0;
   1542          		}
   1543          		
   1544          		USART_ClearITPendingBit(USART1, USART_IT_TC);
   1545          	}
   1546          
   1547            	// =======================================
   1548            	// USART1 RX Process : TILT <- MAIN Board
   1549            	// =======================================
   1550          	if (USART_GetITStatus(USART1, USART_IT_RXNE) != RESET)
   1551          	{
   1552          		Rx_Buf = USART_ReceiveData(USART1);
   1553          
   1554          		Main_Protocol_Data_Pasher(Rx_Buf);
   1555          		
   1556          		//Sony_Data_Pasher(Rx_Buf);
   1557          
   1558          		//if(stUSART_ERROR.RX_CNT == UART4_RX_BUF_MAX) stUSART_ERROR.RX_CNT = 0;
   1559          
   1560          		//stUSART_ERROR.RX_BUF[stUSART_ERROR.RX_CNT++] = Rx_Buf;
   1561          	}
   1562          }
