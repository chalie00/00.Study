   2250          // ============================================================================
   2251          // USART3 : UART4 Data Pasher (TSM)
   2252          // ============================================================================
   2253          void UART4_Data_Pasher(u8 u_Buf, u8 u_Index)
   2254          {
   2255          	if((u_Index == INDEX_NON) || (u_Index == INDEX_THERMAL))
   2256          	{
   2257          		TSM_Data_Pasher(u_Buf);
   2258          	}
   2259          
   2260          	if((u_Index == INDEX_NON) || (u_Index == INDEX_COLOR))
   2261          	{
   2262          		SONY_Data_Pasher(u_Buf);
   2263          	}
   2264          }
