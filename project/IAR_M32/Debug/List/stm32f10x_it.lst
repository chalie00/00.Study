   2107          				default:
   2108          					break;
   2109          			}
   2110          		
   2111          			for(i=0;i<stUART4.RX_BUF_MAX;i++)
   2112          			{
   2113          				//stTEMP.RX_BUF[i] = stUART4.RX_BUF[i];
   2114          				stUART4.RX_BUF[i] = 0x00;
   2115          			}
   2116          
   2117          			stUART4.RX_ENABLE = 0x00;
   2118          			stUART4.RX_CNT = 0x00;
   2119          			stUART4.RX_BUF_MAX = 0x00;
   2120          			stUART4.RX_PARITY = 0x01;
   2121          			
   2122          		}
   2123          		else if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf != 0xAF))
   2124          		{
   2125          			for(i=0;i<stUART4.RX_CNT;i++)
   2126          			{
   2127          				stUART4.RX_BUF[i] = 0x00;
