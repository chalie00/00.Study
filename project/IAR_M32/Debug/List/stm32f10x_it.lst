   2263          // ============================================================================
   2264          // USART3 : UART4 Data Pasher (TSM)
   2265          // ============================================================================
   2266          void UART4_Data_Pasher(u8 u_Buf, u8 u_Index)
   2267          {
   2268          	if((u_Index == INDEX_NON) || (u_Index == INDEX_THERMAL))
   2269          	{
   2270          		TSM_Data_Pasher(u_Buf);
   2271          	}
   2272          
   2273          	if((u_Index == INDEX_NON) || (u_Index == INDEX_COLOR))
   2274          	{
   2275          		SONY_Data_Pasher(u_Buf);
   2276          	}
   2277          }
