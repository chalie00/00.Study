   2023          								stUART4.RX_REQUEST_CMD = 0x00;
   2024          							}
   2025          							
   2026          							break;		
   2027          
   2028          						case TCM_FOCUS_POSITION_INQ:
   2029          
   2030          							if(stUART4.RX_BUF[1] == TCM_FOCUS_POSITION_INQ)
   2031          							{
   2032          								u_Data_P1 = stUART4.RX_BUF[2];
   2033          								u_Data_P2 = stUART4.RX_BUF[3];
