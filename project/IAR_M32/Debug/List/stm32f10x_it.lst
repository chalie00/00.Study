   1770          /*******************************************************************************
   1771          * Function Name  : USART3_IRQHandler
   1772          * Description    : This function handles USART1 interrupt request.
   1773          * Input           : Camera - PB11 USART3_RX
   1774          * Output         : Camera - PB10 USART3_TX
   1775          *******************************************************************************/
   1776          void USART3_IRQHandler(void)
   1777          {
   1778          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1779          
   1780            	// =======================================
   1781            	// USART3 TX Process 
