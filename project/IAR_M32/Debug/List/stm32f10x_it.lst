   2366          void Rx_Usart_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   2367          {
   2368          	// 첫번째 바이트 체크 (Sync Byte Check)
   2369          	if(u_sync == Rx_SYNC_BYTE)	
   2370          	{
   2371          		stUART5.RX_BUF_INDEX = 0x00;	// Index Init
   2372          		stUART5.RX_BUF[stUART5.RX_BUF_INDEX] = u_buf;
   2373          		stUART5.RX_BUF_INDEX++;
   2374          		
   2375          		#if(ERROR_CHECK_BUF)
   2376          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   2377          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   2378          		#endif
   2379          	}
   2380          
   2381          	// 두번째 바이트 체크 (Sync & Byte Check)
   2382          	if(u_sync == Rx_SYNC_AND_BYTE)
   2383          	{
   2384          		#if(ERROR_CHECK_BUF)	
   2385          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
