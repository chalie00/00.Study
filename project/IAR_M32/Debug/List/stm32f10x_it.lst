    997          /****************************************************************************************************
    998          * Function Name  : Boot_Status_Data_Trans
    999          * Data Architecture :
   1000          BYTE1[0]		BYTE2[1]		BYTE3[2]		BYTE4[3]		BYTE5[4]		BYTE5[5]		BYTE5[LENGTH+4]
   1001          0xCA		0xAC		LENGTH		ADDR		COMM		DATA...		CS
   1002          0xCA		0xAC		0x04		0x01		0xD1		DATA...		CS
   1003          
   1004          DATA : 0x00: Nondata, 0x01: ACTIVE Status, 0x02 : Boot Ready Status
   1005          *****************************************************************************************************/
   1006          void Boot_Status_Data_Trans(void)
   1007          {
   1008          	u8 u_Cmd = TILT_BOOT_STATUS, u_Leng = 0x04, u_ID = MDIN_LAYER_CENTER_MSG;
   1009          
   1010          	switch(stSYS.BOOT_STATUS)
   1011          	{
   1012          		// TILT B/D 가 POWER OFF 인 경우
   1013          		case STOP:
   1014          			break;
   1015          
   1016          		// TILT B/D 가 MODULE 을 정상 경우
   1017          		case ACTIVE:
   1018          
   1019          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1020          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1021          			stUSART1.TX_BUF[2] = u_Leng;
   1022          			stUSART1.TX_BUF[3] = u_ID;
   1023          			stUSART1.TX_BUF[4] = u_Cmd;
   1024          			stUSART1.TX_BUF[5] = ACTIVE;
   1025          			stUSART1.TX_BUF[6] = 0x00;
   1026          			
   1027          			stUSART1.TX_BUF[7] = 0x00;		
   1028          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1029          
   1030          			stUSART1.TX_BUF[8] = 0xCF;
   1031          			
   1032          			stUSART1.TX_CNT = 0;
   1033          			stUSART1.TX_MAX = u_Leng + 5;
   1034          
   1035          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1036          
   1037          			stSYS.BOOT_STATUS = STOP;
   1038          			
   1039          			break;
   1040          
   1041          		// TILT B/D 가 MODULE 을 정상 부팅 못하였을 경우 (MAIN으로 부터 체크 송신을 받았으나 MODULE 미인식, 데이터 비정상)
   1042          		case READY:
   1043          
   1044          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1045          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1046          			stUSART1.TX_BUF[2] = u_Leng;
   1047          			stUSART1.TX_BUF[3] = u_ID;
   1048          			stUSART1.TX_BUF[4] = u_Cmd;
   1049          			stUSART1.TX_BUF[5] = READY;
   1050          			stUSART1.TX_BUF[6] = 0x00;
   1051          			
   1052          			stUSART1.TX_BUF[7] = 0x00;		
   1053          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1054          
   1055          			stUSART1.TX_BUF[8] = 0xCF;
   1056          
   1057          			stUSART1.TX_CNT = 0;
   1058          			stUSART1.TX_MAX = u_Leng + 5;
   1059          
   1060          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1061          
   1062          			stSYS.BOOT_STATUS = STOP;
   1063          		
   1064          			break;
   1065          
   1066          		default:
   1067          			break;
   1068          	}
   1069          }
