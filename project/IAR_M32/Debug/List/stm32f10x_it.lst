   1522          		if(u_buf == 0xFF)
   1523          		{
   1524          			stBYPASS.RX_STATUS = 0x00;
   1525          			stBYPASS.RX_CNT = 0x00;
   1526          			stBYPASS.RX_PARITY = 0x01;			
   1527          		}
   1528          	}
   1529          	else
   1530          	{
   1531          		if(u_buf == SONY_FIRST_BYTE)
