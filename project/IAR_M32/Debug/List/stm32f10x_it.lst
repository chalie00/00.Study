   1695          /*******************************************************************************
   1696          * Function Name  : USART2_IRQHandler
   1697          * Description    : This function handles USART2 interrupt request.
   1698          * Input           : MDIN - PA3 USART2_RX
   1699          * Output         : MDIN - PA2 USART2_TX
   1700          *******************************************************************************/
   1701          void USART2_IRQHandler(void)
   1702          {
   1703          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1704          
   1705          	// =======================================
   1706          	// USART2 TX Process 
   1707          	// =======================================
   1708          	if (USART_GetITStatus(USART2, USART_IT_TC) != RESET)
   1709          	{
   1710          
   1711          		if(stUSART2.TX_CNT < stUSART2.TX_MAX)
   1712          		{
   1713          			Tx_Buf = stUSART2.TX_BUF[stUSART2.TX_CNT];
   1714          			stUSART2.TX_CNT++;				
   1715          
   1716          			USART_SendData(USART2, Tx_Buf);
   1717          		}
   1718          		else
   1719          		{
   1720          			stUSART2.TX_CNT = 0x00;
   1721          			stUSART2.TX_MAX = 0x00;
   1722          		}
   1723          
   1724          		USART_ClearITPendingBit(USART2, USART_IT_TC);
   1725          
   1726          	}
   1727          
   1728          	// =======================================
   1729          	// USART2 RX Process
   1730          	// =======================================
   1731          	if (USART_GetITStatus(USART2, USART_IT_RXNE) != RESET)
   1732          	{
   1733          		Rx_Buf = USART_ReceiveData(USART2);
   1734          	}     
   1735          }
