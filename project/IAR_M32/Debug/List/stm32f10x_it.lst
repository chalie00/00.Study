   2580          /*******************************************************************************
   2581          * Function Name  : Protocol_Command_Check
   2582          * Description    : This function handles UART5 interrupt request.
   2583          * Input           : TILT <-> Thermal IP Board (- PA10 UART5_RX)
   2584          * Output         : TILT <-> Thermal IP Board (- PA9 UART5_TX)
   2585          *******************************************************************************/
   2586          void Protocol_Command_Check(u8 u_buf)
   2587          {
   2588          	u8 u_Index_Chk = 0;
   2589          
   2590          	if(stUART5.RX_ENABLE == 0x00)
   2591          	{
   2592          		// 첫번째 바이트 체크 (Sync Byte Check)
   2593          		switch(u_buf)
   2594          		{
   2595          			case IP_SONY_FIRST_BYTE:
   2596          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   2597          				stUART5.RX_ENABLE = IP_SONY_SECOND_BYTE;
   2598          				break;
   2599          
   2600          			case IP_TSM_FIRST_BYTE:
   2601          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   2602          				stUART5.RX_ENABLE = IP_TSM_SECOND_BYTE;
   2603          				break;
   2604          
   2605          			case IP_DRS_FIRST_BYTE:
   2606          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DRS_FIRST_BYTE);
   2607          				stUART5.RX_ENABLE = IP_DRS_SECOND_BYTE;
   2608          				break;
   2609          
   2610          			case IP_TCM_FIRST_BYTE:
   2611          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TCM_FIRST_BYTE);
   2612          				stUART5.RX_ENABLE = IP_TCM_SECOND_BYTE;
   2613          				break;
   2614          
   2615          			case IP_PTZ_FIRST_BYTE:
   2616          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PTZ_FIRST_BYTE);
   2617          				stUART5.RX_ENABLE = IP_PTZ_SECOND_BYTE;
   2618          				break;
   2619          
   2620          			case IP_DATA_FIRST_BYTE:
   2621          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DATA_FIRST_BYTE);
   2622          				stUART5.RX_ENABLE = IP_DATA_SECOND_BYTE;
   2623          				break;
   2624          
   2625          			case IP_OPTION_FIRST_BYTE:
   2626          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_OPTION_FIRST_BYTE);
   2627          				stUART5.RX_ENABLE = IP_OPTION_SECOND_BYTE;
   2628          				break;
   2629          
   2630          			case IP_PELCO_CHK_BYTE:
   2631          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PELCO_CHK_BYTE);
   2632          				stUART5.RX_ENABLE = IP_PELCO_CHK_BYTE;
   2633          				break;	
   2634          
   2635          			default:
   2636          				break;
   2637          		}
   2638          	}
   2639          	else
   2640          	{
   2641          		if(stUART5.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   2642          		else 
   2643          		{
   2644          			if(stUART5.RX_BUF_INDEX >= 0x03)
   2645          			{
   2646          				u_Index_Chk = Rx_DATA_BYTE;
   2647          			}
   2648          			else
   2649          			{
   2650          				if(stUART5.RX_STATUS == 0x00)
   2651          				{
   2652          					u_Index_Chk = Rx_LENGTH_BYTE;
   2653          				}
   2654          			}
   2655          		}
   2656          
   2657          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   2658          		Rx_Usart_Comm_Handling(u_Index_Chk, u_buf);
   2659          	}
   2660          }
