   2547          /*******************************************************************************
   2548          * Function Name  : Protocol_Command_Check
   2549          * Description    : This function handles UART5 interrupt request.
   2550          * Input           : TILT <-> Thermal IP Board (- PA10 UART5_RX)
   2551          * Output         : TILT <-> Thermal IP Board (- PA9 UART5_TX)
   2552          *******************************************************************************/
   2553          void Protocol_Command_Check(u8 u_buf)
   2554          {
   2555          	u8 u_Index_Chk = 0;
   2556          
   2557          	if(stUART5.RX_ENABLE == 0x00)
   2558          	{
   2559          		// 첫번째 바이트 체크 (Sync Byte Check)
   2560          		switch(u_buf)
   2561          		{
   2562          			case IP_SONY_FIRST_BYTE:
   2563          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   2564          				stUART5.RX_ENABLE = IP_SONY_SECOND_BYTE;
   2565          				break;
   2566          
   2567          			case IP_TSM_FIRST_BYTE:
   2568          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   2569          				stUART5.RX_ENABLE = IP_TSM_SECOND_BYTE;
   2570          				break;
   2571          
   2572          			case IP_DRS_FIRST_BYTE:
   2573          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DRS_FIRST_BYTE);
   2574          				stUART5.RX_ENABLE = IP_DRS_SECOND_BYTE;
   2575          				break;
   2576          
   2577          			case IP_TCM_FIRST_BYTE:
   2578          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TCM_FIRST_BYTE);
   2579          				stUART5.RX_ENABLE = IP_TCM_SECOND_BYTE;
   2580          				break;
   2581          
   2582          			case IP_PTZ_FIRST_BYTE:
   2583          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PTZ_FIRST_BYTE);
   2584          				stUART5.RX_ENABLE = IP_PTZ_SECOND_BYTE;
   2585          				break;
   2586          
   2587          			case IP_DATA_FIRST_BYTE:
   2588          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DATA_FIRST_BYTE);
   2589          				stUART5.RX_ENABLE = IP_DATA_SECOND_BYTE;
   2590          				break;
   2591          
   2592          			case IP_OPTION_FIRST_BYTE:
   2593          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_OPTION_FIRST_BYTE);
   2594          				stUART5.RX_ENABLE = IP_OPTION_SECOND_BYTE;
   2595          				break;
   2596          
   2597          			case IP_PELCO_CHK_BYTE:
   2598          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PELCO_CHK_BYTE);
   2599          				stUART5.RX_ENABLE = IP_PELCO_CHK_BYTE;
   2600          				break;	
   2601          
   2602          			default:
   2603          				break;
   2604          		}
   2605          	}
   2606          	else
   2607          	{
   2608          		if(stUART5.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   2609          		else 
   2610          		{
   2611          			if(stUART5.RX_BUF_INDEX >= 0x03)
   2612          			{
   2613          				u_Index_Chk = Rx_DATA_BYTE;
   2614          			}
   2615          			else
   2616          			{
   2617          				if(stUART5.RX_STATUS == 0x00)
   2618          				{
   2619          					u_Index_Chk = Rx_LENGTH_BYTE;
   2620          				}
   2621          			}
   2622          		}
   2623          
   2624          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   2625          		Rx_Usart_Comm_Handling(u_Index_Chk, u_buf);
   2626          	}
   2627          }
