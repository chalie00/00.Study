   1819          								}
   1820          								
   1821          								stTSM.MODULE_STATUS = u_Data_P1;
   1822          								stINFO.MODULE_TYPE = TSM_MODULE;
   1823          
   1824          								stUART4.RX_REQUEST_CMD = 0x00;
   1825          							}
   1826          								
   1827          							break;
   1828          							
   1829          						case TSM_CALL_DIGITAL_ZOOM_POSITION:
   1830          
   1831          							if(stUART4.RX_BUF[1] == TSM_CALL_DIGITAL_ZOOM_POSITION)
   1832          							{
   1833          								u_Data_P1 = stUART4.RX_BUF[4];
   1834          								u16_Convert_Data |= u_Data_P1;
   1835          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1836          
   1837          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1838          
   1839          								stUART4.RX_REQUEST_CMD = 0x00;
   1840          							}
   1841          
   1842          							break;
   1843          
   1844          						case TSM_CALL_OPTICAL_ZOOM_POSITION:
   1845          							break;
   1846          
   1847          						case TSM_CALL_TOTAL_ZOOM_POSITION:
   1848          							
   1849          							if(stUART4.RX_BUF[1] == TSM_CALL_TOTAL_ZOOM_POSITION)
   1850          							{
   1851          								u_Data_P1 = stUART4.RX_BUF[2];
   1852          								u_Data_P2 = stUART4.RX_BUF[3];
   1853          
   1854          								u16_Convert_Data = u_Data_P1 << 8;
   1855          								u16_Convert_Data |= u_Data_P2;
   1856          
   1857          								stTSM.ZOOM_CNT = u16_Convert_Data;
   1858          
   1859          								stUART4.RX_REQUEST_CMD = 0x00;
   1860          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_TOTAL_ZOOM_POSITION;
   1861          							}
   1862          							
   1863          							break;
   1864          
   1865          						case TSM_CALL_FOCUS_POSITION:
   1866          
   1867          							if(stUART4.RX_BUF[1] == TSM_CALL_FOCUS_POSITION)
   1868          							{
   1869          								u_Data_P1 = stUART4.RX_BUF[2];
   1870          								u_Data_P2 = stUART4.RX_BUF[3];
   1871          
   1872          								u16_Convert_Data = u_Data_P1 << 8;
   1873          								u16_Convert_Data |= u_Data_P2;
   1874          
   1875          								stTSM.FOCUS_CNT = u16_Convert_Data;
   1876          
   1877          								stUART4.RX_REQUEST_CMD = 0x00;
   1878          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_FOCUS_POSITION;								
   1879          							}
   1880          							
   1881          							break;
   1882          
   1883          						case TSM_CALL_PRODUCT_IDENTIFICATION:
   1884          
   1885          							if(stUART4.RX_BUF[1] == TSM_CALL_PRODUCT_IDENTIFICATION)
   1886          							{
   1887          								// Camera Information Data Check							
   1888          								stINFO.THERMAL_MODEL = stUART4.RX_BUF[2];
