   1783          	if (USART_GetITStatus(USART3, USART_IT_TC) != RESET)
   1784          	{ 
   1785          		USART_SendData(USART3, Tx_Buf);
   1786          		
   1787          		USART_ClearITPendingBit(USART3, USART_IT_TC);
   1788          	}
   1789          	
   1790            	// =======================================
   1791            	// USART3 RX Process
   1792            	// =======================================
   1793          	if (USART_GetITStatus(USART3, USART_IT_RXNE) != RESET)
   1794          	{
   1795          		Rx_Buf = USART_ReceiveData(USART3);
   1796          	}
   1797          
   1798          }
