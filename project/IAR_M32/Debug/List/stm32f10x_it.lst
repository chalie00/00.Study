    803          /*******************************************************************************
    804          * Function Name  : SYSTEM_Data_Reordering
    805          *******************************************************************************/
    806          void SYSTEM_Data_Reordering(void)
    807          {
    808          	u8 i = 0x00, j = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    809          	
    810          	if((stSYS.RX_PARITY == 0x00) && (stSYS.RX_REORDERING == 0x01))
    811          	{
    812          		if(stSYS.RX_INDEX == 0x00)
    813          		{
    814          			u_buf = stSYS.RX_BUF[0][1];
    815          		
    816          			switch(u_buf)
    817          			{
    818          				case TILT_FIRST_BYTE:
    819          				case IP_SONY_FIRST_BYTE:
    820          				case IP_TSM_FIRST_BYTE:
    821          				case IP_DRS_FIRST_BYTE:
    822          				case IP_PTZ_FIRST_BYTE:
    823          				case IP_DATA_FIRST_BYTE:
    824          				case IP_OPTION_FIRST_BYTE:
    825          
    826          					u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    827          
    828          					u_max = u_length + 6;
    829          					
    830          					for(j=0; j<u_max; j++)
    831          					{
    832          						stSYS.RX_BUF[0][j] = 0x00;
    833          					}
    834          
    835          					stSYS.RX_REORDERING = 0x00;
    836          					
    837          					break;
    838          
    839          				default:
    840          					break;
    841          			}			
    842          		}
    843          		else
    844          		{
    845          			for(i=1; i<SYS_RX_BUF_CNT; i++)
    846          			{
    847          				if(stSYS.RX_BUF[i][0] != 0x00)
    848          				{
    849          					u_buf = stSYS.RX_BUF[i][1];	// Command Byte Check
    850          					
    851          					switch(u_buf)
    852          					{
    853          						case TILT_FIRST_BYTE:
    854          						case IP_SONY_FIRST_BYTE:
    855          						case IP_TSM_FIRST_BYTE:
    856          						case IP_DRS_FIRST_BYTE:
    857          						case IP_PTZ_FIRST_BYTE:
    858          						case IP_DATA_FIRST_BYTE:
    859          						case IP_OPTION_FIRST_BYTE:
    860          
    861          							u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    862          
    863          							u_max = u_length + 6;
    864          							
    865          							for(j=0; j<u_max; j++)
    866          							{
    867          								stSYS.RX_BUF[i-1][j] = stSYS.RX_BUF[i][j];
    868          								if(stSYS.RX_INDEX > 0) stSYS.RX_BUF[stSYS.RX_INDEX - 1][j] = 0x00;
    869          							}
    870          
    871          							stSYS.RX_REORDERING = 0x00;
    872          							
    873          							break;
    874          
    875          						default:
    876          							break;
    877          
    878          					}
    879          				}
    880          			}
    881          		}
    882          		
    883          		stSYS.RX_COMPLETE = 0x00;
    884          		if(stSYS.RX_INDEX > 0) stSYS.RX_INDEX--;
    885          		
    886          	}
    887          }
