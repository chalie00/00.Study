    804          /*******************************************************************************
    805          * Function Name  : SYSTEM_Data_Reordering
    806          *******************************************************************************/
    807          void SYSTEM_Data_Reordering(void)
    808          {
    809          	u8 i = 0x00, j = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    810          	
    811          	if((stSYS.RX_PARITY == 0x00) && (stSYS.RX_REORDERING == 0x01))
    812          	{
    813          		if(stSYS.RX_INDEX == 0x00)
    814          		{
    815          			u_buf = stSYS.RX_BUF[0][1];
    816          		
    817          			switch(u_buf)
    818          			{
    819          				case TILT_FIRST_BYTE:
    820          				case IP_SONY_FIRST_BYTE:
    821          				case IP_TSM_FIRST_BYTE:
    822          				case IP_DRS_FIRST_BYTE:
    823          				case IP_PTZ_FIRST_BYTE:
    824          				case IP_DATA_FIRST_BYTE:
    825          				case IP_OPTION_FIRST_BYTE:
    826          
    827          					u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    828          
    829          					u_max = u_length + 6;
    830          					
    831          					for(j=0; j<u_max; j++)
    832          					{
    833          						stSYS.RX_BUF[0][j] = 0x00;
    834          					}
    835          
    836          					stSYS.RX_REORDERING = 0x00;
    837          					
    838          					break;
    839          
    840          				default:
    841          					break;
    842          			}			
    843          		}
    844          		else
    845          		{
    846          			for(i=1; i<SYS_RX_BUF_CNT; i++)
    847          			{
    848          				if(stSYS.RX_BUF[i][0] != 0x00)
    849          				{
    850          					u_buf = stSYS.RX_BUF[i][1];	// Command Byte Check
    851          					
    852          					switch(u_buf)
    853          					{
    854          						case TILT_FIRST_BYTE:
    855          						case IP_SONY_FIRST_BYTE:
    856          						case IP_TSM_FIRST_BYTE:
    857          						case IP_DRS_FIRST_BYTE:
    858          						case IP_PTZ_FIRST_BYTE:
    859          						case IP_DATA_FIRST_BYTE:
    860          						case IP_OPTION_FIRST_BYTE:
    861          
    862          							u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    863          
    864          							u_max = u_length + 6;
    865          							
    866          							for(j=0; j<u_max; j++)
    867          							{
    868          								stSYS.RX_BUF[i-1][j] = stSYS.RX_BUF[i][j];
    869          								if(stSYS.RX_INDEX > 0) stSYS.RX_BUF[stSYS.RX_INDEX - 1][j] = 0x00;
    870          							}
    871          
    872          							stSYS.RX_REORDERING = 0x00;
    873          							
    874          							break;
    875          
    876          						default:
    877          							break;
    878          
    879          					}
    880          				}
    881          			}
    882          		}
    883          		
    884          		stSYS.RX_COMPLETE = 0x00;
    885          		if(stSYS.RX_INDEX > 0) stSYS.RX_INDEX--;
    886          		
    887          	}
    888          }
