   2520          /*******************************************************************************
   2521          * Function Name  : Protocol_Command_Check
   2522          * Description    : This function handles UART5 interrupt request.
   2523          * Input           : TILT <-> Thermal IP Board (- PA10 UART5_RX)
   2524          * Output         : TILT <-> Thermal IP Board (- PA9 UART5_TX)
   2525          *******************************************************************************/
   2526          void Protocol_Command_Check(u8 u_buf)
   2527          {
   2528          	u8 u_Index_Chk = 0;
   2529          
   2530          	if(stUART5.RX_ENABLE == 0x00)
   2531          	{
   2532          		// 첫번째 바이트 체크 (Sync Byte Check)
   2533          		switch(u_buf)
   2534          		{
   2535          			case IP_SONY_FIRST_BYTE:
   2536          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   2537          				stUART5.RX_ENABLE = IP_SONY_SECOND_BYTE;
   2538          				break;
   2539          
   2540          			case IP_TSM_FIRST_BYTE:
   2541          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   2542          				stUART5.RX_ENABLE = IP_TSM_SECOND_BYTE;
   2543          				break;
   2544          
   2545          			case IP_DRS_FIRST_BYTE:
   2546          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DRS_FIRST_BYTE);
   2547          				stUART5.RX_ENABLE = IP_DRS_SECOND_BYTE;
   2548          				break;
   2549          
   2550          			case IP_TCM_FIRST_BYTE:
   2551          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TCM_FIRST_BYTE);
   2552          				stUART5.RX_ENABLE = IP_TCM_SECOND_BYTE;
   2553          				break;
   2554          
   2555          			case IP_PTZ_FIRST_BYTE:
   2556          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PTZ_FIRST_BYTE);
   2557          				stUART5.RX_ENABLE = IP_PTZ_SECOND_BYTE;
   2558          				break;
   2559          
   2560          			case IP_DATA_FIRST_BYTE:
   2561          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DATA_FIRST_BYTE);
   2562          				stUART5.RX_ENABLE = IP_DATA_SECOND_BYTE;
   2563          				break;
   2564          
   2565          			case IP_OPTION_FIRST_BYTE:
   2566          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_OPTION_FIRST_BYTE);
   2567          				stUART5.RX_ENABLE = IP_OPTION_SECOND_BYTE;
   2568          				break;
   2569          
   2570          			case IP_PELCO_CHK_BYTE:
   2571          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PELCO_CHK_BYTE);
   2572          				stUART5.RX_ENABLE = IP_PELCO_CHK_BYTE;
   2573          				break;	
   2574          
   2575          			default:
   2576          				break;
   2577          		}
   2578          	}
   2579          	else
   2580          	{
   2581          		if(stUART5.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   2582          		else 
   2583          		{
   2584          			if(stUART5.RX_BUF_INDEX >= 0x03)
   2585          			{
   2586          				u_Index_Chk = Rx_DATA_BYTE;
   2587          			}
   2588          			else
   2589          			{
   2590          				if(stUART5.RX_STATUS == 0x00)
   2591          				{
   2592          					u_Index_Chk = Rx_LENGTH_BYTE;
   2593          				}
   2594          			}
   2595          		}
   2596          
   2597          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   2598          		Rx_Usart_Comm_Handling(u_Index_Chk, u_buf);
   2599          	}
   2600          }
