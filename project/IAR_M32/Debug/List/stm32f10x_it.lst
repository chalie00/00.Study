   2047          					}
   2048          						
   2049          					break;
   2050          
   2051          				case 0x07:
   2052          					switch(stUART4.RX_REQUEST_CMD)
   2053          					{
   2054          						case TCM_COOLER_RUNTIME_INQ:
   2055          
   2056          								stINFO.THERMAL_ENGINE_TIME_DAY= 0x0000;
   2057          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[2]<<8);
