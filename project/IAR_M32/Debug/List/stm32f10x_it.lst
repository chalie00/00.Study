   1914          								stINFO.FPGA_VERSION_LOW = stUART4.RX_BUF[3];
   1915          
   1916          								stUART4.RX_REQUEST_CMD = 0x00;
   1917          							}
   1918          							
   1919          							break;
   1920          							
   1921          						case TSM_CALL_TEMPERATURE_MODE:
   1922          
   1923          							if(stUART4.RX_BUF[1] == TSM_CALL_TEMPERATURE_MODE)
   1924          							{
   1925          								// Camera Information Data Check
   1926          								stINFO.THERMAL_TEMP_MODE = stUART4.RX_BUF[2];
   1927          								
   1928          								stUART4.RX_REQUEST_CMD = 0x00;
   1929          							}
   1930          							
   1931          							break;
   1932          
   1933          						case TCM_TOTAL_ZOOM_POSITION_INQ:
   1934          
   1935          							if(stUART4.RX_BUF[1] == TCM_TOTAL_ZOOM_POSITION_INQ)
   1936          							{
   1937          								u_Data_P1 = stUART4.RX_BUF[2];
   1938          								u_Data_P2 = stUART4.RX_BUF[3];
   1939          
   1940          								u16_Convert_Data = u_Data_P1 << 8;
   1941          								u16_Convert_Data |= u_Data_P2;
   1942          
   1943          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1944          
   1945          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1946          
   1947          								stUART4.RX_REQUEST_CMD = 0x00;
   1948          							}
   1949          							
   1950          							break;
   1951          
   1952          						case TCM_GET_FW_UPDATE_INQ:
   1953          
   1954          							if(stUART4.RX_BUF[1] == TCM_GET_FW_UPDATE_INQ)
   1955          							{
   1956          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[2];
   1957          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[3];
   1958          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   1959          									
   1960          								stUART4.RX_REQUEST_CMD = 0x00;
   1961          							}
   1962          							
   1963          							break;
   1964          
   1965          						case TCM_GET_ENGINE_VERSION:
   1966          
   1967          							if(stUART4.RX_BUF[1] == TCM_GET_ENGINE_VERSION)
   1968          							{
   1969          								stINFO.THERMAL_ENGINE_VER_HW = stUART4.RX_BUF[2];
   1970          								stINFO.THERMAL_ENGINE_VER_FW = stUART4.RX_BUF[3];
   1971          
   1972          								stUART4.RX_REQUEST_CMD = 0x00;
   1973          							}
   1974          							
   1975          							break;		
   1976          
   1977          						case TCM_FOCUS_POSITION_INQ:
   1978          
   1979          							if(stUART4.RX_BUF[1] == TCM_FOCUS_POSITION_INQ)
   1980          							{
   1981          								u_Data_P1 = stUART4.RX_BUF[2];
   1982          								u_Data_P2 = stUART4.RX_BUF[3];
   1983          
   1984          								u16_Convert_Data = u_Data_P1 << 8;
   1985          								u16_Convert_Data |= u_Data_P2;
   1986          
   1987          								stTSM.FOCUS_CNT = u16_Convert_Data;
   1988          
   1989          								stUART4.RX_REQUEST_CMD = 0x00;
   1990          							}
   1991          
   1992          							break;	
   1993          
   1994          						default:
   1995          							break;
   1996          					}
   1997          						
   1998          					break;
   1999          
   2000          				case 0x07:
   2001          					switch(stUART4.RX_REQUEST_CMD)
   2002          					{
   2003          						case TCM_COOLER_RUNTIME_INQ:
   2004          
   2005          								stINFO.THERMAL_ENGINE_TIME_DAY= 0x0000;
   2006          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[2]<<8);
   2007          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[3]&0xFF);
   2008          
   2009          								stINFO.THERMAL_ENGINE_TIME_HOUR = 0x0000;
   2010          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[4]<<8);
   2011          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[5]&0xFF);
   2012          
   2013          								stINFO.THERMAL_ENGINE_DAY_HIGH = stUART4.RX_BUF[2];
   2014          								stINFO.THERMAL_ENGINE_DAY_LOW = stUART4.RX_BUF[3];
   2015          								stINFO.THERMAL_ENGINE_HOUR_HIGH = stUART4.RX_BUF[4];
   2016          								stINFO.THERMAL_ENGINE_HOUR_LOW = stUART4.RX_BUF[5];								
   2017          								
   2018          								stINFO.TCM640_ENGINE_RUNTIME_COMPLETE = TRUE;
   2019          								
   2020          								stUART4.RX_REQUEST_CMD = 0x00;	
   2021          
   2022          							break;
   2023          					}
   2024          					break;
   2025          					
   2026          				case 0x08:
   2027          					
   2028          					switch(stUART4.RX_REQUEST_CMD)
   2029          					{
   2030          						case TSM_CALL_FIRMWARE_VERSION:
   2031          
   2032          							if(stUART4.RX_BUF[1] == TSM_CALL_FIRMWARE_VERSION)
   2033          							{
   2034          								// Camera Information Data Check
   2035          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   2036          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[5];
   2037          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[6];
   2038          								
   2039          								stUART4.RX_REQUEST_CMD = 0x00;
   2040          							}
   2041          							
   2042          							break;
   2043          
   2044          						default:
   2045          							break;
   2046          					}
   2047          							
   2048          					break;
   2049          					
   2050          				case 0x23:
   2051          					
   2052          					//stTEMP.TEMPERATURE_READ_COMPLETE = 0x01;	// 온도 체크 데이터 수신 완료.
   2053          					
   2054          					break;
   2055          
   2056          				default:
   2057          					break;
   2058          			}
   2059          		
   2060          			for(i=0;i<stUART4.RX_BUF_MAX;i++)
   2061          			{
   2062          				//stTEMP.RX_BUF[i] = stUART4.RX_BUF[i];
   2063          				stUART4.RX_BUF[i] = 0x00;
   2064          			}
   2065          
   2066          			stUART4.RX_ENABLE = 0x00;
   2067          			stUART4.RX_CNT = 0x00;
   2068          			stUART4.RX_BUF_MAX = 0x00;
   2069          			stUART4.RX_PARITY = 0x01;
   2070          			
   2071          		}
   2072          		else if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf != 0xAF))
   2073          		{
   2074          			for(i=0;i<stUART4.RX_CNT;i++)
   2075          			{
   2076          				stUART4.RX_BUF[i] = 0x00;
   2077          			}
   2078          
   2079          			stUART4.RX_ENABLE = 0x00;
   2080          			stUART4.RX_CNT = 0x00;
   2081          			stUART4.RX_BUF_MAX = 0x00;
   2082          			stUART4.RX_REQUEST_ERROR = 0x01;			
   2083          		}
   2084          	}
   2085          
   2086          	if((stUART4.RX_ENABLE == 0x00) && (u_Buf == 0xA0))
   2087          	{
   2088          		stUART4.RX_BUF[0] = u_Buf;
   2089          		stUART4.RX_CNT++;
   2090          		stUART4.RX_ENABLE = 0x01;
   2091          	}
   2092          }
