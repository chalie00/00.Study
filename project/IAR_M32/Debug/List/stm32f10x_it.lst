   1287          /*******************************************************************************
   1288          * Function Name  : Rx_Main_Comm_Handling
   1289          *******************************************************************************/
   1290          void Rx_Main_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   1291          {
   1292          	// 첫번째 바이트 체크 (Sync Byte Check)
   1293          	if(u_sync == Rx_SYNC_BYTE)	
   1294          	{
   1295          		stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1296          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1297          		stUSART1.RX_BUF_INDEX++;
   1298          		
   1299          		#if(ERROR_CHECK_BUF)
   1300          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1301          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1302          		#endif
   1303          	}
   1304          
   1305          	// 두번째 바이트 체크 (Sync & Byte Check)
   1306          	if(u_sync == Rx_SYNC_AND_BYTE)
   1307          	{
   1308          		#if(ERROR_CHECK_BUF)	
   1309          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1310          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1311          		#endif		
   1312          		
   1313          		// 두번째 명령어 체크 (Sync & Byte Check)
   1314          		if(stUSART1.RX_BUF_INDEX == 0x01)
   1315          		{
   1316          			switch(stUSART1.RX_ENABLE)
   1317          			{
   1318          				case TILT_SECOND_BYTE:
   1319          					Rx_Main_Second_Comm_Check(u_buf, TILT_SECOND_BYTE);
   1320          					break;
   1321          
   1322          				case IP_SONY_SECOND_BYTE:
   1323          					Rx_Main_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   1324          					break;
   1325          					
   1326          				case IP_TSM_SECOND_BYTE:
   1327          					Rx_Main_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   1328          					break;
   1329          
   1330          				default:
   1331          					break;
   1332          			}
   1333          		}	
   1334          	}
   1335          
   1336          	// 세번째 바이트 체크 (Data Length)
   1337          	if(u_sync == Rx_LENGTH_BYTE)
   1338          	{
   1339          		#if(ERROR_CHECK_BUF)	
   1340          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1341          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1342          		#endif
   1343          		
   1344          		if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1345          		{
   1346          			// PELCO-D Command 예외처리
   1347          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1348          			{
   1349          				stUSART1.RX_BUF_LENGTH = 0x02;
   1350          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1351          				
   1352          				stUSART1.RX_BUF[2] =  u_buf;
   1353          				
   1354          				stUSART1.RX_BUF_INDEX++;
   1355          			}					
   1356          		}
   1357          		else
   1358          		{
   1359          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   1360          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1361          			{
   1362          				stUSART1.RX_BUF_LENGTH = u_buf;
   1363          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1364          				
   1365          				stUSART1.RX_BUF[2] =  u_buf;
   1366          				
   1367          				stUSART1.RX_BUF_INDEX++;
   1368          			}
   1369          		}
   1370          	}	
   1371          
   1372          	// 데이터 누적
   1373          	if(u_sync == Rx_DATA_BYTE)
   1374          	{
   1375          		#if(ERROR_CHECK_BUF)
   1376          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1377          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1378          		#endif		
   1379          		
   1380          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1381          
   1382          		stUSART1.RX_BUF_INDEX++;	
   1383          		
   1384          		if(stUSART1.RX_BUF_INDEX == stUSART1.RX_BUF_MAX)
   1385          		{
   1386          			// PELCO-D Command 예외처리
   1387          			if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1388          			{
   1389          				Rx_Pelco_Data_Check();
   1390          			}
   1391          			else
   1392          			{
   1393          				if((stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   1394          				{
   1395          					if(stSYS.START == ACTIVE)
   1396          					{
   1397          						stUSART1.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1398          						stUSART1.RX_PARITY_CNT++;
   1399          						
   1400          						// DEBUG 데이터 체크 ======================================================/
   1401          						//stERROR.PARITY_CNT_CHECK++;
   1402          						//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   1403          						//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
   1404          						// DEBUG 데이터 체크 ===================================================END/
   1405          						
   1406          						//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   1407          					}
   1408          					else
   1409          					{
   1410          						Boot_Status_Check_Parser();
   1411          					}
   1412          
   1413          					stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1414          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   1415          				}
   1416          				else
   1417          				{
   1418          					stUSART1.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1419          					stUSART1.RX_ERROR_CNT++;
   1420          				}
   1421          			}
   1422          			
   1423          			stUSART1.RX_BUF_INDEX = 0x00;
   1424          			stUSART1.RX_BUF_LENGTH = 0x00;
   1425          			
   1426          			stUSART1.RX_ENABLE = 0x00;
   1427          		}
   1428          	}		
   1429          }
