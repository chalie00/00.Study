    549          /*******************************************************************************
    550          * Function Name  : TIM2_IRQHandler
    551          * Description    : This function handles TIM2 global interrupt request.
    552          * Input          : None
    553          * Output         : None
    554          * Return         : None
    555          * Timer		: 10ms
    556          *******************************************************************************/
    557          u8 g_Tx_Buf = 0x00;
    558          
    559          void TIM2_IRQHandler(void)
    560          {
    561          	if (TIM_GetITStatus(TIM2, TIM_IT_Update) != RESET)
    562          	{
    563          		TIM_ClearITPendingBit(TIM2, TIM_IT_Update);	
    564          
    565          		if(GPIO_ReadInputDataBit(GPIOA, GPIO_Pin_1) != RESET) {
    566          			LED_On_Red();
    567          			delay(1000);
    568          		}
    569          		
    570          		
    571          
    572          
    573          
    574          
    575          
    576          		
    577          
    578          		// ================================================================
    579          		// Wiper Sensor Check
    580          		// ================================================================
    581          
    582          		switch(stDIP.MODE2)
    583          		{
    584          			case 0x00:
    585          
    586          				stWIPER2.SENSOR_1 = WIPER_SENSOR_1;
    587          				stWIPER2.SENSOR_2 = WIPER_SENSOR_2;
    588          				
    589          				break;
    590          
    591          			case 0x01:
    592          
    593          				stWIPER2.SENSOR_1 = WIPER_SENSOR_1;
    594          				
    595          				if(stWIPER1.SENSOR_TEMP != stWIPER2.SENSOR_1) 
    596          				{
    597          
    598          					if((stWIPER2.SENSOR_1 == WIPER_SENS1_ON) && (stWIPER1.SENSOR_TEMP == WIPER_SENS1_OFF))
    599          					{
    600          						if((stWIPER1.ACTIVE_COUNT > 0) && (stWIPER1.ACTIVE_COUNT < 0x0F)) 
    601          						{
    602          							stWIPER1.ACTIVE_COUNT--;
    603          						}
    604          						
    605          						if(stWIPER1.ACTIVE_COUNT == 0) 
    606          						{
    607          							//Run_Wiper(WIPER_OFF, DIRECTION_CW);				// Wiper Off
    608          						}
    609          						else
    610          						{
    611          							if((stWIPER1.DWELL_FLAG == 0x00) && (stWIPER1.DWELL_TIME != 0))
    612          							{
    613          								//Run_Wiper(WIPER_OFF, DIRECTION_CW);				// Wiper off
    614          								stWIPER1.DWELL_FLAG = 0x01;
    615          							}
    616          						}
    617          					}
    618          
    619          					stWIPER1.SENSOR_TEMP = WIPER_SENSOR_1;
    620          					
    621          				}
    622          				stWIPER2.SENSOR_2 = WIPER_SENSOR_2;
    623          
    624          
    625          				break;
    626          
    627          			default:
    628          				break;
    629          		}
    630          
    631          		// =======================================
    632          		// Wiper1 Dwell Timer
    633          		// =======================================
    634          		if(stWIPER1.DWELL_FLAG == 0x01)
    635          		{
    636          			stWIPER1.DWELL_TIME_10ms++;
    637          			if(stWIPER1.DWELL_TIME_10ms > 100)
    638          			{
    639          				stWIPER1.DWELL_TIME_10ms = 0;
    640          				stWIPER1.DWELL_TIME_1s++;
    641          
    642          				if(stWIPER1.DWELL_TIME <= stWIPER1.DWELL_TIME_1s)
    643          				{
    644          					stWIPER1.DWELL_FLAG = 0x00;
    645          					if(stWIPER1.ACTIVE_COUNT != 0)
    646          					{
    647          						//Run_Wiper(WIPER_ON, DIRECTION_CW);				// Wiper On
    648          					}
    649          
    650          					stWIPER1.DWELL_TIME_1s = 0;
    651          				}
    652          			}
    653          		}
    654          		
    655          		// ================================================================
    656          		// Wiper2 Dwell Timer
    657          		// ================================================================
    658          		if(stWIPER2.DWELL_TIMER_FLAG)
    659          		{
    660          			stWIPER2.DWELL_TIMER_ms++;
    661          
    662          			if(stWIPER2.DWELL_TIMER_ms >= 100)	// 1s
    663          			{
    664          				stWIPER2.DWELL_TIMER_s++;
    665          				stWIPER2.DWELL_TIMER_ms = 0x00;
    666          		
    667          				if(stWIPER2.DWELL_TIMER_s >= stWIPER2.DWELL_TIME_DATA)
    668          				{
    669          					stWIPER2.DWELL_END_FLAG = 0x01;
    670          					stWIPER2.DWELL_TIMER_s = 0x00;
    671          					stWIPER2.DWELL_TIMER_FLAG = 0x00;
    672          				}
    673          			}
    674          		}
    675          
    676          		if((stWIPER2.COUNT == 0x00) && (stWIPER2.DWELL_TIMER_FLAG != 0x00))
    677          		{
    678          			stWIPER2.DWELL_TIMER_FLAG = 0x00;
    679          			stWIPER2.DWELL_TIMER_ms = 0;
    680          			stWIPER2.DWELL_TIMER_s = 0;
    681          		}
    682          
    683          		// ================================================================
    684          		// LRF Booting Check Time
    685          		// ================================================================
    686          		if(stLRF.INIT_FLAG == 0x01)
    687          		{
    688          			stLRF.INIT_TIME++;
    689          
    690          			if(stLRF.INIT_TIME >= LRF_BOOTING_TIME)
    691          			{
    692          				stLRF.INIT_ACTIVE = 0x01;
    693          
    694          				stLRF.INIT_TIME = 0x00;
    695          				stLRF.INIT_FLAG = 0x00;
    696          			}
    697          		}
    698          
    699          		if(stLRF.CONTINUOUS_FLAG == 0x01)
    700          		{
    701          			stLRF.CONTINUOUS_TIME++;
    702          
    703          			if(stLRF.CONTINUOUS_TIME > stLRF.CONTINUOUS_TIME_MAX)
    704          			{
    705          				stLRF.CONTINUOUS_ACTIVE = 0x01;
    706          				
    707          				stLRF.CONTINUOUS_TIME = 0;
    708          			}
    709          		}
    710          
    711          		// ================================================================
    712          		// BYPASS Tx Check (전송 후 500ms 후 Clear)
    713          		// ================================================================
    714          		if(stBYPASS.TX_FLAG == 0x01)
    715          		{
    716          			stBYPASS.TX_TIMER++;
    717          
    718          			if(stBYPASS.TX_TIMER >= 50)
    719          			{
    720          				stBYPASS.TX_TIMER = 0;
    721          				stBYPASS.TX_FLAG = 0x00;
    722          			}
    723          		}
    724          
    725          		// ================================================================
    726          		// WIPER CONTINUOUS TIMER
    727          		// ================================================================
    728          		if(stWIPER2.CONTINUOUS_FLAG == 0x00)
    729          		{
    730          			if((stWIPER2.CONTINUOUS_ms != 0) || (stWIPER2.CONTINUOUS_s != 0) || (stWIPER2.CONTINUOUS_m != 0) || (stWIPER2.CONTINUOUS_h != 0))
    731          			{
    732          				stWIPER2.CONTINUOUS_ms = 0;
    733          				stWIPER2.CONTINUOUS_s = 0;
    734          				stWIPER2.CONTINUOUS_m = 0;
    735          				stWIPER2.CONTINUOUS_h = 0;
    736          			}
    737          		}
    738          			
    739          		if(stWIPER2.CONTINUOUS_FLAG == 0x01)
    740          		{
    741          			stWIPER2.CONTINUOUS_ms++;
    742          			
    743          			if(stWIPER2.CONTINUOUS_ms >= 100)
    744          			{
    745          				stWIPER2.CONTINUOUS_ms = 0;
    746          				stWIPER2.CONTINUOUS_s++;
    747          				
    748          				if(stWIPER2.CONTINUOUS_s >= 60)
    749          				{
    750          					stWIPER2.CONTINUOUS_s = 0;
    751          					stWIPER2.CONTINUOUS_m++;
    752          
    753          					if(stWIPER2.CONTINUOUS_m >= 60)
    754          					{
    755          						stWIPER2.CONTINUOUS_m = 0;
    756          						stWIPER2.CONTINUOUS_h++;
    757          
    758          						if(stWIPER2.CONTINUOUS_h >= stWIPER2.CONTINUOUS_TIME)
    759          						{
    760          							stWIPER2.CONTINUOUS_h = 0;
    761          							stWIPER2.CONTINUOUS_FLAG = 0x00;
    762          							stWIPER2.CONTINUOUS_STOP_FLAG = 0x01;
    763          						}
    764          					}
    765          				}
    766          			}
    767          		}
    768          
    769          		// ================================================================
    770          		// TEST TIMER
    771          		// ================================================================
    772          /*
    773          		if(stUSART2.TX_CNT < stUSART2.TX_MAX)
    774          		{
    775          			g_Tx_Buf = stUSART2.TX_BUF[stUSART2.TX_CNT];
    776          			stUSART2.TX_CNT++;				
    777          
    778          			USART_SendData(USART2, g_Tx_Buf);
    779          		}
    780          		else
    781          		{
    782          			stUSART2.TX_CNT = 0x00;
    783          			stUSART2.TX_MAX = 0x00;
    784          		
    785          			//USART_SendData(USART2, Tx_Buf);
    786          		}
    787          */
    788          
    789          	}	
    790          }
