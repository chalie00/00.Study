   2541          /*******************************************************************************
   2542          * Function Name  : Protocol_Command_Check
   2543          * Description    : This function handles UART5 interrupt request.
   2544          * Input           : TILT <-> Thermal IP Board (- PA10 UART5_RX)
   2545          * Output         : TILT <-> Thermal IP Board (- PA9 UART5_TX)
   2546          *******************************************************************************/
   2547          void Protocol_Command_Check(u8 u_buf)
   2548          {
   2549          	u8 u_Index_Chk = 0;
   2550          
   2551          	if(stUART5.RX_ENABLE == 0x00)
   2552          	{
   2553          		// 첫번째 바이트 체크 (Sync Byte Check)
   2554          		switch(u_buf)
   2555          		{
   2556          			case IP_SONY_FIRST_BYTE:
   2557          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   2558          				stUART5.RX_ENABLE = IP_SONY_SECOND_BYTE;
   2559          				break;
   2560          
   2561          			case IP_TSM_FIRST_BYTE:
   2562          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   2563          				stUART5.RX_ENABLE = IP_TSM_SECOND_BYTE;
   2564          				break;
   2565          
   2566          			case IP_DRS_FIRST_BYTE:
   2567          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DRS_FIRST_BYTE);
   2568          				stUART5.RX_ENABLE = IP_DRS_SECOND_BYTE;
   2569          				break;
   2570          
   2571          			case IP_TCM_FIRST_BYTE:
   2572          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TCM_FIRST_BYTE);
   2573          				stUART5.RX_ENABLE = IP_TCM_SECOND_BYTE;
   2574          				break;
   2575          
   2576          			case IP_PTZ_FIRST_BYTE:
   2577          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PTZ_FIRST_BYTE);
   2578          				stUART5.RX_ENABLE = IP_PTZ_SECOND_BYTE;
   2579          				break;
   2580          
   2581          			case IP_DATA_FIRST_BYTE:
   2582          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DATA_FIRST_BYTE);
   2583          				stUART5.RX_ENABLE = IP_DATA_SECOND_BYTE;
   2584          				break;
   2585          
   2586          			case IP_OPTION_FIRST_BYTE:
   2587          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_OPTION_FIRST_BYTE);
   2588          				stUART5.RX_ENABLE = IP_OPTION_SECOND_BYTE;
   2589          				break;
   2590          
   2591          			case IP_PELCO_CHK_BYTE:
   2592          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PELCO_CHK_BYTE);
   2593          				stUART5.RX_ENABLE = IP_PELCO_CHK_BYTE;
   2594          				break;	
   2595          
   2596          			default:
   2597          				break;
   2598          		}
   2599          	}
   2600          	else
   2601          	{
   2602          		if(stUART5.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   2603          		else 
   2604          		{
   2605          			if(stUART5.RX_BUF_INDEX >= 0x03)
   2606          			{
   2607          				u_Index_Chk = Rx_DATA_BYTE;
   2608          			}
   2609          			else
   2610          			{
   2611          				if(stUART5.RX_STATUS == 0x00)
   2612          				{
   2613          					u_Index_Chk = Rx_LENGTH_BYTE;
   2614          				}
   2615          			}
   2616          		}
   2617          
   2618          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   2619          		Rx_Usart_Comm_Handling(u_Index_Chk, u_buf);
   2620          	}
   2621          }
