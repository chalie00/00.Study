   1668          /*******************************************************************************
   1669          * Function Name  : USART2_IRQHandler
   1670          * Description    : This function handles USART2 interrupt request.
   1671          * Input           : MDIN - PA3 USART2_RX
   1672          * Output         : MDIN - PA2 USART2_TX
   1673          *******************************************************************************/
   1674          void USART2_IRQHandler(void)
   1675          {
   1676          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1677          
   1678          	// =======================================
   1679          	// USART2 TX Process 
   1680          	// =======================================
   1681          	if (USART_GetITStatus(USART2, USART_IT_TC) != RESET)
   1682          	{
   1683          
   1684          		if(stUSART2.TX_CNT < stUSART2.TX_MAX)
   1685          		{
   1686          			Tx_Buf = stUSART2.TX_BUF[stUSART2.TX_CNT];
   1687          			stUSART2.TX_CNT++;				
   1688          
   1689          			USART_SendData(USART2, Tx_Buf);
   1690          		}
   1691          		else
   1692          		{
   1693          			stUSART2.TX_CNT = 0x00;
   1694          			stUSART2.TX_MAX = 0x00;
   1695          		}
   1696          
   1697          		USART_ClearITPendingBit(USART2, USART_IT_TC);
   1698          
   1699          	}
   1700          
   1701          	// =======================================
   1702          	// USART2 RX Process
   1703          	// =======================================
   1704          	if (USART_GetITStatus(USART2, USART_IT_RXNE) != RESET)
   1705          	{
   1706          		Rx_Buf = USART_ReceiveData(USART2);
   1707          	}     
   1708          }
