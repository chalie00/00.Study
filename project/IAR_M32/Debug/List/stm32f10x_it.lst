    941          /*******************************************************************************
    942          * Function Name  : SYSTEM_Data_Handling
    943          *******************************************************************************/
    944          void SYSTEM_Data_Buffer(void)
    945          {
    946          	u8 i = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    947          
    948          	if(stUSART1.RX_PARITY == 0x01)
    949          	{
    950          		u_buf = stUSART1.RX_BUF[0];
    951          
    952          		switch(u_buf)
    953          		{
    954          /*
    955          			case TILT_FIRST_BYTE:
    956          
    957          				u_length = stUSART1.RX_BUF[2];
    958          
    959          				u_max = u_length + 5;
    960          
    961          				stCOOR.RX_BUF[stCOOR.RX_INDEX][0] = stCOOR.RX_INDEX + 1;
    962          
    963          				for(i=0; i<u_max; i++)
    964          				{
    965          					stCOOR.RX_BUF[stCOOR.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    966          					stUSART1.RX_BUF[i] = 0x00;
    967          				}
    968          
    969          				stCOOR.RX_INDEX++;
    970          				
    971          				break;
    972          */				
    973          			case TILT_FIRST_BYTE:
    974          			case IP_SONY_FIRST_BYTE:
    975          			case IP_TSM_FIRST_BYTE:
    976          			case IP_DRS_FIRST_BYTE:
    977          			case IP_PTZ_FIRST_BYTE:
    978          			case IP_DATA_FIRST_BYTE:
    979          			case IP_OPTION_FIRST_BYTE:
    980          
    981          				u_length = stUSART1.RX_BUF[2];
    982          
    983          				u_max = u_length + 5;
    984          
    985          				stSYS.RX_BUF[stSYS.RX_INDEX][0] = stSYS.RX_INDEX + 1;
    986          
    987          				for(i=0; i<u_max; i++)
    988          				{
    989          					stSYS.RX_BUF[stSYS.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    990          					stUSART1.RX_BUF[i] = 0x00;
    991          				}
    992          
    993          				stSYS.RX_INDEX++;
    994          
    995          				break;
    996          
    997          			default:
    998          				break;
    999          		}
   1000          
   1001          		stUSART1.RX_PARITY = 0x00;
   1002          
   1003          	}
   1004          }
