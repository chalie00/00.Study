   1458          /*******************************************************************************
   1459          * Function Name  : Main_Protocol_Data_Pasher
   1460          * Description    : This function handles USART1 interrupt request.
   1461          * Input           : Main - PA10 USART1_RX
   1462          * Output         : Main - PA9 USART1_TX
   1463          *******************************************************************************/
   1464          void Main_Protocol_Data_Pasher(u8 u_buf)
   1465          {
   1466          	u8 u_Index_Chk = 0;
   1467          
   1468          	if(stUSART1.RX_ENABLE == 0x00)
   1469          	{
   1470          		// 첫번째 바이트 체크 (Sync Byte Check)
   1471          		switch(u_buf)
   1472          		{
   1473          			case TILT_FIRST_BYTE:
   1474          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, TILT_FIRST_BYTE);
   1475          				stUSART1.RX_ENABLE = TILT_SECOND_BYTE;
   1476          				break;
   1477          
   1478          			case IP_SONY_FIRST_BYTE:
   1479          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   1480          				stUSART1.RX_ENABLE = IP_SONY_SECOND_BYTE;
   1481          				break;
   1482          
   1483          			case IP_TSM_FIRST_BYTE:
   1484          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   1485          				stUSART1.RX_ENABLE = IP_TSM_SECOND_BYTE;
   1486          				break;
   1487          
   1488          			default:
   1489          				break;
   1490          		}
   1491          	}
   1492          	else
   1493          	{
   1494          		if(stUSART1.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   1495          		else 
   1496          		{
   1497          			if(stUSART1.RX_BUF_INDEX >= 0x03)
   1498          			{
   1499          				u_Index_Chk = Rx_DATA_BYTE;
   1500          			}
   1501          			else
   1502          			{
   1503          				if(stUSART1.RX_STATUS == 0x00)
   1504          				{
   1505          					u_Index_Chk = Rx_LENGTH_BYTE;
   1506          				}
   1507          			}
   1508          		}
   1509          
   1510          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   1511          		Rx_Main_Comm_Handling(u_Index_Chk, u_buf);
   1512          	}
   1513          }
