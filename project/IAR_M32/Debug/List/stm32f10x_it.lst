   1765          void TSM_Data_Pasher(u8 u_Buf)
   1766          {
   1767          	u8 i = 0;
   1768          	u8 u_Data_P1 = 0x00, u_Data_P2 = 0x00;
   1769          	u16 u16_Convert_Data = 0x0000;
   1770          	u8 u_Convert_Data  = 0x00;	
   1771          
   1772          	if(stUART4.RX_ENABLE == 0x01)
   1773          	{
   1774          		stUART4.RX_BUF[stUART4.RX_CNT++] = u_Buf;
   1775          
   1776          		if(stUART4.RX_BUF_MAX == 0x00)
   1777          		{
   1778          			switch(stUART4.RX_BUF[1])
   1779          			{
