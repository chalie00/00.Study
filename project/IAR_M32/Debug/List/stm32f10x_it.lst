   1278          void Rx_Main_Second_Comm_Check(u8 u_buf, u8 u_andbyte)
   1279          {
   1280          	if(u_buf == u_andbyte)
   1281          	{
   1282          		stUSART1.RX_BUF[1] =  u_buf;
   1283          		stUSART1.RX_BUF_INDEX++;
   1284          		stUSART1.RX_BUF_LENGTH = 0x01;
   1285          	}
   1286          	else
   1287          	{
   1288          		stUSART1.RX_BUF_INDEX = 0x00;					// Index Clear
   1289          		stUSART1.RX_BUF[0] = 0x00;						// Buffer Clear
   1290          		stUSART1.RX_ENABLE = 0x00;						// Command Check Byte Clear
   1291          	}	
   1292          }
