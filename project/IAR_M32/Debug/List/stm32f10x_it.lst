   1497          /*******************************************************************************
   1498          * Function Name  : Sony_Data_Pasher
   1499          * Description    : This function handles USART1 interrupt request.
   1500          * Input           : Main - PA10 USART1_RX
   1501          * Output         : Main - PA9 USART1_TX
   1502          *******************************************************************************/
   1503          void Sony_Data_Pasher(u8 u_buf)
   1504          {
   1505          	if(stBYPASS.RX_STATUS)
   1506          	{
   1507          		stBYPASS.RX_BUF[stBYPASS.RX_CNT++] = u_buf;
   1508          
   1509          		if(u_buf == 0xFF)
   1510          		{
   1511          			stBYPASS.RX_STATUS = 0x00;
   1512          			stBYPASS.RX_CNT = 0x00;
   1513          			stBYPASS.RX_PARITY = 0x01;			
   1514          		}
   1515          	}
   1516          	else
   1517          	{
   1518          		if(u_buf == SONY_FIRST_BYTE)
   1519          		{
   1520          			stBYPASS.RX_STATUS = 0x01;
   1521          			stBYPASS.RX_BUF[0] = u_buf;
   1522          			stBYPASS.RX_CNT++;
   1523          			
   1524          		}
   1525          	}
   1526          }
