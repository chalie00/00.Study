    903          void SYSTEM_Data_Buffer(void)
    904          {
    905          	u8 i = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    906          
    907          	if(stUSART1.RX_PARITY == 0x01)
    908          	{
    909          		u_buf = stUSART1.RX_BUF[0];
    910          
    911          		switch(u_buf)
    912          		{
    913          /*
    914          			case TILT_FIRST_BYTE:
    915          
    916          				u_length = stUSART1.RX_BUF[2];
    917          
    918          				u_max = u_length + 5;
    919          
    920          				stCOOR.RX_BUF[stCOOR.RX_INDEX][0] = stCOOR.RX_INDEX + 1;
    921          
    922          				for(i=0; i<u_max; i++)
    923          				{
    924          					stCOOR.RX_BUF[stCOOR.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    925          					stUSART1.RX_BUF[i] = 0x00;
    926          				}
    927          
    928          				stCOOR.RX_INDEX++;
    929          				
    930          				break;
    931          */				
    932          			case TILT_FIRST_BYTE:
    933          			case IP_SONY_FIRST_BYTE:
    934          			case IP_TSM_FIRST_BYTE:
    935          			case IP_DRS_FIRST_BYTE:
    936          			case IP_PTZ_FIRST_BYTE:
    937          			case IP_DATA_FIRST_BYTE:
    938          			case IP_OPTION_FIRST_BYTE:
    939          
    940          				u_length = stUSART1.RX_BUF[2];
    941          
    942          				u_max = u_length + 5;
    943          
    944          				stSYS.RX_BUF[stSYS.RX_INDEX][0] = stSYS.RX_INDEX + 1;
    945          
    946          				for(i=0; i<u_max; i++)
    947          				{
    948          					stSYS.RX_BUF[stSYS.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    949          					stUSART1.RX_BUF[i] = 0x00;
    950          				}
    951          
    952          				stSYS.RX_INDEX++;
    953          
    954          				break;
    955          
    956          			default:
    957          				break;
    958          		}
    959          
    960          		stUSART1.RX_PARITY = 0x00;
    961          
    962          	}
    963          }
