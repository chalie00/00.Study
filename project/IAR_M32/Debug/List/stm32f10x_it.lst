   1775          /*******************************************************************************
   1776          * Function Name  : TSM_Data_Pasher
   1777          *******************************************************************************/
   1778          void TSM_Data_Pasher(u8 u_Buf)
   1779          {
   1780          	u8 i = 0;
   1781          	u8 u_Data_P1 = 0x00, u_Data_P2 = 0x00;
   1782          	u16 u16_Convert_Data = 0x0000;
   1783          	u8 u_Convert_Data  = 0x00;	
   1784          
   1785          	if(stUART4.RX_ENABLE == 0x01)
   1786          	{
   1787          		stUART4.RX_BUF[stUART4.RX_CNT++] = u_Buf;
   1788          
   1789          		if(stUART4.RX_BUF_MAX == 0x00)
   1790          		{
   1791          			switch(stUART4.RX_BUF[1])
   1792          			{
   1793          				case TSM_CALL_ALL_TEMPERATURE:
   1794          					stUART4.RX_BUF_MAX = 0x23;
   1795          					break;
   1796          
   1797          				case TSM_CALL_FIRMWARE_VERSION:
   1798          					stUART4.RX_BUF_MAX = 0x08;
   1799          					break;
   1800          
   1801          				case TCM_COOLER_RUNTIME_INQ:
   1802          					if(stINFO.THERMAL_MODEL == TCM_640)
   1803          					{
   1804          						stUART4.RX_BUF_MAX = 0x07;
   1805          					}
   1806          					break;
   1807          
   1808          				default:
   1809          					stUART4.RX_BUF_MAX = 0x06;
   1810          					break;
   1811          			}
   1812          		}
   1813          
   1814          		// Rx Data Check & Initialization
   1815          		if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf == 0xAF))
   1816          		{
   1817          			switch(stUART4.RX_BUF_MAX)
   1818          			{
   1819          				case 0x06:
   1820          
   1821          					switch(stUART4.RX_REQUEST_CMD)
   1822          					{
   1823          						case TSM_CALL_MODULE_STATUS:
   1824          
   1825          							if(stUART4.RX_BUF[1] == TSM_CALL_MODULE_STATUS)
   1826          							{
   1827          								u_Data_P1 = stUART4.RX_BUF[2];
   1828          
   1829          								if(u_Data_P1) 
   1830          								{
   1831          									stTSM.MODULE_ENABLE = 0x01;
   1832          								}
   1833          								
   1834          								stTSM.MODULE_STATUS = u_Data_P1;
   1835          								stINFO.MODULE_TYPE = TSM_MODULE;
   1836          
   1837          								stUART4.RX_REQUEST_CMD = 0x00;
   1838          							}
   1839          								
   1840          							break;
   1841          							
   1842          						case TSM_CALL_DIGITAL_ZOOM_POSITION:
   1843          
   1844          							if(stUART4.RX_BUF[1] == TSM_CALL_DIGITAL_ZOOM_POSITION)
   1845          							{
   1846          								u_Data_P1 = stUART4.RX_BUF[4];
   1847          								u16_Convert_Data |= u_Data_P1;
   1848          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1849          
   1850          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1851          
   1852          								stUART4.RX_REQUEST_CMD = 0x00;
   1853          							}
   1854          
   1855          							break;
   1856          
   1857          						case TSM_CALL_OPTICAL_ZOOM_POSITION:
   1858          							break;
   1859          
   1860          						case TSM_CALL_TOTAL_ZOOM_POSITION:
   1861          							
   1862          							if(stUART4.RX_BUF[1] == TSM_CALL_TOTAL_ZOOM_POSITION)
   1863          							{
   1864          								u_Data_P1 = stUART4.RX_BUF[2];
   1865          								u_Data_P2 = stUART4.RX_BUF[3];
   1866          
   1867          								u16_Convert_Data = u_Data_P1 << 8;
   1868          								u16_Convert_Data |= u_Data_P2;
   1869          
   1870          								stTSM.ZOOM_CNT = u16_Convert_Data;
   1871          
   1872          								stUART4.RX_REQUEST_CMD = 0x00;
   1873          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_TOTAL_ZOOM_POSITION;
   1874          							}
   1875          							
   1876          							break;
   1877          
   1878          						case TSM_CALL_FOCUS_POSITION:
   1879          
   1880          							if(stUART4.RX_BUF[1] == TSM_CALL_FOCUS_POSITION)
   1881          							{
   1882          								u_Data_P1 = stUART4.RX_BUF[2];
   1883          								u_Data_P2 = stUART4.RX_BUF[3];
   1884          
   1885          								u16_Convert_Data = u_Data_P1 << 8;
   1886          								u16_Convert_Data |= u_Data_P2;
   1887          
   1888          								stTSM.FOCUS_CNT = u16_Convert_Data;
   1889          
   1890          								stUART4.RX_REQUEST_CMD = 0x00;
   1891          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_FOCUS_POSITION;								
   1892          							}
   1893          							
   1894          							break;
   1895          
   1896          						case TSM_CALL_PRODUCT_IDENTIFICATION:
   1897          
   1898          							if(stUART4.RX_BUF[1] == TSM_CALL_PRODUCT_IDENTIFICATION)
   1899          							{
   1900          								// Camera Information Data Check							
   1901          								stINFO.THERMAL_MODEL = stUART4.RX_BUF[2];
   1902          								stINFO.THERMAL_LENS = stUART4.RX_BUF[4];
   1903          								
   1904          								stUART4.RX_REQUEST_CMD = 0x00;
   1905          							}
   1906          							
   1907          							break;
   1908          
   1909          						case TSM_CALL_CAMERA_INFORMATION:
   1910          
   1911          							if(stUART4.RX_BUF[1] == TSM_CALL_CAMERA_INFORMATION)
   1912          							{
   1913          								// Camera Information Data Check
   1914          								stINFO.THERMAL_VER_HIGH = stUART4.RX_BUF[3];
   1915          								stINFO.THERMAL_VER_LOW = stUART4.RX_BUF[4];
   1916          								
   1917          								stUART4.RX_REQUEST_CMD = 0x00;
   1918          							}
   1919          							
   1920          							break;
   1921          
   1922          						case TSM_CALL_FPGA_VERSION:
   1923          
   1924          							if(stUART4.RX_BUF[1] == TSM_CALL_FPGA_VERSION)
   1925          							{
   1926          								stINFO.FPGA_VERSION_HIGH = stUART4.RX_BUF[2];
   1927          								stINFO.FPGA_VERSION_LOW = stUART4.RX_BUF[3];
   1928          
   1929          								stUART4.RX_REQUEST_CMD = 0x00;
   1930          							}
   1931          							
   1932          							break;
   1933          							
   1934          						case TSM_CALL_TEMPERATURE_MODE:
   1935          
   1936          							if(stUART4.RX_BUF[1] == TSM_CALL_TEMPERATURE_MODE)
   1937          							{
   1938          								// Camera Information Data Check
   1939          								stINFO.THERMAL_TEMP_MODE = stUART4.RX_BUF[2];
   1940          								
   1941          								stUART4.RX_REQUEST_CMD = 0x00;
   1942          							}
   1943          							
   1944          							break;
   1945          
   1946          						case TCM_TOTAL_ZOOM_POSITION_INQ:
   1947          
   1948          							if(stUART4.RX_BUF[1] == TCM_TOTAL_ZOOM_POSITION_INQ)
   1949          							{
   1950          								u_Data_P1 = stUART4.RX_BUF[2];
   1951          								u_Data_P2 = stUART4.RX_BUF[3];
   1952          
   1953          								u16_Convert_Data = u_Data_P1 << 8;
   1954          								u16_Convert_Data |= u_Data_P2;
   1955          
   1956          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1957          
   1958          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1959          
   1960          								stUART4.RX_REQUEST_CMD = 0x00;
   1961          							}
   1962          							
   1963          							break;
   1964          
   1965          						case TCM_GET_FW_UPDATE_INQ:
   1966          
   1967          							if(stUART4.RX_BUF[1] == TCM_GET_FW_UPDATE_INQ)
   1968          							{
   1969          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[2];
   1970          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[3];
   1971          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   1972          									
   1973          								stUART4.RX_REQUEST_CMD = 0x00;
   1974          							}
   1975          							
   1976          							break;
   1977          
   1978          						case TCM_GET_ENGINE_VERSION:
   1979          
   1980          							if(stUART4.RX_BUF[1] == TCM_GET_ENGINE_VERSION)
   1981          							{
   1982          								stINFO.THERMAL_ENGINE_VER_HW = stUART4.RX_BUF[2];
   1983          								stINFO.THERMAL_ENGINE_VER_FW = stUART4.RX_BUF[3];
   1984          
   1985          								stUART4.RX_REQUEST_CMD = 0x00;
   1986          							}
   1987          							
   1988          							break;		
   1989          
   1990          						case TCM_FOCUS_POSITION_INQ:
   1991          
   1992          							if(stUART4.RX_BUF[1] == TCM_FOCUS_POSITION_INQ)
   1993          							{
   1994          								u_Data_P1 = stUART4.RX_BUF[2];
   1995          								u_Data_P2 = stUART4.RX_BUF[3];
   1996          
   1997          								u16_Convert_Data = u_Data_P1 << 8;
   1998          								u16_Convert_Data |= u_Data_P2;
   1999          
   2000          								stTSM.FOCUS_CNT = u16_Convert_Data;
   2001          
   2002          								stUART4.RX_REQUEST_CMD = 0x00;
   2003          							}
   2004          
   2005          							break;	
   2006          
   2007          						default:
   2008          							break;
   2009          					}
   2010          						
   2011          					break;
   2012          
   2013          				case 0x07:
   2014          					switch(stUART4.RX_REQUEST_CMD)
   2015          					{
   2016          						case TCM_COOLER_RUNTIME_INQ:
   2017          
   2018          								stINFO.THERMAL_ENGINE_TIME_DAY= 0x0000;
   2019          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[2]<<8);
   2020          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[3]&0xFF);
   2021          
   2022          								stINFO.THERMAL_ENGINE_TIME_HOUR = 0x0000;
   2023          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[4]<<8);
   2024          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[5]&0xFF);
   2025          
   2026          								stINFO.THERMAL_ENGINE_DAY_HIGH = stUART4.RX_BUF[2];
   2027          								stINFO.THERMAL_ENGINE_DAY_LOW = stUART4.RX_BUF[3];
   2028          								stINFO.THERMAL_ENGINE_HOUR_HIGH = stUART4.RX_BUF[4];
   2029          								stINFO.THERMAL_ENGINE_HOUR_LOW = stUART4.RX_BUF[5];								
   2030          								
   2031          								stINFO.TCM640_ENGINE_RUNTIME_COMPLETE = TRUE;
   2032          								
   2033          								stUART4.RX_REQUEST_CMD = 0x00;	
   2034          
   2035          							break;
   2036          					}
   2037          					break;
   2038          					
   2039          				case 0x08:
   2040          					
   2041          					switch(stUART4.RX_REQUEST_CMD)
   2042          					{
   2043          						case TSM_CALL_FIRMWARE_VERSION:
   2044          
   2045          							if(stUART4.RX_BUF[1] == TSM_CALL_FIRMWARE_VERSION)
   2046          							{
   2047          								// Camera Information Data Check
   2048          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   2049          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[5];
   2050          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[6];
   2051          								
   2052          								stUART4.RX_REQUEST_CMD = 0x00;
   2053          							}
   2054          							
   2055          							break;
   2056          
   2057          						default:
   2058          							break;
   2059          					}
   2060          							
   2061          					break;
   2062          					
   2063          				case 0x23:
   2064          					
   2065          					//stTEMP.TEMPERATURE_READ_COMPLETE = 0x01;	// 온도 체크 데이터 수신 완료.
   2066          					
   2067          					break;
   2068          
   2069          				default:
   2070          					break;
   2071          			}
   2072          		
   2073          			for(i=0;i<stUART4.RX_BUF_MAX;i++)
   2074          			{
   2075          				//stTEMP.RX_BUF[i] = stUART4.RX_BUF[i];
   2076          				stUART4.RX_BUF[i] = 0x00;
   2077          			}
   2078          
   2079          			stUART4.RX_ENABLE = 0x00;
   2080          			stUART4.RX_CNT = 0x00;
   2081          			stUART4.RX_BUF_MAX = 0x00;
   2082          			stUART4.RX_PARITY = 0x01;
   2083          			
   2084          		}
   2085          		else if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf != 0xAF))
   2086          		{
   2087          			for(i=0;i<stUART4.RX_CNT;i++)
   2088          			{
   2089          				stUART4.RX_BUF[i] = 0x00;
   2090          			}
   2091          
   2092          			stUART4.RX_ENABLE = 0x00;
   2093          			stUART4.RX_CNT = 0x00;
   2094          			stUART4.RX_BUF_MAX = 0x00;
   2095          			stUART4.RX_REQUEST_ERROR = 0x01;			
   2096          		}
   2097          	}
   2098          
   2099          	if((stUART4.RX_ENABLE == 0x00) && (u_Buf == 0xA0))
   2100          	{
   2101          		stUART4.RX_BUF[0] = u_Buf;
   2102          		stUART4.RX_CNT++;
   2103          		stUART4.RX_ENABLE = 0x01;
   2104          	}
   2105          }
