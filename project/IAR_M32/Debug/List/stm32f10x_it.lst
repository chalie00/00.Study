   2130          			stUART4.RX_ENABLE = 0x00;
   2131          			stUART4.RX_CNT = 0x00;
   2132          			stUART4.RX_BUF_MAX = 0x00;
   2133          			stUART4.RX_REQUEST_ERROR = 0x01;			
   2134          		}
   2135          	}
   2136          
   2137          	if((stUART4.RX_ENABLE == 0x00) && (u_Buf == 0xA0))
   2138          	{
   2139          		stUART4.RX_BUF[0] = u_Buf;
   2140          		stUART4.RX_CNT++;
   2141          		stUART4.RX_ENABLE = 0x01;
   2142          	}
   2143          }
