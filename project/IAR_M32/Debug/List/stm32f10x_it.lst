   1496          	if(stBYPASS.RX_STATUS)
   1497          	{
   1498          		stBYPASS.RX_BUF[stBYPASS.RX_CNT++] = u_buf;
   1499          
   1500          		if(u_buf == 0xFF)
   1501          		{
   1502          			stBYPASS.RX_STATUS = 0x00;
   1503          			stBYPASS.RX_CNT = 0x00;
   1504          			stBYPASS.RX_PARITY = 0x01;			
   1505          		}
   1506          	}
   1507          	else
   1508          	{
   1509          		if(u_buf == SONY_FIRST_BYTE)
