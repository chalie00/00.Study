   1579          /*******************************************************************************
   1580          * Function Name  : USART1_IRQHandler
   1581          * Description    : This function handles USART1 interrupt request.
   1582          * Input           : Main - PA10 USART1_RX
   1583          * Output         : Main - PA9 USART1_TX
   1584          *******************************************************************************/
   1585          void USART1_IRQHandler(void)
   1586          {
   1587          	u8 Rx_Buf = 0x00;
   1588          	
   1589            	// =======================================
   1590            	// USART1 TX Process : TILT -> MAIN Board
   1591            	// =======================================
   1592          	if (USART_GetITStatus(USART1, USART_IT_TC) != RESET)
   1593          	{
   1594          		if (stUSART1.TX_CNT < stUSART1.TX_MAX) 
   1595          		{
   1596          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1597          		}
   1598          		else 
   1599          		{
   1600          			stUSART1.TX_MAX = 0;
   1601          			stUSART1.TX_CNT = 0;
   1602          		}
   1603          		
   1604          		USART_ClearITPendingBit(USART1, USART_IT_TC);
   1605          	}
   1606          
   1607            	// =======================================
   1608            	// USART1 RX Process : TILT <- MAIN Board
   1609            	// =======================================
   1610          	if (USART_GetITStatus(USART1, USART_IT_RXNE) != RESET)
   1611          	{
   1612          		Rx_Buf = USART_ReceiveData(USART1);
   1613          
   1614          		Main_Protocol_Data_Pasher(Rx_Buf);
   1615          		
   1616          		//Sony_Data_Pasher(Rx_Buf);
   1617          
   1618          		//if(stUSART_ERROR.RX_CNT == UART4_RX_BUF_MAX) stUSART_ERROR.RX_CNT = 0;
   1619          
   1620          		//stUSART_ERROR.RX_BUF[stUSART_ERROR.RX_CNT++] = Rx_Buf;
   1621          	}
   1622          }
