   2257          /*******************************************************************************
   2258          * Function Name  : UART4_IRQHandler
   2259          * Description    : This function handles UART4 interrupt request.
   2260          * Input           : - PC11 UART4_RX
   2261          * Output         : - PC10 UART4_TX
   2262          *******************************************************************************/
   2263          void UART4_IRQHandler(void)
   2264          {
   2265          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   2266          
   2267            	// =======================================
   2268            	// USART4 TX Process 
   2269            	// =======================================
   2270          	if (USART_GetITStatus(UART4, USART_IT_TC) != RESET)
   2271          	{
   2272          		if(stUART4.TX_CNT < stUART4.TX_MAX)
   2273          		{
   2274          			Tx_Buf = stUART4.TX_BUF[stUART4.TX_CNT];
   2275          			stUART4.TX_CNT++;				
   2276          
   2277          			USART_SendData(UART4, Tx_Buf);
   2278          		}
   2279          		else
   2280          		{
   2281          			stUART4.TX_CNT = 0x00;
   2282          			stUART4.TX_MAX = 0x00;
   2283          		}
   2284          
   2285          		USART_ClearITPendingBit(UART4, USART_IT_TC);
   2286          	}
   2287          	
   2288            	// =======================================
   2289            	// USART4 RX Process
