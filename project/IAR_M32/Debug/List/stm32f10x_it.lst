   1690          /*******************************************************************************
   1691          * Function Name  : USART2_IRQHandler
   1692          * Description    : This function handles USART2 interrupt request.
   1693          * Input           : MDIN - PA3 USART2_RX
   1694          * Output         : MDIN - PA2 USART2_TX
   1695          *******************************************************************************/
   1696          void USART2_IRQHandler(void)
   1697          {
   1698          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1699          
   1700          	// =======================================
   1701          	// USART2 TX Process 
   1702          	// =======================================
   1703          	if (USART_GetITStatus(USART2, USART_IT_TC) != RESET)
   1704          	{
   1705          
   1706          		if(stUSART2.TX_CNT < stUSART2.TX_MAX)
   1707          		{
   1708          			Tx_Buf = stUSART2.TX_BUF[stUSART2.TX_CNT];
   1709          			stUSART2.TX_CNT++;				
   1710          
   1711          			USART_SendData(USART2, Tx_Buf);
   1712          		}
   1713          		else
   1714          		{
   1715          			stUSART2.TX_CNT = 0x00;
   1716          			stUSART2.TX_MAX = 0x00;
   1717          		}
   1718          
   1719          		USART_ClearITPendingBit(USART2, USART_IT_TC);
   1720          
   1721          	}
   1722          
   1723          	// =======================================
   1724          	// USART2 RX Process
   1725          	// =======================================
   1726          	if (USART_GetITStatus(USART2, USART_IT_RXNE) != RESET)
   1727          	{
   1728          		Rx_Buf = USART_ReceiveData(USART2);
   1729          	}     
   1730          }
