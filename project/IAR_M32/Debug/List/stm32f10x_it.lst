   2390          		if(stUART5.RX_BUF_INDEX == 0x01)
   2391          		{
   2392          			switch(stUART5.RX_ENABLE)
   2393          			{
   2394          				case IP_SONY_SECOND_BYTE:
   2395          					Rx_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   2396          					break;
   2397          
   2398          				case IP_TSM_SECOND_BYTE:
   2399          					Rx_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   2400          					break;
   2401          
   2402          				case IP_DRS_SECOND_BYTE:
   2403          					Rx_Second_Comm_Check(u_buf, IP_DRS_SECOND_BYTE);
   2404          					break;
   2405          
   2406          				case IP_TCM_SECOND_BYTE:
   2407          					Rx_Second_Comm_Check(u_buf, IP_TCM_SECOND_BYTE);
   2408          					break;
   2409          					
   2410          				case IP_PTZ_SECOND_BYTE:
   2411          					Rx_Second_Comm_Check(u_buf, IP_PTZ_SECOND_BYTE);
   2412          					break;
   2413          
   2414          				case IP_DATA_SECOND_BYTE:
   2415          					Rx_Second_Comm_Check(u_buf, IP_DATA_SECOND_BYTE);
   2416          					break;	
   2417          
   2418          				case IP_OPTION_SECOND_BYTE:
   2419          					Rx_Second_Comm_Check(u_buf, IP_OPTION_SECOND_BYTE);
   2420          					break;		
   2421          
   2422          				case IP_PELCO_CHK_BYTE:
   2423          					Rx_Pelco_Address_Check(u_buf);
   2424          					break;
   2425          
   2426          				default:
   2427          					break;
   2428          			}
   2429          		}	
   2430          	}
   2431          
   2432          	// 세번째 바이트 체크 (Data Length)
   2433          	if(u_sync == Rx_LENGTH_BYTE)
   2434          	{
   2435          		#if(ERROR_CHECK_BUF)	
   2436          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   2437          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   2438          		#endif
