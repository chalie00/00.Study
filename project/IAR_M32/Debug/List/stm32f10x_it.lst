   2542          /*******************************************************************************
   2543          * Function Name  : Protocol_Command_Check
   2544          * Description    : This function handles UART5 interrupt request.
   2545          * Input           : TILT <-> Thermal IP Board (- PA10 UART5_RX)
   2546          * Output         : TILT <-> Thermal IP Board (- PA9 UART5_TX)
   2547          *******************************************************************************/
   2548          void Protocol_Command_Check(u8 u_buf)
   2549          {
   2550          	u8 u_Index_Chk = 0;
   2551          
   2552          	if(stUART5.RX_ENABLE == 0x00)
   2553          	{
   2554          		// 첫번째 바이트 체크 (Sync Byte Check)
   2555          		switch(u_buf)
   2556          		{
   2557          			case IP_SONY_FIRST_BYTE:
   2558          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   2559          				stUART5.RX_ENABLE = IP_SONY_SECOND_BYTE;
   2560          				break;
   2561          
   2562          			case IP_TSM_FIRST_BYTE:
   2563          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   2564          				stUART5.RX_ENABLE = IP_TSM_SECOND_BYTE;
   2565          				break;
   2566          
   2567          			case IP_DRS_FIRST_BYTE:
   2568          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DRS_FIRST_BYTE);
   2569          				stUART5.RX_ENABLE = IP_DRS_SECOND_BYTE;
   2570          				break;
   2571          
   2572          			case IP_TCM_FIRST_BYTE:
   2573          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TCM_FIRST_BYTE);
   2574          				stUART5.RX_ENABLE = IP_TCM_SECOND_BYTE;
   2575          				break;
   2576          
   2577          			case IP_PTZ_FIRST_BYTE:
   2578          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PTZ_FIRST_BYTE);
   2579          				stUART5.RX_ENABLE = IP_PTZ_SECOND_BYTE;
   2580          				break;
   2581          
   2582          			case IP_DATA_FIRST_BYTE:
   2583          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DATA_FIRST_BYTE);
   2584          				stUART5.RX_ENABLE = IP_DATA_SECOND_BYTE;
   2585          				break;
   2586          
   2587          			case IP_OPTION_FIRST_BYTE:
   2588          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_OPTION_FIRST_BYTE);
   2589          				stUART5.RX_ENABLE = IP_OPTION_SECOND_BYTE;
   2590          				break;
   2591          
   2592          			case IP_PELCO_CHK_BYTE:
   2593          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PELCO_CHK_BYTE);
   2594          				stUART5.RX_ENABLE = IP_PELCO_CHK_BYTE;
   2595          				break;	
   2596          
   2597          			default:
   2598          				break;
   2599          		}
   2600          	}
   2601          	else
   2602          	{
   2603          		if(stUART5.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   2604          		else 
   2605          		{
   2606          			if(stUART5.RX_BUF_INDEX >= 0x03)
   2607          			{
   2608          				u_Index_Chk = Rx_DATA_BYTE;
   2609          			}
   2610          			else
   2611          			{
   2612          				if(stUART5.RX_STATUS == 0x00)
   2613          				{
   2614          					u_Index_Chk = Rx_LENGTH_BYTE;
   2615          				}
   2616          			}
   2617          		}
   2618          
   2619          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   2620          		Rx_Usart_Comm_Handling(u_Index_Chk, u_buf);
   2621          	}
   2622          }
