   2112          /*******************************************************************************
   2113          * Function Name  : Rx_Sony_Inquiry_Command
   2114          *******************************************************************************/
   2115          void SONY_Data_Pasher(u8 u_buf)
   2116          {
   2117          	u16 u_CZoomData = 0x0000, u_CFocusData = 0x0000;
   2118          
   2119          	// Data Save
   2120          	if(stSONY.RX_CNT >= 0x02)
   2121          	{
   2122          		stSONY.RX_BUF[stSONY.RX_CNT] = u_buf;
   2123          		stSONY.RX_CNT++;
   2124          	}
   2125          
   2126          	if(stSONY.RX_CNT == 0x04)
   2127          	{
   2128          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[3] == SONY_BYTE_END))
   2129          		{
   2130          			switch(stUART4.RX_REQUEST_CMD)
   2131          			{
   2132          				case SONY_POWER_INQ:
   2133          
   2134          					stSONY.MODULE_STATUS = stSONY.RX_BUF[2];
   2135          
   2136          					switch(stSONY.MODULE_STATUS)
   2137          					{
   2138          						case 0x02:
   2139          							stSONY.MODULE_ENABLE = 1;
   2140          							break;
   2141          
   2142          						case 0x03:
   2143          							stSONY.MODULE_ENABLE = 0;
   2144          							break;
   2145          
   2146          						default:
   2147          							break;
   2148          					}
   2149          
   2150          					stINFO.MODULE_TYPE = SONY_MODULE;
   2151          					
   2152          					break;
   2153          
   2154          				default:
   2155          					break;
   2156          			}
   2157          
   2158          			SONY_Data_Clear();
   2159          			
   2160          			stSONY.RX_CNT = 0;
   2161          			stSONY.RX_END_FLAG = 0x01;	
   2162          		}
   2163          	}
   2164          
   2165          	// Data Compare Check
   2166          	if(stSONY.RX_CNT == 0x07)
   2167          	{
   2168          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[6] == SONY_BYTE_END))
   2169          		{
   2170          			switch(stUART4.RX_REQUEST_CMD)
   2171          			{
   2172          				case SONY_CALL_ZOOM_POSITION:
   2173          					
   2174          					u_CZoomData = stSONY.RX_BUF[2];
   2175          					stSONY.ZOOM_CNT = u_CZoomData << 12;
   2176          					u_CZoomData = stSONY.RX_BUF[3];
   2177          					stSONY.ZOOM_CNT |= u_CZoomData << 8;
   2178          					u_CZoomData = stSONY.RX_BUF[4];
   2179          					stSONY.ZOOM_CNT |= u_CZoomData << 4;
   2180          					u_CZoomData = stSONY.RX_BUF[5];
   2181          					stSONY.ZOOM_CNT |= u_CZoomData;
   2182          
   2183          					if(stSONY.RX_BUF[6] == 0x00) 
   2184          					{
   2185          						stUART4.RX_REQUEST_ERROR = 0x01;
   2186          					}
   2187          					else 
   2188          					{
   2189          						stUART4.RX_REQUEST_CMD = 0x00;
   2190          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_ZOOM_POSITION;
   2191          					}
   2192          					
   2193          					break;
   2194          
   2195          				case SONY_CALL_FOCUS_POSITION:
   2196          					
   2197          					u_CFocusData = stSONY.RX_BUF[2];
   2198          					stSONY.FOCUS_CNT = u_CFocusData << 12;
   2199          					u_CFocusData = stSONY.RX_BUF[3];
   2200          					stSONY.FOCUS_CNT |= u_CFocusData << 8;
   2201          					u_CFocusData = stSONY.RX_BUF[4];
   2202          					stSONY.FOCUS_CNT |= u_CFocusData << 4;
   2203          					u_CFocusData = stSONY.RX_BUF[5];
   2204          					stSONY.FOCUS_CNT |= u_CFocusData;
   2205          
   2206          					if(stSONY.RX_BUF[6] == 0x00) 
   2207          					{
   2208          						stUART4.RX_REQUEST_ERROR = 0x01;
   2209          					}
   2210          					else 
   2211          					{
   2212          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_FOCUS_POSITION;
   2213          						stUART4.RX_REQUEST_CMD = 0x00;
   2214          					}
   2215          
   2216          					break;
   2217          
   2218          				default:
   2219          
   2220          					stUART4.RX_ENABLE = 0x00;
   2221          					if(stUART4.RX_REQUEST_ERROR != 0x00) stUART4.RX_REQUEST_ERROR = 0x00;
   2222          					
   2223          					break;
   2224          			}
   2225          
   2226          			SONY_Data_Clear();
   2227          			
   2228          			stSONY.RX_CNT = 0;
   2229          			stSONY.RX_END_FLAG = 0x01;
   2230          
   2231          		}
   2232          	}
   2233          	
   2234          	if(stSONY.RX_END_FLAG == 0x00)
   2235          	{
   2236          		// Second Data Check
   2237          		if((u_buf == SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2238          		{
   2239          			stSONY.RX_BUF[1] = SONY_BYTE_2;
   2240          			stSONY.RX_CNT++;
   2241          		}
   2242          		else if((u_buf != SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2243          		{
   2244          			stSONY.RX_BUF[0] = 0x00;
   2245          			stSONY.RX_CNT = 0;
   2246          		}
   2247          
   2248          		// First Data Check
   2249          		if((u_buf == SONY_BYTE_1) && (stSONY.RX_CNT == 0))
   2250          		{
   2251          			stSONY.RX_BUF[0] = SONY_BYTE_1;
   2252          			stSONY.RX_CNT++;
   2253          		}
   2254          	}
   2255          	else
   2256          	{
   2257          		stSONY.RX_END_FLAG = 0x00;
   2258          	}
   2259          
   2260          	if(stSONY.RX_CNT >= SONY_RX_BUF_MAX)
   2261          	{
   2262          		stSONY.RX_CNT = 0;
   2263          		SONY_Data_Clear();
   2264          		
   2265          	}
   2266          }
