   1280          /*******************************************************************************
   1281          * Function Name  : Boot_Status_Check_Parser
   1282          *******************************************************************************/
   1283          void Boot_Status_Check_Parser(void)
   1284          {
   1285          	if((stUSART1.RX_BUF[0] == TILT_FIRST_BYTE) && (stUSART1.RX_BUF[4] == TILT_BOOT_STATUS))
   1286          	{
   1287          		if(stSYS.START == STOP)
   1288          		{
   1289          			stSYS.BOOT_STATUS = READY;
   1290          		}
