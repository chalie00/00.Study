    549          /*******************************************************************************
    550          * Function Name  : TIM2_IRQHandler
    551          * Description    : This function handles TIM2 global interrupt request.
    552          * Input          : None
    553          * Output         : None
    554          * Return         : None
    555          * Timer		: 10ms
    556          *******************************************************************************/
    557          u8 g_Tx_Buf = 0x00;
    558          
    559          void TIM2_IRQHandler(void)
    560          {
    561          	if (TIM_GetITStatus(TIM2, TIM_IT_Update) != RESET)
    562          	{
    563          		TIM_ClearITPendingBit(TIM2, TIM_IT_Update);	
    564          
    565          		// =======================================
    566          		// LED Control : 1s
    567          		// =======================================
    568          		stLED_SET.LED_COUNT++;
    569          		if(stLED_SET.LED_COUNT >= 100)
    570          		{
    571          			stLED_SET.LED_COUNT = 0;
    572          			stLED_SET.LED_FLAG ^= 0xFF;
    573          
    574          			//if(stLED_SET.LED_FLAG) LED1_ON;
    575          			//else LED1_OFF;
    576          		}
    577          
    578          		if(stINFO.BOOT_FLAG == 0x01)
    579          		{
    580          			stINFO.BOOT_CHECK_LIMIT_ms++;
    581          
    582          			if(stINFO.BOOT_CHECK_LIMIT_ms >= 1000)
    583          			{
    584          				stINFO.BOOT_CHECK_LIMIT_ms = 0;
    585          				stINFO.BOOT_FLAG = 0x00;
    586          			}
    587          		}
    588          
    589          		
    590          
    591          		// =======================================
    592          		// Wiper1 Dwell Timer
    593          		// =======================================
    594          		if(stWIPER1.DWELL_FLAG == 0x01)
    595          		{
    596          			stWIPER1.DWELL_TIME_10ms++;
    597          			if(stWIPER1.DWELL_TIME_10ms > 100)
    598          			{
    599          				stWIPER1.DWELL_TIME_10ms = 0;
    600          				stWIPER1.DWELL_TIME_1s++;
    601          
    602          				if(stWIPER1.DWELL_TIME <= stWIPER1.DWELL_TIME_1s)
    603          				{
    604          					stWIPER1.DWELL_FLAG = 0x00;
    605          					if(stWIPER1.ACTIVE_COUNT != 0)
    606          					{
    607          						//Run_Wiper(WIPER_ON, DIRECTION_CW);				// Wiper On
    608          					}
    609          
    610          					stWIPER1.DWELL_TIME_1s = 0;
    611          				}
    612          			}
    613          		}
    614          		
    615          		// ================================================================
    616          		// Wiper2 Dwell Timer
    617          		// ================================================================
    618          		if(stWIPER2.DWELL_TIMER_FLAG)
    619          		{
    620          			stWIPER2.DWELL_TIMER_ms++;
    621          
    622          			if(stWIPER2.DWELL_TIMER_ms >= 100)	// 1s
    623          			{
    624          				stWIPER2.DWELL_TIMER_s++;
    625          				stWIPER2.DWELL_TIMER_ms = 0x00;
    626          		
    627          				if(stWIPER2.DWELL_TIMER_s >= stWIPER2.DWELL_TIME_DATA)
    628          				{
    629          					stWIPER2.DWELL_END_FLAG = 0x01;
    630          					stWIPER2.DWELL_TIMER_s = 0x00;
    631          					stWIPER2.DWELL_TIMER_FLAG = 0x00;
    632          				}
    633          			}
    634          		}
    635          
    636          		if((stWIPER2.COUNT == 0x00) && (stWIPER2.DWELL_TIMER_FLAG != 0x00))
    637          		{
    638          			stWIPER2.DWELL_TIMER_FLAG = 0x00;
    639          			stWIPER2.DWELL_TIMER_ms = 0;
    640          			stWIPER2.DWELL_TIMER_s = 0;
    641          		}
    642          
    643          		// ================================================================
    644          		// LRF Booting Check Time
    645          		// ================================================================
    646          		if(stLRF.INIT_FLAG == 0x01)
    647          		{
    648          			stLRF.INIT_TIME++;
    649          
    650          			if(stLRF.INIT_TIME >= LRF_BOOTING_TIME)
    651          			{
    652          				stLRF.INIT_ACTIVE = 0x01;
    653          
    654          				stLRF.INIT_TIME = 0x00;
    655          				stLRF.INIT_FLAG = 0x00;
    656          			}
    657          		}
    658          
    659          		if(stLRF.CONTINUOUS_FLAG == 0x01)
    660          		{
    661          			stLRF.CONTINUOUS_TIME++;
    662          
    663          			if(stLRF.CONTINUOUS_TIME > stLRF.CONTINUOUS_TIME_MAX)
    664          			{
    665          				stLRF.CONTINUOUS_ACTIVE = 0x01;
    666          				
    667          				stLRF.CONTINUOUS_TIME = 0;
    668          			}
    669          		}
    670          
    671          		// ================================================================
    672          		// BYPASS Tx Check (전송 후 500ms 후 Clear)
    673          		// ================================================================
    674          		if(stBYPASS.TX_FLAG == 0x01)
    675          		{
    676          			stBYPASS.TX_TIMER++;
    677          
    678          			if(stBYPASS.TX_TIMER >= 50)
    679          			{
    680          				stBYPASS.TX_TIMER = 0;
    681          				stBYPASS.TX_FLAG = 0x00;
    682          			}
    683          		}
    684          
    685          		// ================================================================
    686          		// WIPER CONTINUOUS TIMER
    687          		// ================================================================
    688          		if(stWIPER2.CONTINUOUS_FLAG == 0x00)
    689          		{
    690          			if((stWIPER2.CONTINUOUS_ms != 0) || (stWIPER2.CONTINUOUS_s != 0) || (stWIPER2.CONTINUOUS_m != 0) || (stWIPER2.CONTINUOUS_h != 0))
    691          			{
    692          				stWIPER2.CONTINUOUS_ms = 0;
    693          				stWIPER2.CONTINUOUS_s = 0;
    694          				stWIPER2.CONTINUOUS_m = 0;
    695          				stWIPER2.CONTINUOUS_h = 0;
    696          			}
    697          		}
    698          			
    699          		if(stWIPER2.CONTINUOUS_FLAG == 0x01)
    700          		{
    701          			stWIPER2.CONTINUOUS_ms++;
    702          			
    703          			if(stWIPER2.CONTINUOUS_ms >= 100)
    704          			{
    705          				stWIPER2.CONTINUOUS_ms = 0;
    706          				stWIPER2.CONTINUOUS_s++;
    707          				
    708          				if(stWIPER2.CONTINUOUS_s >= 60)
    709          				{
    710          					stWIPER2.CONTINUOUS_s = 0;
    711          					stWIPER2.CONTINUOUS_m++;
    712          
    713          					if(stWIPER2.CONTINUOUS_m >= 60)
    714          					{
    715          						stWIPER2.CONTINUOUS_m = 0;
    716          						stWIPER2.CONTINUOUS_h++;
    717          
    718          						if(stWIPER2.CONTINUOUS_h >= stWIPER2.CONTINUOUS_TIME)
    719          						{
    720          							stWIPER2.CONTINUOUS_h = 0;
    721          							stWIPER2.CONTINUOUS_FLAG = 0x00;
    722          							stWIPER2.CONTINUOUS_STOP_FLAG = 0x01;
    723          						}
    724          					}
    725          				}
    726          			}
    727          		}
    728          
    729          		// ================================================================
    730          		// TEST TIMER
    731          		// ================================================================
    732          /*
    733          		if(stUSART2.TX_CNT < stUSART2.TX_MAX)
    734          		{
    735          			g_Tx_Buf = stUSART2.TX_BUF[stUSART2.TX_CNT];
    736          			stUSART2.TX_CNT++;				
    737          
    738          			USART_SendData(USART2, g_Tx_Buf);
    739          		}
    740          		else
    741          		{
    742          			stUSART2.TX_CNT = 0x00;
    743          			stUSART2.TX_MAX = 0x00;
    744          		
    745          			//USART_SendData(USART2, Tx_Buf);
    746          		}
    747          */
    748          
    749          	}	
    750          }
