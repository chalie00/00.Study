   1548          /*******************************************************************************
   1549          * Function Name  : Sony_Data_Pasher
   1550          * Description    : This function handles USART1 interrupt request.
   1551          * Input           : Main - PA10 USART1_RX
   1552          * Output         : Main - PA9 USART1_TX
   1553          *******************************************************************************/
   1554          void Sony_Data_Pasher(u8 u_buf)
   1555          {
   1556          	if(stBYPASS.RX_STATUS)
   1557          	{
   1558          		stBYPASS.RX_BUF[stBYPASS.RX_CNT++] = u_buf;
   1559          
   1560          		if(u_buf == 0xFF)
   1561          		{
   1562          			stBYPASS.RX_STATUS = 0x00;
   1563          			stBYPASS.RX_CNT = 0x00;
   1564          			stBYPASS.RX_PARITY = 0x01;			
   1565          		}
   1566          	}
   1567          	else
   1568          	{
   1569          		if(u_buf == SONY_FIRST_BYTE)
   1570          		{
   1571          			stBYPASS.RX_STATUS = 0x01;
   1572          			stBYPASS.RX_BUF[0] = u_buf;
   1573          			stBYPASS.RX_CNT++;
   1574          			
   1575          		}
   1576          	}
   1577          }
