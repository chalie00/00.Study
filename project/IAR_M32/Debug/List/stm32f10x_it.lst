   1453          /*******************************************************************************
   1454          * Function Name  : Main_Protocol_Data_Pasher
   1455          * Description    : This function handles USART1 interrupt request.
   1456          * Input           : Main - PA10 USART1_RX
   1457          * Output         : Main - PA9 USART1_TX
   1458          *******************************************************************************/
   1459          void Main_Protocol_Data_Pasher(u8 u_buf)
   1460          {
   1461          	u8 u_Index_Chk = 0;
   1462          
   1463          	if(stUSART1.RX_ENABLE == 0x00)
   1464          	{
   1465          		// 첫번째 바이트 체크 (Sync Byte Check)
   1466          		switch(u_buf)
   1467          		{
   1468          			case TILT_FIRST_BYTE:
   1469          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, TILT_FIRST_BYTE);
   1470          				stUSART1.RX_ENABLE = TILT_SECOND_BYTE;
   1471          				break;
   1472          
   1473          			case IP_SONY_FIRST_BYTE:
   1474          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   1475          				stUSART1.RX_ENABLE = IP_SONY_SECOND_BYTE;
   1476          				break;
   1477          
   1478          			case IP_TSM_FIRST_BYTE:
   1479          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   1480          				stUSART1.RX_ENABLE = IP_TSM_SECOND_BYTE;
   1481          				break;
   1482          
   1483          			default:
   1484          				break;
   1485          		}
   1486          	}
   1487          	else
   1488          	{
   1489          		if(stUSART1.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   1490          		else 
   1491          		{
   1492          			if(stUSART1.RX_BUF_INDEX >= 0x03)
   1493          			{
   1494          				u_Index_Chk = Rx_DATA_BYTE;
   1495          			}
   1496          			else
   1497          			{
   1498          				if(stUSART1.RX_STATUS == 0x00)
   1499          				{
   1500          					u_Index_Chk = Rx_LENGTH_BYTE;
   1501          				}
   1502          			}
   1503          		}
   1504          
   1505          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   1506          		Rx_Main_Comm_Handling(u_Index_Chk, u_buf);
   1507          	}
   1508          }
