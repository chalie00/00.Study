   2529          /*******************************************************************************
   2530          * Function Name  : Protocol_Command_Check
   2531          * Description    : This function handles UART5 interrupt request.
   2532          * Input           : TILT <-> Thermal IP Board (- PA10 UART5_RX)
   2533          * Output         : TILT <-> Thermal IP Board (- PA9 UART5_TX)
   2534          *******************************************************************************/
   2535          void Protocol_Command_Check(u8 u_buf)
   2536          {
   2537          	u8 u_Index_Chk = 0;
   2538          
   2539          	if(stUART5.RX_ENABLE == 0x00)
   2540          	{
   2541          		// 첫번째 바이트 체크 (Sync Byte Check)
   2542          		switch(u_buf)
   2543          		{
   2544          			case IP_SONY_FIRST_BYTE:
   2545          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   2546          				stUART5.RX_ENABLE = IP_SONY_SECOND_BYTE;
   2547          				break;
   2548          
   2549          			case IP_TSM_FIRST_BYTE:
   2550          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   2551          				stUART5.RX_ENABLE = IP_TSM_SECOND_BYTE;
   2552          				break;
   2553          
   2554          			case IP_DRS_FIRST_BYTE:
   2555          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DRS_FIRST_BYTE);
   2556          				stUART5.RX_ENABLE = IP_DRS_SECOND_BYTE;
   2557          				break;
   2558          
   2559          			case IP_TCM_FIRST_BYTE:
   2560          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_TCM_FIRST_BYTE);
   2561          				stUART5.RX_ENABLE = IP_TCM_SECOND_BYTE;
   2562          				break;
   2563          
   2564          			case IP_PTZ_FIRST_BYTE:
   2565          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PTZ_FIRST_BYTE);
   2566          				stUART5.RX_ENABLE = IP_PTZ_SECOND_BYTE;
   2567          				break;
   2568          
   2569          			case IP_DATA_FIRST_BYTE:
   2570          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_DATA_FIRST_BYTE);
   2571          				stUART5.RX_ENABLE = IP_DATA_SECOND_BYTE;
   2572          				break;
   2573          
   2574          			case IP_OPTION_FIRST_BYTE:
   2575          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_OPTION_FIRST_BYTE);
   2576          				stUART5.RX_ENABLE = IP_OPTION_SECOND_BYTE;
   2577          				break;
   2578          
   2579          			case IP_PELCO_CHK_BYTE:
   2580          				Rx_Usart_Comm_Handling(Rx_SYNC_BYTE, IP_PELCO_CHK_BYTE);
   2581          				stUART5.RX_ENABLE = IP_PELCO_CHK_BYTE;
   2582          				break;	
   2583          
   2584          			default:
   2585          				break;
   2586          		}
   2587          	}
   2588          	else
   2589          	{
   2590          		if(stUART5.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   2591          		else 
   2592          		{
   2593          			if(stUART5.RX_BUF_INDEX >= 0x03)
   2594          			{
   2595          				u_Index_Chk = Rx_DATA_BYTE;
   2596          			}
   2597          			else
   2598          			{
   2599          				if(stUART5.RX_STATUS == 0x00)
   2600          				{
   2601          					u_Index_Chk = Rx_LENGTH_BYTE;
   2602          				}
   2603          			}
   2604          		}
   2605          
   2606          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   2607          		Rx_Usart_Comm_Handling(u_Index_Chk, u_buf);
   2608          	}
   2609          }
