   2094          /*******************************************************************************
   2095          * Function Name  : Rx_Sony_Inquiry_Command
   2096          *******************************************************************************/
   2097          void SONY_Data_Pasher(u8 u_buf)
   2098          {
   2099          	u16 u_CZoomData = 0x0000, u_CFocusData = 0x0000;
   2100          
   2101          	// Data Save
   2102          	if(stSONY.RX_CNT >= 0x02)
   2103          	{
   2104          		stSONY.RX_BUF[stSONY.RX_CNT] = u_buf;
   2105          		stSONY.RX_CNT++;
   2106          	}
   2107          
   2108          	if(stSONY.RX_CNT == 0x04)
   2109          	{
   2110          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[3] == SONY_BYTE_END))
   2111          		{
   2112          			switch(stUART4.RX_REQUEST_CMD)
   2113          			{
   2114          				case SONY_POWER_INQ:
   2115          
   2116          					stSONY.MODULE_STATUS = stSONY.RX_BUF[2];
   2117          
   2118          					switch(stSONY.MODULE_STATUS)
   2119          					{
   2120          						case 0x02:
   2121          							stSONY.MODULE_ENABLE = 1;
   2122          							break;
   2123          
   2124          						case 0x03:
   2125          							stSONY.MODULE_ENABLE = 0;
   2126          							break;
   2127          
   2128          						default:
   2129          							break;
   2130          					}
   2131          
   2132          					stINFO.MODULE_TYPE = SONY_MODULE;
   2133          					
   2134          					break;
   2135          
   2136          				default:
   2137          					break;
   2138          			}
   2139          
   2140          			SONY_Data_Clear();
   2141          			
   2142          			stSONY.RX_CNT = 0;
   2143          			stSONY.RX_END_FLAG = 0x01;	
   2144          		}
   2145          	}
   2146          
   2147          	// Data Compare Check
   2148          	if(stSONY.RX_CNT == 0x07)
   2149          	{
   2150          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[6] == SONY_BYTE_END))
   2151          		{
   2152          			switch(stUART4.RX_REQUEST_CMD)
   2153          			{
   2154          				case SONY_CALL_ZOOM_POSITION:
   2155          					
   2156          					u_CZoomData = stSONY.RX_BUF[2];
   2157          					stSONY.ZOOM_CNT = u_CZoomData << 12;
   2158          					u_CZoomData = stSONY.RX_BUF[3];
   2159          					stSONY.ZOOM_CNT |= u_CZoomData << 8;
   2160          					u_CZoomData = stSONY.RX_BUF[4];
   2161          					stSONY.ZOOM_CNT |= u_CZoomData << 4;
   2162          					u_CZoomData = stSONY.RX_BUF[5];
   2163          					stSONY.ZOOM_CNT |= u_CZoomData;
   2164          
   2165          					if(stSONY.RX_BUF[6] == 0x00) 
   2166          					{
   2167          						stUART4.RX_REQUEST_ERROR = 0x01;
   2168          					}
   2169          					else 
   2170          					{
   2171          						stUART4.RX_REQUEST_CMD = 0x00;
   2172          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_ZOOM_POSITION;
   2173          					}
   2174          					
   2175          					break;
   2176          
   2177          				case SONY_CALL_FOCUS_POSITION:
   2178          					
   2179          					u_CFocusData = stSONY.RX_BUF[2];
   2180          					stSONY.FOCUS_CNT = u_CFocusData << 12;
   2181          					u_CFocusData = stSONY.RX_BUF[3];
   2182          					stSONY.FOCUS_CNT |= u_CFocusData << 8;
   2183          					u_CFocusData = stSONY.RX_BUF[4];
   2184          					stSONY.FOCUS_CNT |= u_CFocusData << 4;
   2185          					u_CFocusData = stSONY.RX_BUF[5];
   2186          					stSONY.FOCUS_CNT |= u_CFocusData;
   2187          
   2188          					if(stSONY.RX_BUF[6] == 0x00) 
   2189          					{
   2190          						stUART4.RX_REQUEST_ERROR = 0x01;
   2191          					}
   2192          					else 
   2193          					{
   2194          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_FOCUS_POSITION;
   2195          						stUART4.RX_REQUEST_CMD = 0x00;
   2196          					}
   2197          
   2198          					break;
   2199          
   2200          				default:
   2201          
   2202          					stUART4.RX_ENABLE = 0x00;
   2203          					if(stUART4.RX_REQUEST_ERROR != 0x00) stUART4.RX_REQUEST_ERROR = 0x00;
   2204          					
   2205          					break;
   2206          			}
   2207          
   2208          			SONY_Data_Clear();
   2209          			
   2210          			stSONY.RX_CNT = 0;
   2211          			stSONY.RX_END_FLAG = 0x01;
   2212          
   2213          		}
   2214          	}
   2215          	
   2216          	if(stSONY.RX_END_FLAG == 0x00)
   2217          	{
   2218          		// Second Data Check
   2219          		if((u_buf == SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2220          		{
   2221          			stSONY.RX_BUF[1] = SONY_BYTE_2;
   2222          			stSONY.RX_CNT++;
   2223          		}
   2224          		else if((u_buf != SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2225          		{
   2226          			stSONY.RX_BUF[0] = 0x00;
   2227          			stSONY.RX_CNT = 0;
   2228          		}
   2229          
   2230          		// First Data Check
   2231          		if((u_buf == SONY_BYTE_1) && (stSONY.RX_CNT == 0))
   2232          		{
   2233          			stSONY.RX_BUF[0] = SONY_BYTE_1;
   2234          			stSONY.RX_CNT++;
   2235          		}
   2236          	}
   2237          	else
   2238          	{
   2239          		stSONY.RX_END_FLAG = 0x00;
   2240          	}
   2241          
   2242          	if(stSONY.RX_CNT >= SONY_RX_BUF_MAX)
   2243          	{
   2244          		stSONY.RX_CNT = 0;
   2245          		SONY_Data_Clear();
   2246          		
   2247          	}
   2248          }
