   1726          /*******************************************************************************
   1727          * Function Name  : USART3_IRQHandler
   1728          * Description    : This function handles USART1 interrupt request.
   1729          * Input           : Camera - PB11 USART3_RX[
   1730          * Output         : Camera - PB10 USART3_TX
   1731          *******************************************************************************/
   1732          void USART3_IRQHandler(void)
   1733          {
   1734          	
   1735          	
   1736            	// =======================================
   1737            	// USART3 TX Process 
   1738            	// =======================================
   1739          	if (USART_GetITStatus(USART3, USART_IT_TC) != RESET){
   1740          		//RED
   1741          		GPIOB -> ODR ^= GPIO_Pin_9;
   1742          	}
   1743          	USART_ClearITPendingBit(USART3, USART_IT_TC);
   1744          	
   1745          	
   1746            	// =======================================
   1747            	// USART3 RX Process
   1748            	// =======================================
   1749          	if (USART_GetITStatus(USART3, USART_IT_RXNE) != RESET){
   1750          		u8 Rx_Buf = 0x00;
   1751          		Rx_Buf = USART_ReceiveData(USART3);
   1752          		USART_SendData(USART3, Rx_Buf);
   1753          		if (RxBuf != 0x00) {
   1754          			//Green
   1755          			GPIOB -> ODR ^= GPIO_Pin_8;	
   1756          		}
   1757          	}
   1758          	USART_ClearITPendingBit(USART3, USART_IT_RXNE);
   1759          }
