   1037          /****************************************************************************************************
   1038          * Function Name  : Boot_Status_Data_Trans
   1039          * Data Architecture :
   1040          BYTE1[0]		BYTE2[1]		BYTE3[2]		BYTE4[3]		BYTE5[4]		BYTE5[5]		BYTE5[LENGTH+4]
   1041          0xCA		0xAC		LENGTH		ADDR		COMM		DATA...		CS
   1042          0xCA		0xAC		0x04		0x01		0xD1		DATA...		CS
   1043          
   1044          DATA : 0x00: Nondata, 0x01: ACTIVE Status, 0x02 : Boot Ready Status
   1045          *****************************************************************************************************/
   1046          void Boot_Status_Data_Trans(void)
   1047          {
   1048          	u8 u_Cmd = TILT_BOOT_STATUS, u_Leng = 0x04, u_ID = MDIN_LAYER_CENTER_MSG;
   1049          
   1050          	switch(stSYS.BOOT_STATUS)
   1051          	{
   1052          		// TILT B/D 가 POWER OFF 인 경우
   1053          		case STOP:
   1054          			break;
   1055          
   1056          		// TILT B/D 가 MODULE 을 정상 경우
   1057          		case ACTIVE:
   1058          
   1059          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1060          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1061          			stUSART1.TX_BUF[2] = u_Leng;
   1062          			stUSART1.TX_BUF[3] = u_ID;
   1063          			stUSART1.TX_BUF[4] = u_Cmd;
   1064          			stUSART1.TX_BUF[5] = ACTIVE;
   1065          			stUSART1.TX_BUF[6] = 0x00;
   1066          			
   1067          			stUSART1.TX_BUF[7] = 0x00;		
   1068          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1069          
   1070          			stUSART1.TX_BUF[8] = 0xCF;
   1071          			
   1072          			stUSART1.TX_CNT = 0;
   1073          			stUSART1.TX_MAX = u_Leng + 5;
   1074          
   1075          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1076          
   1077          			stSYS.BOOT_STATUS = STOP;
   1078          			
   1079          			break;
   1080          
   1081          		// TILT B/D 가 MODULE 을 정상 부팅 못하였을 경우 (MAIN으로 부터 체크 송신을 받았으나 MODULE 미인식, 데이터 비정상)
   1082          		case READY:
   1083          
   1084          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1085          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1086          			stUSART1.TX_BUF[2] = u_Leng;
   1087          			stUSART1.TX_BUF[3] = u_ID;
   1088          			stUSART1.TX_BUF[4] = u_Cmd;
   1089          			stUSART1.TX_BUF[5] = READY;
   1090          			stUSART1.TX_BUF[6] = 0x00;
   1091          			
   1092          			stUSART1.TX_BUF[7] = 0x00;		
   1093          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1094          
   1095          			stUSART1.TX_BUF[8] = 0xCF;
   1096          
   1097          			stUSART1.TX_CNT = 0;
   1098          			stUSART1.TX_MAX = u_Leng + 5;
   1099          
   1100          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1101          
   1102          			stSYS.BOOT_STATUS = STOP;
   1103          		
   1104          			break;
   1105          
   1106          		default:
   1107          			break;
   1108          	}
   1109          }
