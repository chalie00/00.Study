   1677          /*******************************************************************************
   1678          * Function Name  : USART2_IRQHandler
   1679          * Description    : This function handles USART2 interrupt request.
   1680          * Input           : MDIN - PA3 USART2_RX
   1681          * Output         : MDIN - PA2 USART2_TX
   1682          *******************************************************************************/
   1683          void USART2_IRQHandler(void)
   1684          {
   1685          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1686          
   1687          	// =======================================
   1688          	// USART2 TX Process 
   1689          	// =======================================
   1690          	if (USART_GetITStatus(USART2, USART_IT_TC) != RESET)
   1691          	{
   1692          
   1693          		if(stUSART2.TX_CNT < stUSART2.TX_MAX)
   1694          		{
   1695          			Tx_Buf = stUSART2.TX_BUF[stUSART2.TX_CNT];
   1696          			stUSART2.TX_CNT++;				
   1697          
   1698          			USART_SendData(USART2, Tx_Buf);
   1699          		}
   1700          		else
   1701          		{
   1702          			stUSART2.TX_CNT = 0x00;
   1703          			stUSART2.TX_MAX = 0x00;
   1704          		}
   1705          
   1706          		USART_ClearITPendingBit(USART2, USART_IT_TC);
   1707          
   1708          	}
   1709          
   1710          	// =======================================
   1711          	// USART2 RX Process
   1712          	// =======================================
   1713          	if (USART_GetITStatus(USART2, USART_IT_RXNE) != RESET)
   1714          	{
   1715          		Rx_Buf = USART_ReceiveData(USART2);
   1716          	}     
   1717          }
