   1890          								
   1891          								stUART4.RX_REQUEST_CMD = 0x00;
   1892          							}
   1893          							
   1894          							break;
   1895          
   1896          						case TSM_CALL_CAMERA_INFORMATION:
   1897          
   1898          							if(stUART4.RX_BUF[1] == TSM_CALL_CAMERA_INFORMATION)
   1899          							{
   1900          								// Camera Information Data Check
   1901          								stINFO.THERMAL_VER_HIGH = stUART4.RX_BUF[3];
   1902          								stINFO.THERMAL_VER_LOW = stUART4.RX_BUF[4];
   1903          								
   1904          								stUART4.RX_REQUEST_CMD = 0x00;
   1905          							}
   1906          							
   1907          							break;
   1908          
   1909          						case TSM_CALL_FPGA_VERSION:
   1910          
   1911          							if(stUART4.RX_BUF[1] == TSM_CALL_FPGA_VERSION)
   1912          							{
