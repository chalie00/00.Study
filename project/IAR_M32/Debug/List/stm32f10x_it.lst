    844          /*******************************************************************************
    845          * Function Name  : SYSTEM_Data_Reordering
    846          *******************************************************************************/
    847          void SYSTEM_Data_Reordering(void)
    848          {
    849          	u8 i = 0x00, j = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    850          	
    851          	if((stSYS.RX_PARITY == 0x00) && (stSYS.RX_REORDERING == 0x01))
    852          	{
    853          		if(stSYS.RX_INDEX == 0x00)
    854          		{
    855          			u_buf = stSYS.RX_BUF[0][1];
    856          		
    857          			switch(u_buf)
    858          			{
    859          				case TILT_FIRST_BYTE:
    860          				case IP_SONY_FIRST_BYTE:
    861          				case IP_TSM_FIRST_BYTE:
    862          				case IP_DRS_FIRST_BYTE:
    863          				case IP_PTZ_FIRST_BYTE:
    864          				case IP_DATA_FIRST_BYTE:
    865          				case IP_OPTION_FIRST_BYTE:
    866          
    867          					u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    868          
    869          					u_max = u_length + 6;
    870          					
    871          					for(j=0; j<u_max; j++)
    872          					{
    873          						stSYS.RX_BUF[0][j] = 0x00;
    874          					}
    875          
    876          					stSYS.RX_REORDERING = 0x00;
    877          					
    878          					break;
    879          
    880          				default:
    881          					break;
    882          			}			
    883          		}
    884          		else
    885          		{
    886          			for(i=1; i<SYS_RX_BUF_CNT; i++)
    887          			{
    888          				if(stSYS.RX_BUF[i][0] != 0x00)
    889          				{
    890          					u_buf = stSYS.RX_BUF[i][1];	// Command Byte Check
    891          					
    892          					switch(u_buf)
    893          					{
    894          						case TILT_FIRST_BYTE:
    895          						case IP_SONY_FIRST_BYTE:
    896          						case IP_TSM_FIRST_BYTE:
    897          						case IP_DRS_FIRST_BYTE:
    898          						case IP_PTZ_FIRST_BYTE:
    899          						case IP_DATA_FIRST_BYTE:
    900          						case IP_OPTION_FIRST_BYTE:
    901          
    902          							u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    903          
    904          							u_max = u_length + 6;
    905          							
    906          							for(j=0; j<u_max; j++)
    907          							{
    908          								stSYS.RX_BUF[i-1][j] = stSYS.RX_BUF[i][j];
    909          								if(stSYS.RX_INDEX > 0) stSYS.RX_BUF[stSYS.RX_INDEX - 1][j] = 0x00;
    910          							}
    911          
    912          							stSYS.RX_REORDERING = 0x00;
    913          							
    914          							break;
    915          
    916          						default:
    917          							break;
    918          
    919          					}
    920          				}
    921          			}
    922          		}
    923          		
    924          		stSYS.RX_COMPLETE = 0x00;
    925          		if(stSYS.RX_INDEX > 0) stSYS.RX_INDEX--;
    926          		
    927          	}
    928          }
