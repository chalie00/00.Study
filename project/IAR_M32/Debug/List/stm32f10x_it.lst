    996          /****************************************************************************************************
    997          * Function Name  : Boot_Status_Data_Trans
    998          * Data Architecture :
    999          BYTE1[0]		BYTE2[1]		BYTE3[2]		BYTE4[3]		BYTE5[4]		BYTE5[5]		BYTE5[LENGTH+4]
   1000          0xCA		0xAC		LENGTH		ADDR		COMM		DATA...		CS
   1001          0xCA		0xAC		0x04		0x01		0xD1		DATA...		CS
   1002          
   1003          DATA : 0x00: Nondata, 0x01: ACTIVE Status, 0x02 : Boot Ready Status
   1004          *****************************************************************************************************/
   1005          void Boot_Status_Data_Trans(void)
   1006          {
   1007          	u8 u_Cmd = TILT_BOOT_STATUS, u_Leng = 0x04, u_ID = MDIN_LAYER_CENTER_MSG;
   1008          
   1009          	switch(stSYS.BOOT_STATUS)
   1010          	{
   1011          		// TILT B/D 가 POWER OFF 인 경우
   1012          		case STOP:
   1013          			break;
   1014          
   1015          		// TILT B/D 가 MODULE 을 정상 경우
   1016          		case ACTIVE:
   1017          
   1018          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1019          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1020          			stUSART1.TX_BUF[2] = u_Leng;
   1021          			stUSART1.TX_BUF[3] = u_ID;
   1022          			stUSART1.TX_BUF[4] = u_Cmd;
   1023          			stUSART1.TX_BUF[5] = ACTIVE;
   1024          			stUSART1.TX_BUF[6] = 0x00;
   1025          			
   1026          			stUSART1.TX_BUF[7] = 0x00;		
   1027          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1028          
   1029          			stUSART1.TX_BUF[8] = 0xCF;
   1030          			
   1031          			stUSART1.TX_CNT = 0;
   1032          			stUSART1.TX_MAX = u_Leng + 5;
   1033          
   1034          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1035          
   1036          			stSYS.BOOT_STATUS = STOP;
   1037          			
   1038          			break;
   1039          
   1040          		// TILT B/D 가 MODULE 을 정상 부팅 못하였을 경우 (MAIN으로 부터 체크 송신을 받았으나 MODULE 미인식, 데이터 비정상)
   1041          		case READY:
   1042          
   1043          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1044          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1045          			stUSART1.TX_BUF[2] = u_Leng;
   1046          			stUSART1.TX_BUF[3] = u_ID;
   1047          			stUSART1.TX_BUF[4] = u_Cmd;
   1048          			stUSART1.TX_BUF[5] = READY;
   1049          			stUSART1.TX_BUF[6] = 0x00;
   1050          			
   1051          			stUSART1.TX_BUF[7] = 0x00;		
   1052          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1053          
   1054          			stUSART1.TX_BUF[8] = 0xCF;
   1055          
   1056          			stUSART1.TX_CNT = 0;
   1057          			stUSART1.TX_MAX = u_Leng + 5;
   1058          
   1059          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1060          
   1061          			stSYS.BOOT_STATUS = STOP;
   1062          		
   1063          			break;
   1064          
   1065          		default:
   1066          			break;
   1067          	}
   1068          }
