   1314          /*******************************************************************************
   1315          * Function Name  : Rx_Main_Comm_Handling
   1316          *******************************************************************************/
   1317          void Rx_Main_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   1318          {
   1319          	// 첫번째 바이트 체크 (Sync Byte Check)
   1320          	if(u_sync == Rx_SYNC_BYTE)	
   1321          	{
   1322          		stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1323          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1324          		stUSART1.RX_BUF_INDEX++;
   1325          		
   1326          		#if(ERROR_CHECK_BUF)
   1327          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1328          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1329          		#endif
   1330          	}
   1331          
   1332          	// 두번째 바이트 체크 (Sync & Byte Check)
   1333          	if(u_sync == Rx_SYNC_AND_BYTE)
   1334          	{
   1335          		#if(ERROR_CHECK_BUF)	
   1336          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1337          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1338          		#endif		
   1339          		
   1340          		// 두번째 명령어 체크 (Sync & Byte Check)
   1341          		if(stUSART1.RX_BUF_INDEX == 0x01)
   1342          		{
   1343          			switch(stUSART1.RX_ENABLE)
   1344          			{
   1345          				case TILT_SECOND_BYTE:
   1346          					Rx_Main_Second_Comm_Check(u_buf, TILT_SECOND_BYTE);
   1347          					break;
   1348          
   1349          				case IP_SONY_SECOND_BYTE:
   1350          					Rx_Main_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   1351          					break;
   1352          					
   1353          				case IP_TSM_SECOND_BYTE:
   1354          					Rx_Main_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   1355          					break;
   1356          
   1357          				default:
   1358          					break;
   1359          			}
   1360          		}	
   1361          	}
   1362          
   1363          	// 세번째 바이트 체크 (Data Length)
   1364          	if(u_sync == Rx_LENGTH_BYTE)
   1365          	{
   1366          		#if(ERROR_CHECK_BUF)	
   1367          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1368          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1369          		#endif
   1370          		
   1371          		if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1372          		{
   1373          			// PELCO-D Command 예외처리
   1374          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1375          			{
   1376          				stUSART1.RX_BUF_LENGTH = 0x02;
   1377          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1378          				
   1379          				stUSART1.RX_BUF[2] =  u_buf;
   1380          				
   1381          				stUSART1.RX_BUF_INDEX++;
   1382          			}					
   1383          		}
   1384          		else
   1385          		{
   1386          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   1387          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1388          			{
   1389          				stUSART1.RX_BUF_LENGTH = u_buf;
   1390          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1391          				
   1392          				stUSART1.RX_BUF[2] =  u_buf;
   1393          				
   1394          				stUSART1.RX_BUF_INDEX++;
   1395          			}
   1396          		}
   1397          	}	
   1398          
   1399          	// 데이터 누적
   1400          	if(u_sync == Rx_DATA_BYTE)
   1401          	{
   1402          		#if(ERROR_CHECK_BUF)
   1403          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1404          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1405          		#endif		
   1406          		
   1407          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1408          
   1409          		stUSART1.RX_BUF_INDEX++;	
   1410          		
   1411          		if(stUSART1.RX_BUF_INDEX == stUSART1.RX_BUF_MAX)
   1412          		{
   1413          			// PELCO-D Command 예외처리
   1414          			if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1415          			{
   1416          				Rx_Pelco_Data_Check();
   1417          			}
   1418          			else
   1419          			{
   1420          				if((stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   1421          				{
   1422          					if(stSYS.START == ACTIVE)
   1423          					{
   1424          						stUSART1.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1425          						stUSART1.RX_PARITY_CNT++;
   1426          						
   1427          						// DEBUG 데이터 체크 ======================================================/
   1428          						//stERROR.PARITY_CNT_CHECK++;
   1429          						//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   1430          						//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
   1431          						// DEBUG 데이터 체크 ===================================================END/
   1432          						
   1433          						//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   1434          					}
   1435          					else
   1436          					{
   1437          						Boot_Status_Check_Parser();
   1438          					}
   1439          
   1440          					stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1441          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   1442          				}
   1443          				else
   1444          				{
   1445          					stUSART1.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1446          					stUSART1.RX_ERROR_CNT++;
   1447          				}
   1448          			}
   1449          			
   1450          			stUSART1.RX_BUF_INDEX = 0x00;
   1451          			stUSART1.RX_BUF_LENGTH = 0x00;
   1452          			
   1453          			stUSART1.RX_ENABLE = 0x00;
   1454          		}
   1455          	}		
   1456          }
