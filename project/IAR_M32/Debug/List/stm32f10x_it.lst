   1257          		stUSART1.RX_BUF_LENGTH = 0x01;
   1258          	}
   1259          	else
   1260          	{
   1261          		stUSART1.RX_BUF_INDEX = 0x00;					// Index Clear
   1262          		stUSART1.RX_BUF[0] = 0x00;						// Buffer Clear
   1263          		stUSART1.RX_ENABLE = 0x00;						// Command Check Byte Clear
   1264          	}	
   1265          }
