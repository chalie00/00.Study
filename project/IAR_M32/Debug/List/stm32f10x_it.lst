   1041          /****************************************************************************************************
   1042          * Function Name  : Boot_Status_Data_Trans
   1043          * Data Architecture :
   1044          BYTE1[0]		BYTE2[1]		BYTE3[2]		BYTE4[3]		BYTE5[4]		BYTE5[5]		BYTE5[LENGTH+4]
   1045          0xCA		0xAC		LENGTH		ADDR		COMM		DATA...		CS
   1046          0xCA		0xAC		0x04		0x01		0xD1		DATA...		CS
   1047          
   1048          DATA : 0x00: Nondata, 0x01: ACTIVE Status, 0x02 : Boot Ready Status
   1049          *****************************************************************************************************/
   1050          void Boot_Status_Data_Trans(void)
   1051          {
   1052          	u8 u_Cmd = TILT_BOOT_STATUS, u_Leng = 0x04, u_ID = MDIN_LAYER_CENTER_MSG;
   1053          
   1054          	switch(stSYS.BOOT_STATUS)
   1055          	{
   1056          		// TILT B/D 가 POWER OFF 인 경우
   1057          		case STOP:
   1058          			break;
   1059          
   1060          		// TILT B/D 가 MODULE 을 정상 경우
   1061          		case ACTIVE:
   1062          
   1063          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1064          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1065          			stUSART1.TX_BUF[2] = u_Leng;
   1066          			stUSART1.TX_BUF[3] = u_ID;
   1067          			stUSART1.TX_BUF[4] = u_Cmd;
   1068          			stUSART1.TX_BUF[5] = ACTIVE;
   1069          			stUSART1.TX_BUF[6] = 0x00;
   1070          			
   1071          			stUSART1.TX_BUF[7] = 0x00;		
   1072          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1073          
   1074          			stUSART1.TX_BUF[8] = 0xCF;
   1075          			
   1076          			stUSART1.TX_CNT = 0;
   1077          			stUSART1.TX_MAX = u_Leng + 5;
   1078          
   1079          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1080          
   1081          			stSYS.BOOT_STATUS = STOP;
   1082          			
   1083          			break;
   1084          
   1085          		// TILT B/D 가 MODULE 을 정상 부팅 못하였을 경우 (MAIN으로 부터 체크 송신을 받았으나 MODULE 미인식, 데이터 비정상)
   1086          		case READY:
   1087          
   1088          			stUSART1.TX_BUF[0] = TILT_FIRST_BYTE;
   1089          			stUSART1.TX_BUF[1] = TILT_SECOND_BYTE;
   1090          			stUSART1.TX_BUF[2] = u_Leng;
   1091          			stUSART1.TX_BUF[3] = u_ID;
   1092          			stUSART1.TX_BUF[4] = u_Cmd;
   1093          			stUSART1.TX_BUF[5] = READY;
   1094          			stUSART1.TX_BUF[6] = 0x00;
   1095          			
   1096          			stUSART1.TX_BUF[7] = 0x00;		
   1097          			stUSART1.TX_BUF[7] = stUSART1.TX_BUF[3] + stUSART1.TX_BUF[4] + stUSART1.TX_BUF[5] + stUSART1.TX_BUF[6];
   1098          
   1099          			stUSART1.TX_BUF[8] = 0xCF;
   1100          
   1101          			stUSART1.TX_CNT = 0;
   1102          			stUSART1.TX_MAX = u_Leng + 5;
   1103          
   1104          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1105          
   1106          			stSYS.BOOT_STATUS = STOP;
   1107          		
   1108          			break;
   1109          
   1110          		default:
   1111          			break;
   1112          	}
   1113          }
