   1737          /*******************************************************************************
   1738          * Function Name  : USART3_IRQHandler
   1739          * Description    : This function handles USART1 interrupt request.
   1740          * Input           : Camera - PB11 USART3_RX
   1741          * Output         : Camera - PB10 USART3_TX
   1742          *******************************************************************************/
   1743          void USART3_IRQHandler(void)
   1744          {
   1745          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1746          
   1747            	// =======================================
   1748            	// USART3 TX Process 
   1749            	// =======================================
   1750          	if (USART_GetITStatus(USART3, USART_IT_TC) != RESET)
   1751          	{ 
   1752          		USART_SendData(USART3, Tx_Buf);
   1753          		
   1754          		USART_ClearITPendingBit(USART3, USART_IT_TC);
   1755          	}
   1756          	
   1757            	// =======================================
   1758            	// USART3 RX Process
   1759            	// =======================================
   1760          	if (USART_GetITStatus(USART3, USART_IT_RXNE) != RESET)
   1761          	{
   1762          		Rx_Buf = USART_ReceiveData(USART3);
   1763          	}
   1764          
   1765          }
