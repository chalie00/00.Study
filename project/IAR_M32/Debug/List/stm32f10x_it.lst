   1431          /*******************************************************************************
   1432          * Function Name  : Main_Protocol_Data_Pasher
   1433          * Description    : This function handles USART1 interrupt request.
   1434          * Input           : Main - PA10 USART1_RX
   1435          * Output         : Main - PA9 USART1_TX
   1436          *******************************************************************************/
   1437          void Main_Protocol_Data_Pasher(u8 u_buf)
   1438          {
   1439          	u8 u_Index_Chk = 0;
   1440          
   1441          	if(stUSART1.RX_ENABLE == 0x00)
   1442          	{
   1443          		// 첫번째 바이트 체크 (Sync Byte Check)
   1444          		switch(u_buf)
   1445          		{
   1446          			case TILT_FIRST_BYTE:
   1447          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, TILT_FIRST_BYTE);
   1448          				stUSART1.RX_ENABLE = TILT_SECOND_BYTE;
   1449          				break;
   1450          
   1451          			case IP_SONY_FIRST_BYTE:
   1452          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   1453          				stUSART1.RX_ENABLE = IP_SONY_SECOND_BYTE;
   1454          				break;
   1455          
   1456          			case IP_TSM_FIRST_BYTE:
   1457          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   1458          				stUSART1.RX_ENABLE = IP_TSM_SECOND_BYTE;
   1459          				break;
   1460          
   1461          			default:
   1462          				break;
   1463          		}
   1464          	}
   1465          	else
   1466          	{
   1467          		if(stUSART1.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   1468          		else 
   1469          		{
   1470          			if(stUSART1.RX_BUF_INDEX >= 0x03)
   1471          			{
   1472          				u_Index_Chk = Rx_DATA_BYTE;
   1473          			}
   1474          			else
   1475          			{
   1476          				if(stUSART1.RX_STATUS == 0x00)
   1477          				{
   1478          					u_Index_Chk = Rx_LENGTH_BYTE;
   1479          				}
   1480          			}
   1481          		}
   1482          
   1483          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   1484          		Rx_Main_Comm_Handling(u_Index_Chk, u_buf);
   1485          	}
   1486          }
