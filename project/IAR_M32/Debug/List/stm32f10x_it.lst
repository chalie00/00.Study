   1874          
   1875          								stUART4.RX_REQUEST_CMD = 0x00;
   1876          							}
   1877          								
   1878          							break;
   1879          							
   1880          						case TSM_CALL_DIGITAL_ZOOM_POSITION:
   1881          
   1882          							if(stUART4.RX_BUF[1] == TSM_CALL_DIGITAL_ZOOM_POSITION)
   1883          							{
   1884          								u_Data_P1 = stUART4.RX_BUF[4];
