    848          /*******************************************************************************
    849          * Function Name  : SYSTEM_Data_Reordering
    850          *******************************************************************************/
    851          void SYSTEM_Data_Reordering(void)
    852          {
    853          	u8 i = 0x00, j = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    854          	
    855          	if((stSYS.RX_PARITY == 0x00) && (stSYS.RX_REORDERING == 0x01))
    856          	{
    857          		if(stSYS.RX_INDEX == 0x00)
    858          		{
    859          			u_buf = stSYS.RX_BUF[0][1];
    860          		
    861          			switch(u_buf)
    862          			{
    863          				case TILT_FIRST_BYTE:
    864          				case IP_SONY_FIRST_BYTE:
    865          				case IP_TSM_FIRST_BYTE:
    866          				case IP_DRS_FIRST_BYTE:
    867          				case IP_PTZ_FIRST_BYTE:
    868          				case IP_DATA_FIRST_BYTE:
    869          				case IP_OPTION_FIRST_BYTE:
    870          
    871          					u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    872          
    873          					u_max = u_length + 6;
    874          					
    875          					for(j=0; j<u_max; j++)
    876          					{
    877          						stSYS.RX_BUF[0][j] = 0x00;
    878          					}
    879          
    880          					stSYS.RX_REORDERING = 0x00;
    881          					
    882          					break;
    883          
    884          				default:
    885          					break;
    886          			}			
    887          		}
    888          		else
    889          		{
    890          			for(i=1; i<SYS_RX_BUF_CNT; i++)
    891          			{
    892          				if(stSYS.RX_BUF[i][0] != 0x00)
    893          				{
    894          					u_buf = stSYS.RX_BUF[i][1];	// Command Byte Check
    895          					
    896          					switch(u_buf)
    897          					{
    898          						case TILT_FIRST_BYTE:
    899          						case IP_SONY_FIRST_BYTE:
    900          						case IP_TSM_FIRST_BYTE:
    901          						case IP_DRS_FIRST_BYTE:
    902          						case IP_PTZ_FIRST_BYTE:
    903          						case IP_DATA_FIRST_BYTE:
    904          						case IP_OPTION_FIRST_BYTE:
    905          
    906          							u_length = stSYS.RX_BUF[i][3];		// Length Byte Check
    907          
    908          							u_max = u_length + 6;
    909          							
    910          							for(j=0; j<u_max; j++)
    911          							{
    912          								stSYS.RX_BUF[i-1][j] = stSYS.RX_BUF[i][j];
    913          								if(stSYS.RX_INDEX > 0) stSYS.RX_BUF[stSYS.RX_INDEX - 1][j] = 0x00;
    914          							}
    915          
    916          							stSYS.RX_REORDERING = 0x00;
    917          							
    918          							break;
    919          
    920          						default:
    921          							break;
    922          
    923          					}
    924          				}
    925          			}
    926          		}
    927          		
    928          		stSYS.RX_COMPLETE = 0x00;
    929          		if(stSYS.RX_INDEX > 0) stSYS.RX_INDEX--;
    930          		
    931          	}
    932          }
