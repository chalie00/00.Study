   2071          								stUART4.RX_REQUEST_CMD = 0x00;	
   2072          
   2073          							break;
   2074          					}
   2075          					break;
   2076          					
   2077          				case 0x08:
   2078          					
   2079          					switch(stUART4.RX_REQUEST_CMD)
   2080          					{
   2081          						case TSM_CALL_FIRMWARE_VERSION:
   2082          
   2083          							if(stUART4.RX_BUF[1] == TSM_CALL_FIRMWARE_VERSION)
