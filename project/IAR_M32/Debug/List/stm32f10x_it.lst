   1296          /*******************************************************************************
   1297          * Function Name  : Rx_Main_Comm_Handling
   1298          *******************************************************************************/
   1299          void Rx_Main_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   1300          {
   1301          	// 첫번째 바이트 체크 (Sync Byte Check)
   1302          	if(u_sync == Rx_SYNC_BYTE)	
   1303          	{
   1304          		stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1305          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1306          		stUSART1.RX_BUF_INDEX++;
   1307          		
   1308          		#if(ERROR_CHECK_BUF)
   1309          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1310          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1311          		#endif
   1312          	}
   1313          
   1314          	// 두번째 바이트 체크 (Sync & Byte Check)
   1315          	if(u_sync == Rx_SYNC_AND_BYTE)
   1316          	{
   1317          		#if(ERROR_CHECK_BUF)	
   1318          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1319          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1320          		#endif		
   1321          		
   1322          		// 두번째 명령어 체크 (Sync & Byte Check)
   1323          		if(stUSART1.RX_BUF_INDEX == 0x01)
   1324          		{
   1325          			switch(stUSART1.RX_ENABLE)
   1326          			{
   1327          				case TILT_SECOND_BYTE:
   1328          					Rx_Main_Second_Comm_Check(u_buf, TILT_SECOND_BYTE);
   1329          					break;
   1330          
   1331          				case IP_SONY_SECOND_BYTE:
   1332          					Rx_Main_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   1333          					break;
   1334          					
   1335          				case IP_TSM_SECOND_BYTE:
   1336          					Rx_Main_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   1337          					break;
   1338          
   1339          				default:
   1340          					break;
   1341          			}
   1342          		}	
   1343          	}
   1344          
   1345          	// 세번째 바이트 체크 (Data Length)
   1346          	if(u_sync == Rx_LENGTH_BYTE)
   1347          	{
   1348          		#if(ERROR_CHECK_BUF)	
   1349          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1350          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1351          		#endif
   1352          		
   1353          		if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1354          		{
   1355          			// PELCO-D Command 예외처리
   1356          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1357          			{
   1358          				stUSART1.RX_BUF_LENGTH = 0x02;
   1359          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1360          				
   1361          				stUSART1.RX_BUF[2] =  u_buf;
   1362          				
   1363          				stUSART1.RX_BUF_INDEX++;
   1364          			}					
   1365          		}
   1366          		else
   1367          		{
   1368          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   1369          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1370          			{
   1371          				stUSART1.RX_BUF_LENGTH = u_buf;
   1372          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1373          				
   1374          				stUSART1.RX_BUF[2] =  u_buf;
   1375          				
   1376          				stUSART1.RX_BUF_INDEX++;
   1377          			}
   1378          		}
   1379          	}	
   1380          
   1381          	// 데이터 누적
   1382          	if(u_sync == Rx_DATA_BYTE)
   1383          	{
   1384          		#if(ERROR_CHECK_BUF)
   1385          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1386          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1387          		#endif		
   1388          		
   1389          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1390          
   1391          		stUSART1.RX_BUF_INDEX++;	
   1392          		
   1393          		if(stUSART1.RX_BUF_INDEX == stUSART1.RX_BUF_MAX)
   1394          		{
   1395          			// PELCO-D Command 예외처리
   1396          			if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1397          			{
   1398          				Rx_Pelco_Data_Check();
   1399          			}
   1400          			else
   1401          			{
   1402          				if((stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   1403          				{
   1404          					if(stSYS.START == ACTIVE)
   1405          					{
   1406          						stUSART1.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1407          						stUSART1.RX_PARITY_CNT++;
   1408          						
   1409          						// DEBUG 데이터 체크 ======================================================/
   1410          						//stERROR.PARITY_CNT_CHECK++;
   1411          						//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   1412          						//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
   1413          						// DEBUG 데이터 체크 ===================================================END/
   1414          						
   1415          						//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   1416          					}
   1417          					else
   1418          					{
   1419          						Boot_Status_Check_Parser();
   1420          					}
   1421          
   1422          					stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1423          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   1424          				}
   1425          				else
   1426          				{
   1427          					stUSART1.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1428          					stUSART1.RX_ERROR_CNT++;
   1429          				}
   1430          			}
   1431          			
   1432          			stUSART1.RX_BUF_INDEX = 0x00;
   1433          			stUSART1.RX_BUF_LENGTH = 0x00;
   1434          			
   1435          			stUSART1.RX_ENABLE = 0x00;
   1436          		}
   1437          	}		
   1438          }
