   1444          /*******************************************************************************
   1445          * Function Name  : Main_Protocol_Data_Pasher
   1446          * Description    : This function handles USART1 interrupt request.
   1447          * Input           : Main - PA10 USART1_RX
   1448          * Output         : Main - PA9 USART1_TX
   1449          *******************************************************************************/
   1450          void Main_Protocol_Data_Pasher(u8 u_buf)
   1451          {
   1452          	u8 u_Index_Chk = 0;
   1453          
   1454          	if(stUSART1.RX_ENABLE == 0x00)
   1455          	{
   1456          		// 첫번째 바이트 체크 (Sync Byte Check)
   1457          		switch(u_buf)
   1458          		{
   1459          			case TILT_FIRST_BYTE:
   1460          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, TILT_FIRST_BYTE);
   1461          				stUSART1.RX_ENABLE = TILT_SECOND_BYTE;
   1462          				break;
   1463          
   1464          			case IP_SONY_FIRST_BYTE:
   1465          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   1466          				stUSART1.RX_ENABLE = IP_SONY_SECOND_BYTE;
   1467          				break;
   1468          
   1469          			case IP_TSM_FIRST_BYTE:
   1470          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   1471          				stUSART1.RX_ENABLE = IP_TSM_SECOND_BYTE;
   1472          				break;
   1473          
   1474          			default:
   1475          				break;
   1476          		}
   1477          	}
   1478          	else
   1479          	{
   1480          		if(stUSART1.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   1481          		else 
   1482          		{
   1483          			if(stUSART1.RX_BUF_INDEX >= 0x03)
   1484          			{
   1485          				u_Index_Chk = Rx_DATA_BYTE;
   1486          			}
   1487          			else
   1488          			{
   1489          				if(stUSART1.RX_STATUS == 0x00)
   1490          				{
   1491          					u_Index_Chk = Rx_LENGTH_BYTE;
   1492          				}
   1493          			}
   1494          		}
   1495          
   1496          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   1497          		Rx_Main_Comm_Handling(u_Index_Chk, u_buf);
   1498          	}
   1499          }
