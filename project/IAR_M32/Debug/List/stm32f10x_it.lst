   1515          /*******************************************************************************
   1516          * Function Name  : Sony_Data_Pasher
   1517          * Description    : This function handles USART1 interrupt request.
   1518          * Input           : Main - PA10 USART1_RX
   1519          * Output         : Main - PA9 USART1_TX
   1520          *******************************************************************************/
   1521          void Sony_Data_Pasher(u8 u_buf)
   1522          {
   1523          	if(stBYPASS.RX_STATUS)
   1524          	{
   1525          		stBYPASS.RX_BUF[stBYPASS.RX_CNT++] = u_buf;
   1526          
   1527          		if(u_buf == 0xFF)
   1528          		{
   1529          			stBYPASS.RX_STATUS = 0x00;
   1530          			stBYPASS.RX_CNT = 0x00;
   1531          			stBYPASS.RX_PARITY = 0x01;			
   1532          		}
   1533          	}
   1534          	else
   1535          	{
   1536          		if(u_buf == SONY_FIRST_BYTE)
   1537          		{
   1538          			stBYPASS.RX_STATUS = 0x01;
   1539          			stBYPASS.RX_BUF[0] = u_buf;
   1540          			stBYPASS.RX_CNT++;
   1541          			
   1542          		}
   1543          	}
   1544          }
