   2603          * Function Name  : UART5_IRQHandler
   2604          * Description    : This function handles UART5 interrupt request.
   2605          * Input           : IP Board - PD2 USART5_RX
   2606          * Output         : IP Board - PC12 USART5_TX
   2607          *******************************************************************************/
   2608          void UART5_IRQHandler(void)
   2609          {
   2610          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   2611          
   2612            	// =======================================
   2613            	// USART5 TX Process 
   2614            	// =======================================
   2615          	if (USART_GetITStatus(UART5, USART_IT_TC) != RESET)
   2616          	{ 
   2617          
   2618          		if(stUART5.TX_CNT < stUART5.TX_MAX)
   2619          		{
   2620          			Tx_Buf = stUART5.TX_BUF[stUART5.TX_CNT];
   2621          			stUART5.TX_CNT++;				
   2622          
   2623          			USART_SendData(UART5, Tx_Buf);
   2624          		}
   2625          		else
   2626          		{
   2627          			stUART5.TX_CNT = 0x00;
   2628          			stUART5.TX_MAX = 0x00;
   2629          		}
   2630          		
   2631          		USART_ClearITPendingBit(UART5, USART_IT_TC);
   2632          	}
   2633          	
   2634            	// =======================================
   2635            	// USART5RX Process
   2636            	// =======================================
   2637          
   2638          	if (USART_GetITStatus(UART5, USART_IT_RXNE) != RESET)
   2639          	{
   2640          		Rx_Buf = USART_ReceiveData(UART5);
   2641          
   2642          		Protocol_Command_Check(Rx_Buf);
   2643          	}
