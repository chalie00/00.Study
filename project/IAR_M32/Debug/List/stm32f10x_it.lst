   1440          /*******************************************************************************
   1441          * Function Name  : Main_Protocol_Data_Pasher
   1442          * Description    : This function handles USART1 interrupt request.
   1443          * Input           : Main - PA10 USART1_RX
   1444          * Output         : Main - PA9 USART1_TX
   1445          *******************************************************************************/
   1446          void Main_Protocol_Data_Pasher(u8 u_buf)
   1447          {
   1448          	u8 u_Index_Chk = 0;
   1449          
   1450          	if(stUSART1.RX_ENABLE == 0x00)
   1451          	{
   1452          		// 첫번째 바이트 체크 (Sync Byte Check)
   1453          		switch(u_buf)
   1454          		{
   1455          			case TILT_FIRST_BYTE:
   1456          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, TILT_FIRST_BYTE);
   1457          				stUSART1.RX_ENABLE = TILT_SECOND_BYTE;
   1458          				break;
   1459          
   1460          			case IP_SONY_FIRST_BYTE:
   1461          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   1462          				stUSART1.RX_ENABLE = IP_SONY_SECOND_BYTE;
   1463          				break;
   1464          
   1465          			case IP_TSM_FIRST_BYTE:
   1466          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   1467          				stUSART1.RX_ENABLE = IP_TSM_SECOND_BYTE;
   1468          				break;
   1469          
   1470          			default:
   1471          				break;
   1472          		}
   1473          	}
   1474          	else
   1475          	{
   1476          		if(stUSART1.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   1477          		else 
   1478          		{
   1479          			if(stUSART1.RX_BUF_INDEX >= 0x03)
   1480          			{
   1481          				u_Index_Chk = Rx_DATA_BYTE;
   1482          			}
   1483          			else
   1484          			{
   1485          				if(stUSART1.RX_STATUS == 0x00)
   1486          				{
   1487          					u_Index_Chk = Rx_LENGTH_BYTE;
   1488          				}
   1489          			}
   1490          		}
   1491          
   1492          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   1493          		Rx_Main_Comm_Handling(u_Index_Chk, u_buf);
   1494          	}
   1495          }
