   1311          void Rx_Main_Second_Comm_Check(u8 u_buf, u8 u_andbyte)
   1312          {
   1313          	if(u_buf == u_andbyte)
   1314          	{
   1315          		stUSART1.RX_BUF[1] =  u_buf;
   1316          		stUSART1.RX_BUF_INDEX++;
   1317          		stUSART1.RX_BUF_LENGTH = 0x01;
   1318          	}
   1319          	else
   1320          	{
   1321          		stUSART1.RX_BUF_INDEX = 0x00;					// Index Clear
   1322          		stUSART1.RX_BUF[0] = 0x00;						// Buffer Clear
   1323          		stUSART1.RX_ENABLE = 0x00;						// Command Check Byte Clear
   1324          	}	
   1325          }
