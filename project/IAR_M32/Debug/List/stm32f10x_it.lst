   1300          /*******************************************************************************
   1301          * Function Name  : Rx_Main_Comm_Handling
   1302          *******************************************************************************/
   1303          void Rx_Main_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   1304          {
   1305          	// 첫번째 바이트 체크 (Sync Byte Check)
   1306          	if(u_sync == Rx_SYNC_BYTE)	
   1307          	{
   1308          		stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1309          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1310          		stUSART1.RX_BUF_INDEX++;
   1311          		
   1312          		#if(ERROR_CHECK_BUF)
   1313          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1314          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1315          		#endif
   1316          	}
   1317          
   1318          	// 두번째 바이트 체크 (Sync & Byte Check)
   1319          	if(u_sync == Rx_SYNC_AND_BYTE)
   1320          	{
   1321          		#if(ERROR_CHECK_BUF)	
   1322          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1323          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1324          		#endif		
   1325          		
   1326          		// 두번째 명령어 체크 (Sync & Byte Check)
   1327          		if(stUSART1.RX_BUF_INDEX == 0x01)
   1328          		{
   1329          			switch(stUSART1.RX_ENABLE)
   1330          			{
   1331          				case TILT_SECOND_BYTE:
   1332          					Rx_Main_Second_Comm_Check(u_buf, TILT_SECOND_BYTE);
   1333          					break;
   1334          
   1335          				case IP_SONY_SECOND_BYTE:
   1336          					Rx_Main_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   1337          					break;
   1338          					
   1339          				case IP_TSM_SECOND_BYTE:
   1340          					Rx_Main_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   1341          					break;
   1342          
   1343          				default:
   1344          					break;
   1345          			}
   1346          		}	
   1347          	}
   1348          
   1349          	// 세번째 바이트 체크 (Data Length)
   1350          	if(u_sync == Rx_LENGTH_BYTE)
   1351          	{
   1352          		#if(ERROR_CHECK_BUF)	
   1353          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1354          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1355          		#endif
   1356          		
   1357          		if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1358          		{
   1359          			// PELCO-D Command 예외처리
   1360          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1361          			{
   1362          				stUSART1.RX_BUF_LENGTH = 0x02;
   1363          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1364          				
   1365          				stUSART1.RX_BUF[2] =  u_buf;
   1366          				
   1367          				stUSART1.RX_BUF_INDEX++;
   1368          			}					
   1369          		}
   1370          		else
   1371          		{
   1372          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   1373          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1374          			{
   1375          				stUSART1.RX_BUF_LENGTH = u_buf;
   1376          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1377          				
   1378          				stUSART1.RX_BUF[2] =  u_buf;
   1379          				
   1380          				stUSART1.RX_BUF_INDEX++;
   1381          			}
   1382          		}
   1383          	}	
   1384          
   1385          	// 데이터 누적
   1386          	if(u_sync == Rx_DATA_BYTE)
   1387          	{
   1388          		#if(ERROR_CHECK_BUF)
   1389          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1390          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1391          		#endif		
   1392          		
   1393          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1394          
   1395          		stUSART1.RX_BUF_INDEX++;	
   1396          		
   1397          		if(stUSART1.RX_BUF_INDEX == stUSART1.RX_BUF_MAX)
   1398          		{
   1399          			// PELCO-D Command 예외처리
   1400          			if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1401          			{
   1402          				Rx_Pelco_Data_Check();
   1403          			}
   1404          			else
   1405          			{
   1406          				if((stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   1407          				{
   1408          					if(stSYS.START == ACTIVE)
   1409          					{
   1410          						stUSART1.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1411          						stUSART1.RX_PARITY_CNT++;
   1412          						
   1413          						// DEBUG 데이터 체크 ======================================================/
   1414          						//stERROR.PARITY_CNT_CHECK++;
   1415          						//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   1416          						//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
   1417          						// DEBUG 데이터 체크 ===================================================END/
   1418          						
   1419          						//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   1420          					}
   1421          					else
   1422          					{
   1423          						Boot_Status_Check_Parser();
   1424          					}
   1425          
   1426          					stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1427          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   1428          				}
   1429          				else
   1430          				{
   1431          					stUSART1.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1432          					stUSART1.RX_ERROR_CNT++;
   1433          				}
   1434          			}
   1435          			
   1436          			stUSART1.RX_BUF_INDEX = 0x00;
   1437          			stUSART1.RX_BUF_LENGTH = 0x00;
   1438          			
   1439          			stUSART1.RX_ENABLE = 0x00;
   1440          		}
   1441          	}		
   1442          }
