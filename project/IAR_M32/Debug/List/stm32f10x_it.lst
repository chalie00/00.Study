   2488          			{
   2489          				if((stUART5.RX_BUF[stUART5.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   2490          				{
   2491          					stUART5.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터)
   2492          					stUART5.RX_PARITY_CNT++;
   2493          
   2494          					// DEBUG 데이터 체크 ======================================================/
   2495          					//stERROR.PARITY_CNT_CHECK++;
   2496          					//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   2497          					//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
