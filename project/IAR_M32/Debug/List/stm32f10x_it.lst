   1546          /*******************************************************************************
   1547          * Function Name  : USART1_IRQHandler
   1548          * Description    : This function handles USART1 interrupt request.
   1549          * Input           : Main - PA10 USART1_RX
   1550          * Output         : Main - PA9 USART1_TX
   1551          *******************************************************************************/
   1552          void USART1_IRQHandler(void)
   1553          {
   1554          	u8 Rx_Buf = 0x00;
   1555          	
   1556            	// =======================================
   1557            	// USART1 TX Process : TILT -> MAIN Board
   1558            	// =======================================
   1559          	if (USART_GetITStatus(USART1, USART_IT_TC) != RESET)
   1560          	{
   1561          		if (stUSART1.TX_CNT < stUSART1.TX_MAX) 
   1562          		{
   1563          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1564          		}
   1565          		else 
   1566          		{
   1567          			stUSART1.TX_MAX = 0;
   1568          			stUSART1.TX_CNT = 0;
   1569          		}
   1570          		
   1571          		USART_ClearITPendingBit(USART1, USART_IT_TC);
   1572          	}
   1573          
   1574            	// =======================================
   1575            	// USART1 RX Process : TILT <- MAIN Board
   1576            	// =======================================
   1577          	if (USART_GetITStatus(USART1, USART_IT_RXNE) != RESET)
   1578          	{
   1579          		Rx_Buf = USART_ReceiveData(USART1);
   1580          
   1581          		Main_Protocol_Data_Pasher(Rx_Buf);
   1582          		
   1583          		//Sony_Data_Pasher(Rx_Buf);
   1584          
   1585          		//if(stUSART_ERROR.RX_CNT == UART4_RX_BUF_MAX) stUSART_ERROR.RX_CNT = 0;
   1586          
   1587          		//stUSART_ERROR.RX_BUF[stUSART_ERROR.RX_CNT++] = Rx_Buf;
   1588          	}
   1589          }
