   1528          /*******************************************************************************
   1529          * Function Name  : USART1_IRQHandler
   1530          * Description    : This function handles USART1 interrupt request.
   1531          * Input           : Main - PA10 USART1_RX
   1532          * Output         : Main - PA9 USART1_TX
   1533          *******************************************************************************/
   1534          void USART1_IRQHandler(void)
   1535          {
   1536          	u8 Rx_Buf = 0x00;
   1537          	
   1538            	// =======================================
   1539            	// USART1 TX Process : TILT -> MAIN Board
   1540            	// =======================================
   1541          	if (USART_GetITStatus(USART1, USART_IT_TC) != RESET)
   1542          	{
   1543          		if (stUSART1.TX_CNT < stUSART1.TX_MAX) 
   1544          		{
   1545          			USART_SendData(USART1, stUSART1.TX_BUF[stUSART1.TX_CNT++]);
   1546          		}
   1547          		else 
   1548          		{
   1549          			stUSART1.TX_MAX = 0;
   1550          			stUSART1.TX_CNT = 0;
   1551          		}
   1552          		
   1553          		USART_ClearITPendingBit(USART1, USART_IT_TC);
   1554          	}
   1555          
   1556            	// =======================================
   1557            	// USART1 RX Process : TILT <- MAIN Board
   1558            	// =======================================
   1559          	if (USART_GetITStatus(USART1, USART_IT_RXNE) != RESET)
   1560          	{
   1561          		Rx_Buf = USART_ReceiveData(USART1);
   1562          
   1563          		Main_Protocol_Data_Pasher(Rx_Buf);
   1564          		
   1565          		//Sony_Data_Pasher(Rx_Buf);
   1566          
   1567          		//if(stUSART_ERROR.RX_CNT == UART4_RX_BUF_MAX) stUSART_ERROR.RX_CNT = 0;
   1568          
   1569          		//stUSART_ERROR.RX_BUF[stUSART_ERROR.RX_CNT++] = Rx_Buf;
   1570          	}
   1571          }
