   1097          		}
   1098          		counterNine++;
   1099          		if(counterNine>= 6) {
   1100          			GPIOE-> ODR ^= GPIO_Pin_4;
   1101          			counterNine = 0;
   1102          		}
   1103          		
   1104          	}	
   1105          
   1106          }//End Of The TIM4_IRQ_Handler
