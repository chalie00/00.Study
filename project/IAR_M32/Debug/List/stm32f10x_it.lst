   2106          /*******************************************************************************
   2107          * Function Name  : Rx_Sony_Inquiry_Command
   2108          *******************************************************************************/
   2109          void SONY_Data_Pasher(u8 u_buf)
   2110          {
   2111          	u16 u_CZoomData = 0x0000, u_CFocusData = 0x0000;
   2112          
   2113          	// Data Save
   2114          	if(stSONY.RX_CNT >= 0x02)
   2115          	{
   2116          		stSONY.RX_BUF[stSONY.RX_CNT] = u_buf;
   2117          		stSONY.RX_CNT++;
   2118          	}
   2119          
   2120          	if(stSONY.RX_CNT == 0x04)
   2121          	{
   2122          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[3] == SONY_BYTE_END))
   2123          		{
   2124          			switch(stUART4.RX_REQUEST_CMD)
   2125          			{
   2126          				case SONY_POWER_INQ:
   2127          
   2128          					stSONY.MODULE_STATUS = stSONY.RX_BUF[2];
   2129          
   2130          					switch(stSONY.MODULE_STATUS)
   2131          					{
   2132          						case 0x02:
   2133          							stSONY.MODULE_ENABLE = 1;
   2134          							break;
   2135          
   2136          						case 0x03:
   2137          							stSONY.MODULE_ENABLE = 0;
   2138          							break;
   2139          
   2140          						default:
   2141          							break;
   2142          					}
   2143          
   2144          					stINFO.MODULE_TYPE = SONY_MODULE;
   2145          					
   2146          					break;
   2147          
   2148          				default:
   2149          					break;
   2150          			}
   2151          
   2152          			SONY_Data_Clear();
   2153          			
   2154          			stSONY.RX_CNT = 0;
   2155          			stSONY.RX_END_FLAG = 0x01;	
   2156          		}
   2157          	}
   2158          
   2159          	// Data Compare Check
   2160          	if(stSONY.RX_CNT == 0x07)
   2161          	{
   2162          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[6] == SONY_BYTE_END))
   2163          		{
   2164          			switch(stUART4.RX_REQUEST_CMD)
   2165          			{
   2166          				case SONY_CALL_ZOOM_POSITION:
   2167          					
   2168          					u_CZoomData = stSONY.RX_BUF[2];
   2169          					stSONY.ZOOM_CNT = u_CZoomData << 12;
   2170          					u_CZoomData = stSONY.RX_BUF[3];
   2171          					stSONY.ZOOM_CNT |= u_CZoomData << 8;
   2172          					u_CZoomData = stSONY.RX_BUF[4];
   2173          					stSONY.ZOOM_CNT |= u_CZoomData << 4;
   2174          					u_CZoomData = stSONY.RX_BUF[5];
   2175          					stSONY.ZOOM_CNT |= u_CZoomData;
   2176          
   2177          					if(stSONY.RX_BUF[6] == 0x00) 
   2178          					{
   2179          						stUART4.RX_REQUEST_ERROR = 0x01;
   2180          					}
   2181          					else 
   2182          					{
   2183          						stUART4.RX_REQUEST_CMD = 0x00;
   2184          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_ZOOM_POSITION;
   2185          					}
   2186          					
   2187          					break;
   2188          
   2189          				case SONY_CALL_FOCUS_POSITION:
   2190          					
   2191          					u_CFocusData = stSONY.RX_BUF[2];
   2192          					stSONY.FOCUS_CNT = u_CFocusData << 12;
   2193          					u_CFocusData = stSONY.RX_BUF[3];
   2194          					stSONY.FOCUS_CNT |= u_CFocusData << 8;
   2195          					u_CFocusData = stSONY.RX_BUF[4];
   2196          					stSONY.FOCUS_CNT |= u_CFocusData << 4;
   2197          					u_CFocusData = stSONY.RX_BUF[5];
   2198          					stSONY.FOCUS_CNT |= u_CFocusData;
   2199          
   2200          					if(stSONY.RX_BUF[6] == 0x00) 
   2201          					{
   2202          						stUART4.RX_REQUEST_ERROR = 0x01;
   2203          					}
   2204          					else 
   2205          					{
   2206          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_FOCUS_POSITION;
   2207          						stUART4.RX_REQUEST_CMD = 0x00;
   2208          					}
   2209          
   2210          					break;
   2211          
   2212          				default:
   2213          
   2214          					stUART4.RX_ENABLE = 0x00;
   2215          					if(stUART4.RX_REQUEST_ERROR != 0x00) stUART4.RX_REQUEST_ERROR = 0x00;
   2216          					
   2217          					break;
   2218          			}
   2219          
   2220          			SONY_Data_Clear();
   2221          			
   2222          			stSONY.RX_CNT = 0;
   2223          			stSONY.RX_END_FLAG = 0x01;
   2224          
   2225          		}
   2226          	}
   2227          	
   2228          	if(stSONY.RX_END_FLAG == 0x00)
   2229          	{
   2230          		// Second Data Check
   2231          		if((u_buf == SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2232          		{
   2233          			stSONY.RX_BUF[1] = SONY_BYTE_2;
   2234          			stSONY.RX_CNT++;
   2235          		}
   2236          		else if((u_buf != SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2237          		{
   2238          			stSONY.RX_BUF[0] = 0x00;
   2239          			stSONY.RX_CNT = 0;
   2240          		}
   2241          
   2242          		// First Data Check
   2243          		if((u_buf == SONY_BYTE_1) && (stSONY.RX_CNT == 0))
   2244          		{
   2245          			stSONY.RX_BUF[0] = SONY_BYTE_1;
   2246          			stSONY.RX_CNT++;
   2247          		}
   2248          	}
   2249          	else
   2250          	{
   2251          		stSONY.RX_END_FLAG = 0x00;
   2252          	}
   2253          
   2254          	if(stSONY.RX_CNT >= SONY_RX_BUF_MAX)
   2255          	{
   2256          		stSONY.RX_CNT = 0;
   2257          		SONY_Data_Clear();
   2258          		
   2259          	}
   2260          }
