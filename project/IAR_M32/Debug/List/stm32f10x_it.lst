   2262          // ============================================================================
   2263          // USART3 : UART4 Data Pasher (TSM)
   2264          // ============================================================================
   2265          void UART4_Data_Pasher(u8 u_Buf, u8 u_Index)
   2266          {
   2267          	if((u_Index == INDEX_NON) || (u_Index == INDEX_THERMAL))
   2268          	{
   2269          		TSM_Data_Pasher(u_Buf);
   2270          	}
   2271          
   2272          	if((u_Index == INDEX_NON) || (u_Index == INDEX_COLOR))
   2273          	{
   2274          		SONY_Data_Pasher(u_Buf);
   2275          	}
   2276          }
