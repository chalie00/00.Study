   1491          /*******************************************************************************
   1492          * Function Name  : Main_Protocol_Data_Pasher
   1493          * Description    : This function handles USART1 interrupt request.
   1494          * Input           : Main - PA10 USART1_RX
   1495          * Output         : Main - PA9 USART1_TX
   1496          *******************************************************************************/
   1497          void Main_Protocol_Data_Pasher(u8 u_buf)
   1498          {
   1499          	u8 u_Index_Chk = 0;
   1500          
   1501          	if(stUSART1.RX_ENABLE == 0x00)
   1502          	{
   1503          		// 첫번째 바이트 체크 (Sync Byte Check)
   1504          		switch(u_buf)
   1505          		{
   1506          			case TILT_FIRST_BYTE:
   1507          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, TILT_FIRST_BYTE);
   1508          				stUSART1.RX_ENABLE = TILT_SECOND_BYTE;
   1509          				break;
   1510          
   1511          			case IP_SONY_FIRST_BYTE:
   1512          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_SONY_FIRST_BYTE);
   1513          				stUSART1.RX_ENABLE = IP_SONY_SECOND_BYTE;
   1514          				break;
   1515          
   1516          			case IP_TSM_FIRST_BYTE:
   1517          				Rx_Main_Comm_Handling(Rx_SYNC_BYTE, IP_TSM_FIRST_BYTE);
   1518          				stUSART1.RX_ENABLE = IP_TSM_SECOND_BYTE;
   1519          				break;
   1520          
   1521          			default:
   1522          				break;
   1523          		}
   1524          	}
   1525          	else
   1526          	{
   1527          		if(stUSART1.RX_BUF_LENGTH == 0x00) u_Index_Chk = Rx_SYNC_AND_BYTE;
   1528          		else 
   1529          		{
   1530          			if(stUSART1.RX_BUF_INDEX >= 0x03)
   1531          			{
   1532          				u_Index_Chk = Rx_DATA_BYTE;
   1533          			}
   1534          			else
   1535          			{
   1536          				if(stUSART1.RX_STATUS == 0x00)
   1537          				{
   1538          					u_Index_Chk = Rx_LENGTH_BYTE;
   1539          				}
   1540          			}
   1541          		}
   1542          
   1543          		// 두번째, 세번째 바이트 체크 (Rx_SYNC_AND_BYTE : Sync & Byte Check, Rx_LENGTH_BYTE : Length Check)
   1544          		Rx_Main_Comm_Handling(u_Index_Chk, u_buf);
   1545          	}
   1546          }
