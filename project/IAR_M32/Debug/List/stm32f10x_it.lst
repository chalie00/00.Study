   1986          							if(stUART4.RX_BUF[1] == TCM_TOTAL_ZOOM_POSITION_INQ)
   1987          							{
   1988          								u_Data_P1 = stUART4.RX_BUF[2];
   1989          								u_Data_P2 = stUART4.RX_BUF[3];
