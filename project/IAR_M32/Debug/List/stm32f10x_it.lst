   1687          /*******************************************************************************
   1688          * Function Name  : USART2_IRQHandler
   1689          * Description    : This function handles USART2 interrupt request.
   1690          * Input           : MDIN - PA3 USART2_RX
   1691          * Output         : MDIN - PA2 USART2_TX
   1692          *******************************************************************************/
   1693          void USART2_IRQHandler(void)
   1694          {
   1695          	u8 Rx_Buf = 0x00, Tx_Buf = 0x00;	
   1696          
   1697            	// =======================================
   1698            	// USART2 TX Process 
   1699            	// =======================================
   1700          	if (USART_GetITStatus(USART2, USART_IT_TC) != RESET)
   1701          	{ 
   1702          		USART_SendData(USART2, Tx_Buf);
   1703          		if(Tx_Buf != 0x00) {
   1704          					//RED
   1705          		GPIOE-> ODR ^= GPIO_Pin_3;
   1706          		USART_ClearITPendingBit(USART2, USART_IT_TC);
   1707          		}
   1708          	}
   1709          	
   1710            	// =======================================
   1711            	// USART2 RX Process
   1712            	// =======================================
   1713          	if (USART_GetITStatus(USART3, USART_IT_RXNE) != RESET)
   1714          	{
   1715          		Rx_Buf = USART_ReceiveData(USART2);
   1716          		if(Rx_Buf != 0x00) {
   1717          			//Green
   1718          			GPIOE -> ODR ^= GPIO_Pin_4;
   1719          			Rx_Buf = 0x00;
   1720          			
   1721          		}
   1722          		
   1723          	}  
   1724          }
