   2145          /*******************************************************************************
   2146          * Function Name  : Rx_Sony_Inquiry_Command
   2147          *******************************************************************************/
   2148          void SONY_Data_Pasher(u8 u_buf)
   2149          {
   2150          	u16 u_CZoomData = 0x0000, u_CFocusData = 0x0000;
   2151          
   2152          	// Data Save
   2153          	if(stSONY.RX_CNT >= 0x02)
   2154          	{
   2155          		stSONY.RX_BUF[stSONY.RX_CNT] = u_buf;
   2156          		stSONY.RX_CNT++;
   2157          	}
   2158          
   2159          	if(stSONY.RX_CNT == 0x04)
   2160          	{
   2161          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[3] == SONY_BYTE_END))
   2162          		{
   2163          			switch(stUART4.RX_REQUEST_CMD)
   2164          			{
   2165          				case SONY_POWER_INQ:
   2166          
   2167          					stSONY.MODULE_STATUS = stSONY.RX_BUF[2];
   2168          
   2169          					switch(stSONY.MODULE_STATUS)
   2170          					{
   2171          						case 0x02:
   2172          							stSONY.MODULE_ENABLE = 1;
   2173          							break;
   2174          
   2175          						case 0x03:
   2176          							stSONY.MODULE_ENABLE = 0;
   2177          							break;
   2178          
   2179          						default:
   2180          							break;
   2181          					}
   2182          
   2183          					stINFO.MODULE_TYPE = SONY_MODULE;
   2184          					
   2185          					break;
   2186          
   2187          				default:
   2188          					break;
   2189          			}
   2190          
   2191          			SONY_Data_Clear();
   2192          			
   2193          			stSONY.RX_CNT = 0;
   2194          			stSONY.RX_END_FLAG = 0x01;	
   2195          		}
   2196          	}
   2197          
   2198          	// Data Compare Check
   2199          	if(stSONY.RX_CNT == 0x07)
   2200          	{
   2201          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[6] == SONY_BYTE_END))
   2202          		{
   2203          			switch(stUART4.RX_REQUEST_CMD)
   2204          			{
   2205          				case SONY_CALL_ZOOM_POSITION:
   2206          					
   2207          					u_CZoomData = stSONY.RX_BUF[2];
   2208          					stSONY.ZOOM_CNT = u_CZoomData << 12;
   2209          					u_CZoomData = stSONY.RX_BUF[3];
   2210          					stSONY.ZOOM_CNT |= u_CZoomData << 8;
   2211          					u_CZoomData = stSONY.RX_BUF[4];
   2212          					stSONY.ZOOM_CNT |= u_CZoomData << 4;
   2213          					u_CZoomData = stSONY.RX_BUF[5];
   2214          					stSONY.ZOOM_CNT |= u_CZoomData;
   2215          
   2216          					if(stSONY.RX_BUF[6] == 0x00) 
   2217          					{
   2218          						stUART4.RX_REQUEST_ERROR = 0x01;
   2219          					}
   2220          					else 
   2221          					{
   2222          						stUART4.RX_REQUEST_CMD = 0x00;
   2223          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_ZOOM_POSITION;
   2224          					}
   2225          					
   2226          					break;
   2227          
   2228          				case SONY_CALL_FOCUS_POSITION:
   2229          					
   2230          					u_CFocusData = stSONY.RX_BUF[2];
   2231          					stSONY.FOCUS_CNT = u_CFocusData << 12;
   2232          					u_CFocusData = stSONY.RX_BUF[3];
   2233          					stSONY.FOCUS_CNT |= u_CFocusData << 8;
   2234          					u_CFocusData = stSONY.RX_BUF[4];
   2235          					stSONY.FOCUS_CNT |= u_CFocusData << 4;
   2236          					u_CFocusData = stSONY.RX_BUF[5];
   2237          					stSONY.FOCUS_CNT |= u_CFocusData;
   2238          
   2239          					if(stSONY.RX_BUF[6] == 0x00) 
   2240          					{
   2241          						stUART4.RX_REQUEST_ERROR = 0x01;
   2242          					}
   2243          					else 
   2244          					{
   2245          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_FOCUS_POSITION;
   2246          						stUART4.RX_REQUEST_CMD = 0x00;
   2247          					}
   2248          
   2249          					break;
   2250          
   2251          				default:
   2252          
   2253          					stUART4.RX_ENABLE = 0x00;
   2254          					if(stUART4.RX_REQUEST_ERROR != 0x00) stUART4.RX_REQUEST_ERROR = 0x00;
   2255          					
   2256          					break;
   2257          			}
   2258          
   2259          			SONY_Data_Clear();
   2260          			
   2261          			stSONY.RX_CNT = 0;
   2262          			stSONY.RX_END_FLAG = 0x01;
   2263          
   2264          		}
   2265          	}
   2266          	
   2267          	if(stSONY.RX_END_FLAG == 0x00)
   2268          	{
   2269          		// Second Data Check
   2270          		if((u_buf == SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2271          		{
   2272          			stSONY.RX_BUF[1] = SONY_BYTE_2;
   2273          			stSONY.RX_CNT++;
   2274          		}
   2275          		else if((u_buf != SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2276          		{
   2277          			stSONY.RX_BUF[0] = 0x00;
   2278          			stSONY.RX_CNT = 0;
   2279          		}
   2280          
   2281          		// First Data Check
   2282          		if((u_buf == SONY_BYTE_1) && (stSONY.RX_CNT == 0))
   2283          		{
   2284          			stSONY.RX_BUF[0] = SONY_BYTE_1;
   2285          			stSONY.RX_CNT++;
   2286          		}
   2287          	}
   2288          	else
   2289          	{
