   2440          		if(stUART5.RX_ENABLE == IP_PELCO_CHK_BYTE)
   2441          		{
   2442          			// PELCO-D Command 예외처리
   2443          			if(stUART5.RX_BUF_INDEX == 0x02)
   2444          			{
   2445          				stUART5.RX_BUF_LENGTH = 0x02;
   2446          				stUART5.RX_BUF_MAX = stUART5.RX_BUF_LENGTH + 5;
   2447          				
   2448          				stUART5.RX_BUF[2] =  u_buf;
   2449          				
   2450          				stUART5.RX_BUF_INDEX++;
   2451          			}					
   2452          		}
   2453          		else
   2454          		{
   2455          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   2456          			if(stUART5.RX_BUF_INDEX == 0x02)
   2457          			{
   2458          				stUART5.RX_BUF_LENGTH = u_buf;
   2459          				stUART5.RX_BUF_MAX = stUART5.RX_BUF_LENGTH + 5;
   2460          				
   2461          				stUART5.RX_BUF[2] =  u_buf;
   2462          				
   2463          				stUART5.RX_BUF_INDEX++;
   2464          			}
   2465          		}
   2466          	}	
   2467          
   2468          	// 데이터 누적
   2469          	if(u_sync == Rx_DATA_BYTE)
   2470          	{
   2471          		#if(ERROR_CHECK_BUF)
   2472          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   2473          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   2474          		#endif		
   2475          		
   2476          		stUART5.RX_BUF[stUART5.RX_BUF_INDEX] = u_buf;
   2477          
   2478          		stUART5.RX_BUF_INDEX++;	
   2479          		
   2480          		if(stUART5.RX_BUF_INDEX == stUART5.RX_BUF_MAX)
   2481          		{
   2482          			// PELCO-D Command 예외처리
   2483          			if(stUART5.RX_ENABLE == IP_PELCO_CHK_BYTE)
