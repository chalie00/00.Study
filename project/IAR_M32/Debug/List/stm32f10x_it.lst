   1262          * Function Name  : Rx_Second_Comm_Check
   1263          *******************************************************************************/
   1264          void Rx_Main_Second_Comm_Check(u8 u_buf, u8 u_andbyte)
   1265          {
   1266          	if(u_buf == u_andbyte)
   1267          	{
   1268          		stUSART1.RX_BUF[1] =  u_buf;
   1269          		stUSART1.RX_BUF_INDEX++;
   1270          		stUSART1.RX_BUF_LENGTH = 0x01;
   1271          	}
   1272          	else
   1273          	{
   1274          		stUSART1.RX_BUF_INDEX = 0x00;					// Index Clear
   1275          		stUSART1.RX_BUF[0] = 0x00;						// Buffer Clear
   1276          		stUSART1.RX_ENABLE = 0x00;						// Command Check Byte Clear
   1277          	}	
   1278          }
