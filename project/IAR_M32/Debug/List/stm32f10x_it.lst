    945          /*******************************************************************************
    946          * Function Name  : SYSTEM_Data_Handling
    947          *******************************************************************************/
    948          void SYSTEM_Data_Buffer(void)
    949          {
    950          	u8 i = 0x00, u_buf = 0x00, u_length = 0x00, u_max = 0x00;
    951          
    952          	if(stUSART1.RX_PARITY == 0x01)
    953          	{
    954          		u_buf = stUSART1.RX_BUF[0];
    955          
    956          		switch(u_buf)
    957          		{
    958          /*
    959          			case TILT_FIRST_BYTE:
    960          
    961          				u_length = stUSART1.RX_BUF[2];
    962          
    963          				u_max = u_length + 5;
    964          
    965          				stCOOR.RX_BUF[stCOOR.RX_INDEX][0] = stCOOR.RX_INDEX + 1;
    966          
    967          				for(i=0; i<u_max; i++)
    968          				{
    969          					stCOOR.RX_BUF[stCOOR.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    970          					stUSART1.RX_BUF[i] = 0x00;
    971          				}
    972          
    973          				stCOOR.RX_INDEX++;
    974          				
    975          				break;
    976          */				
    977          			case TILT_FIRST_BYTE:
    978          			case IP_SONY_FIRST_BYTE:
    979          			case IP_TSM_FIRST_BYTE:
    980          			case IP_DRS_FIRST_BYTE:
    981          			case IP_PTZ_FIRST_BYTE:
    982          			case IP_DATA_FIRST_BYTE:
    983          			case IP_OPTION_FIRST_BYTE:
    984          
    985          				u_length = stUSART1.RX_BUF[2];
    986          
    987          				u_max = u_length + 5;
    988          
    989          				stSYS.RX_BUF[stSYS.RX_INDEX][0] = stSYS.RX_INDEX + 1;
    990          
    991          				for(i=0; i<u_max; i++)
    992          				{
    993          					stSYS.RX_BUF[stSYS.RX_INDEX][i+1] =  stUSART1.RX_BUF[i];
    994          					stUSART1.RX_BUF[i] = 0x00;
    995          				}
    996          
    997          				stSYS.RX_INDEX++;
    998          
    999          				break;
   1000          
   1001          			default:
   1002          				break;
   1003          		}
   1004          
   1005          		stUSART1.RX_PARITY = 0x00;
   1006          
   1007          	}
   1008          }
