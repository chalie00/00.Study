   1309          /*******************************************************************************
   1310          * Function Name  : Rx_Main_Comm_Handling
   1311          *******************************************************************************/
   1312          void Rx_Main_Comm_Handling(u8 u_sync, u8 u_buf)	// u_port : Uart Port, u_sync : Sync Byte Check (0 : First Byte, 1 : Second Byte)
   1313          {
   1314          	// 첫번째 바이트 체크 (Sync Byte Check)
   1315          	if(u_sync == Rx_SYNC_BYTE)	
   1316          	{
   1317          		stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1318          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1319          		stUSART1.RX_BUF_INDEX++;
   1320          		
   1321          		#if(ERROR_CHECK_BUF)
   1322          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1323          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1324          		#endif
   1325          	}
   1326          
   1327          	// 두번째 바이트 체크 (Sync & Byte Check)
   1328          	if(u_sync == Rx_SYNC_AND_BYTE)
   1329          	{
   1330          		#if(ERROR_CHECK_BUF)	
   1331          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1332          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1333          		#endif		
   1334          		
   1335          		// 두번째 명령어 체크 (Sync & Byte Check)
   1336          		if(stUSART1.RX_BUF_INDEX == 0x01)
   1337          		{
   1338          			switch(stUSART1.RX_ENABLE)
   1339          			{
   1340          				case TILT_SECOND_BYTE:
   1341          					Rx_Main_Second_Comm_Check(u_buf, TILT_SECOND_BYTE);
   1342          					break;
   1343          
   1344          				case IP_SONY_SECOND_BYTE:
   1345          					Rx_Main_Second_Comm_Check(u_buf, IP_SONY_SECOND_BYTE);
   1346          					break;
   1347          					
   1348          				case IP_TSM_SECOND_BYTE:
   1349          					Rx_Main_Second_Comm_Check(u_buf, IP_TSM_SECOND_BYTE);
   1350          					break;
   1351          
   1352          				default:
   1353          					break;
   1354          			}
   1355          		}	
   1356          	}
   1357          
   1358          	// 세번째 바이트 체크 (Data Length)
   1359          	if(u_sync == Rx_LENGTH_BYTE)
   1360          	{
   1361          		#if(ERROR_CHECK_BUF)	
   1362          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1363          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1364          		#endif
   1365          		
   1366          		if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1367          		{
   1368          			// PELCO-D Command 예외처리
   1369          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1370          			{
   1371          				stUSART1.RX_BUF_LENGTH = 0x02;
   1372          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1373          				
   1374          				stUSART1.RX_BUF[2] =  u_buf;
   1375          				
   1376          				stUSART1.RX_BUF_INDEX++;
   1377          			}					
   1378          		}
   1379          		else
   1380          		{
   1381          			// 세번째 데이터 길이 체크 (Sync & Byte Check)
   1382          			if(stUSART1.RX_BUF_INDEX == 0x02)
   1383          			{
   1384          				stUSART1.RX_BUF_LENGTH = u_buf;
   1385          				stUSART1.RX_BUF_MAX = stUSART1.RX_BUF_LENGTH + 5;
   1386          				
   1387          				stUSART1.RX_BUF[2] =  u_buf;
   1388          				
   1389          				stUSART1.RX_BUF_INDEX++;
   1390          			}
   1391          		}
   1392          	}	
   1393          
   1394          	// 데이터 누적
   1395          	if(u_sync == Rx_DATA_BYTE)
   1396          	{
   1397          		#if(ERROR_CHECK_BUF)
   1398          		stERROR.RX_BUF[stERROR.RX_CNT++] = u_buf;
   1399          		if(stERROR.RX_CNT >= ERROR_DATA_MAX) stERROR.RX_CNT = 0;
   1400          		#endif		
   1401          		
   1402          		stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = u_buf;
   1403          
   1404          		stUSART1.RX_BUF_INDEX++;	
   1405          		
   1406          		if(stUSART1.RX_BUF_INDEX == stUSART1.RX_BUF_MAX)
   1407          		{
   1408          			// PELCO-D Command 예외처리
   1409          			if(stUSART1.RX_ENABLE == IP_PELCO_CHK_BYTE)
   1410          			{
   1411          				Rx_Pelco_Data_Check();
   1412          			}
   1413          			else
   1414          			{
   1415          				if((stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX - 1]&0x0F) == 0x0F)		// End Byte Check
   1416          				{
   1417          					if(stSYS.START == ACTIVE)
   1418          					{
   1419          						stUSART1.RX_PARITY = 0x01;	// 정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1420          						stUSART1.RX_PARITY_CNT++;
   1421          						
   1422          						// DEBUG 데이터 체크 ======================================================/
   1423          						//stERROR.PARITY_CNT_CHECK++;
   1424          						//if(stERROR.PARITY_CNT_CHECK == 0x01) stERROR.TIMER_CHECK_FLAG = 0x01;
   1425          						//if(stERROR.PARITY_CNT_CHECK == 0x02) stERROR.TIMER_CHECK_FLAG = 0x00;
   1426          						// DEBUG 데이터 체크 ===================================================END/
   1427          						
   1428          						//if(stUSART1.RX_PRIORITY == 0x00) stUSART1.RX_PRIORITY = 0x01;
   1429          					}
   1430          					else
   1431          					{
   1432          						Boot_Status_Check_Parser();
   1433          					}
   1434          
   1435          					stUSART1.RX_BUF_INDEX = 0x00;	// Index Init
   1436          					//stUSART1.RX_BUF[stUSART1.RX_BUF_INDEX] = stUSART1.RX_PRIORITY++;
   1437          				}
   1438          				else
   1439          				{
   1440          					stUSART1.RX_PARITY = 0xFF;	// 비정상 데이터 수신 (0x00: Nondata(수신대기), 0x01: 정상데이터, 0xFF, 비정상 데이터, 0xFE = Boot Check)
   1441          					stUSART1.RX_ERROR_CNT++;
   1442          				}
   1443          			}
   1444          			
   1445          			stUSART1.RX_BUF_INDEX = 0x00;
   1446          			stUSART1.RX_BUF_LENGTH = 0x00;
   1447          			
   1448          			stUSART1.RX_ENABLE = 0x00;
   1449          		}
   1450          	}		
   1451          }
