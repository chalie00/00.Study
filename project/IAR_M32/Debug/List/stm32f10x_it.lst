   2085          /*******************************************************************************
   2086          * Function Name  : Rx_Sony_Inquiry_Command
   2087          *******************************************************************************/
   2088          void SONY_Data_Pasher(u8 u_buf)
   2089          {
   2090          	u16 u_CZoomData = 0x0000, u_CFocusData = 0x0000;
   2091          
   2092          	// Data Save
   2093          	if(stSONY.RX_CNT >= 0x02)
   2094          	{
   2095          		stSONY.RX_BUF[stSONY.RX_CNT] = u_buf;
   2096          		stSONY.RX_CNT++;
   2097          	}
   2098          
   2099          	if(stSONY.RX_CNT == 0x04)
   2100          	{
   2101          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[3] == SONY_BYTE_END))
   2102          		{
   2103          			switch(stUART4.RX_REQUEST_CMD)
   2104          			{
   2105          				case SONY_POWER_INQ:
   2106          
   2107          					stSONY.MODULE_STATUS = stSONY.RX_BUF[2];
   2108          
   2109          					switch(stSONY.MODULE_STATUS)
   2110          					{
   2111          						case 0x02:
   2112          							stSONY.MODULE_ENABLE = 1;
   2113          							break;
   2114          
   2115          						case 0x03:
   2116          							stSONY.MODULE_ENABLE = 0;
   2117          							break;
   2118          
   2119          						default:
   2120          							break;
   2121          					}
   2122          
   2123          					stINFO.MODULE_TYPE = SONY_MODULE;
   2124          					
   2125          					break;
   2126          
   2127          				default:
   2128          					break;
   2129          			}
   2130          
   2131          			SONY_Data_Clear();
   2132          			
   2133          			stSONY.RX_CNT = 0;
   2134          			stSONY.RX_END_FLAG = 0x01;	
   2135          		}
   2136          	}
   2137          
   2138          	// Data Compare Check
   2139          	if(stSONY.RX_CNT == 0x07)
   2140          	{
   2141          		if((stSONY.RX_BUF[0] == SONY_BYTE_1) && (stSONY.RX_BUF[1] == SONY_BYTE_2) && (stSONY.RX_BUF[6] == SONY_BYTE_END))
   2142          		{
   2143          			switch(stUART4.RX_REQUEST_CMD)
   2144          			{
   2145          				case SONY_CALL_ZOOM_POSITION:
   2146          					
   2147          					u_CZoomData = stSONY.RX_BUF[2];
   2148          					stSONY.ZOOM_CNT = u_CZoomData << 12;
   2149          					u_CZoomData = stSONY.RX_BUF[3];
   2150          					stSONY.ZOOM_CNT |= u_CZoomData << 8;
   2151          					u_CZoomData = stSONY.RX_BUF[4];
   2152          					stSONY.ZOOM_CNT |= u_CZoomData << 4;
   2153          					u_CZoomData = stSONY.RX_BUF[5];
   2154          					stSONY.ZOOM_CNT |= u_CZoomData;
   2155          
   2156          					if(stSONY.RX_BUF[6] == 0x00) 
   2157          					{
   2158          						stUART4.RX_REQUEST_ERROR = 0x01;
   2159          					}
   2160          					else 
   2161          					{
   2162          						stUART4.RX_REQUEST_CMD = 0x00;
   2163          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_ZOOM_POSITION;
   2164          					}
   2165          					
   2166          					break;
   2167          
   2168          				case SONY_CALL_FOCUS_POSITION:
   2169          					
   2170          					u_CFocusData = stSONY.RX_BUF[2];
   2171          					stSONY.FOCUS_CNT = u_CFocusData << 12;
   2172          					u_CFocusData = stSONY.RX_BUF[3];
   2173          					stSONY.FOCUS_CNT |= u_CFocusData << 8;
   2174          					u_CFocusData = stSONY.RX_BUF[4];
   2175          					stSONY.FOCUS_CNT |= u_CFocusData << 4;
   2176          					u_CFocusData = stSONY.RX_BUF[5];
   2177          					stSONY.FOCUS_CNT |= u_CFocusData;
   2178          
   2179          					if(stSONY.RX_BUF[6] == 0x00) 
   2180          					{
   2181          						stUART4.RX_REQUEST_ERROR = 0x01;
   2182          					}
   2183          					else 
   2184          					{
   2185          						stUART4.RX_REQUEST_COMPLETE = SONY_CALL_FOCUS_POSITION;
   2186          						stUART4.RX_REQUEST_CMD = 0x00;
   2187          					}
   2188          
   2189          					break;
   2190          
   2191          				default:
   2192          
   2193          					stUART4.RX_ENABLE = 0x00;
   2194          					if(stUART4.RX_REQUEST_ERROR != 0x00) stUART4.RX_REQUEST_ERROR = 0x00;
   2195          					
   2196          					break;
   2197          			}
   2198          
   2199          			SONY_Data_Clear();
   2200          			
   2201          			stSONY.RX_CNT = 0;
   2202          			stSONY.RX_END_FLAG = 0x01;
   2203          
   2204          		}
   2205          	}
   2206          	
   2207          	if(stSONY.RX_END_FLAG == 0x00)
   2208          	{
   2209          		// Second Data Check
   2210          		if((u_buf == SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2211          		{
   2212          			stSONY.RX_BUF[1] = SONY_BYTE_2;
   2213          			stSONY.RX_CNT++;
   2214          		}
   2215          		else if((u_buf != SONY_BYTE_2) && (stSONY.RX_CNT == 1))
   2216          		{
   2217          			stSONY.RX_BUF[0] = 0x00;
   2218          			stSONY.RX_CNT = 0;
   2219          		}
   2220          
   2221          		// First Data Check
   2222          		if((u_buf == SONY_BYTE_1) && (stSONY.RX_CNT == 0))
   2223          		{
   2224          			stSONY.RX_BUF[0] = SONY_BYTE_1;
   2225          			stSONY.RX_CNT++;
   2226          		}
   2227          	}
   2228          	else
   2229          	{
   2230          		stSONY.RX_END_FLAG = 0x00;
   2231          	}
   2232          
   2233          	if(stSONY.RX_CNT >= SONY_RX_BUF_MAX)
   2234          	{
   2235          		stSONY.RX_CNT = 0;
   2236          		SONY_Data_Clear();
   2237          		
   2238          	}
   2239          }
