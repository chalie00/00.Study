   1780          /*******************************************************************************
   1781          * Function Name  : TSM_Data_Pasher
   1782          *******************************************************************************/
   1783          void TSM_Data_Pasher(u8 u_Buf)
   1784          {
   1785          	u8 i = 0;
   1786          	u8 u_Data_P1 = 0x00, u_Data_P2 = 0x00;
   1787          	u16 u16_Convert_Data = 0x0000;
   1788          	u8 u_Convert_Data  = 0x00;	
   1789          
   1790          	if(stUART4.RX_ENABLE == 0x01)
   1791          	{
   1792          		stUART4.RX_BUF[stUART4.RX_CNT++] = u_Buf;
   1793          
   1794          		if(stUART4.RX_BUF_MAX == 0x00)
   1795          		{
   1796          			switch(stUART4.RX_BUF[1])
   1797          			{
   1798          				case TSM_CALL_ALL_TEMPERATURE:
   1799          					stUART4.RX_BUF_MAX = 0x23;
   1800          					break;
   1801          
   1802          				case TSM_CALL_FIRMWARE_VERSION:
   1803          					stUART4.RX_BUF_MAX = 0x08;
   1804          					break;
   1805          
   1806          				case TCM_COOLER_RUNTIME_INQ:
   1807          					if(stINFO.THERMAL_MODEL == TCM_640)
   1808          					{
   1809          						stUART4.RX_BUF_MAX = 0x07;
   1810          					}
   1811          					break;
   1812          
   1813          				default:
   1814          					stUART4.RX_BUF_MAX = 0x06;
   1815          					break;
   1816          			}
   1817          		}
   1818          
   1819          		// Rx Data Check & Initialization
   1820          		if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf == 0xAF))
   1821          		{
   1822          			switch(stUART4.RX_BUF_MAX)
   1823          			{
   1824          				case 0x06:
   1825          
   1826          					switch(stUART4.RX_REQUEST_CMD)
   1827          					{
   1828          						case TSM_CALL_MODULE_STATUS:
   1829          
   1830          							if(stUART4.RX_BUF[1] == TSM_CALL_MODULE_STATUS)
   1831          							{
   1832          								u_Data_P1 = stUART4.RX_BUF[2];
   1833          
   1834          								if(u_Data_P1) 
   1835          								{
   1836          									stTSM.MODULE_ENABLE = 0x01;
   1837          								}
   1838          								
   1839          								stTSM.MODULE_STATUS = u_Data_P1;
   1840          								stINFO.MODULE_TYPE = TSM_MODULE;
   1841          
   1842          								stUART4.RX_REQUEST_CMD = 0x00;
   1843          							}
   1844          								
   1845          							break;
   1846          							
   1847          						case TSM_CALL_DIGITAL_ZOOM_POSITION:
   1848          
   1849          							if(stUART4.RX_BUF[1] == TSM_CALL_DIGITAL_ZOOM_POSITION)
   1850          							{
   1851          								u_Data_P1 = stUART4.RX_BUF[4];
   1852          								u16_Convert_Data |= u_Data_P1;
   1853          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1854          
   1855          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1856          
   1857          								stUART4.RX_REQUEST_CMD = 0x00;
   1858          							}
   1859          
   1860          							break;
   1861          
   1862          						case TSM_CALL_OPTICAL_ZOOM_POSITION:
   1863          							break;
   1864          
   1865          						case TSM_CALL_TOTAL_ZOOM_POSITION:
   1866          							
   1867          							if(stUART4.RX_BUF[1] == TSM_CALL_TOTAL_ZOOM_POSITION)
   1868          							{
   1869          								u_Data_P1 = stUART4.RX_BUF[2];
   1870          								u_Data_P2 = stUART4.RX_BUF[3];
   1871          
   1872          								u16_Convert_Data = u_Data_P1 << 8;
   1873          								u16_Convert_Data |= u_Data_P2;
   1874          
   1875          								stTSM.ZOOM_CNT = u16_Convert_Data;
   1876          
   1877          								stUART4.RX_REQUEST_CMD = 0x00;
   1878          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_TOTAL_ZOOM_POSITION;
   1879          							}
   1880          							
   1881          							break;
   1882          
   1883          						case TSM_CALL_FOCUS_POSITION:
   1884          
   1885          							if(stUART4.RX_BUF[1] == TSM_CALL_FOCUS_POSITION)
   1886          							{
   1887          								u_Data_P1 = stUART4.RX_BUF[2];
   1888          								u_Data_P2 = stUART4.RX_BUF[3];
   1889          
   1890          								u16_Convert_Data = u_Data_P1 << 8;
   1891          								u16_Convert_Data |= u_Data_P2;
   1892          
   1893          								stTSM.FOCUS_CNT = u16_Convert_Data;
   1894          
   1895          								stUART4.RX_REQUEST_CMD = 0x00;
   1896          								stUART4.RX_REQUEST_COMPLETE = TSM_CALL_FOCUS_POSITION;								
   1897          							}
   1898          							
   1899          							break;
   1900          
   1901          						case TSM_CALL_PRODUCT_IDENTIFICATION:
   1902          
   1903          							if(stUART4.RX_BUF[1] == TSM_CALL_PRODUCT_IDENTIFICATION)
   1904          							{
   1905          								// Camera Information Data Check							
   1906          								stINFO.THERMAL_MODEL = stUART4.RX_BUF[2];
   1907          								stINFO.THERMAL_LENS = stUART4.RX_BUF[4];
   1908          								
   1909          								stUART4.RX_REQUEST_CMD = 0x00;
   1910          							}
   1911          							
   1912          							break;
   1913          
   1914          						case TSM_CALL_CAMERA_INFORMATION:
   1915          
   1916          							if(stUART4.RX_BUF[1] == TSM_CALL_CAMERA_INFORMATION)
   1917          							{
   1918          								// Camera Information Data Check
   1919          								stINFO.THERMAL_VER_HIGH = stUART4.RX_BUF[3];
   1920          								stINFO.THERMAL_VER_LOW = stUART4.RX_BUF[4];
   1921          								
   1922          								stUART4.RX_REQUEST_CMD = 0x00;
   1923          							}
   1924          							
   1925          							break;
   1926          
   1927          						case TSM_CALL_FPGA_VERSION:
   1928          
   1929          							if(stUART4.RX_BUF[1] == TSM_CALL_FPGA_VERSION)
   1930          							{
   1931          								stINFO.FPGA_VERSION_HIGH = stUART4.RX_BUF[2];
   1932          								stINFO.FPGA_VERSION_LOW = stUART4.RX_BUF[3];
   1933          
   1934          								stUART4.RX_REQUEST_CMD = 0x00;
   1935          							}
   1936          							
   1937          							break;
   1938          							
   1939          						case TSM_CALL_TEMPERATURE_MODE:
   1940          
   1941          							if(stUART4.RX_BUF[1] == TSM_CALL_TEMPERATURE_MODE)
   1942          							{
   1943          								// Camera Information Data Check
   1944          								stINFO.THERMAL_TEMP_MODE = stUART4.RX_BUF[2];
   1945          								
   1946          								stUART4.RX_REQUEST_CMD = 0x00;
   1947          							}
   1948          							
   1949          							break;
   1950          
   1951          						case TCM_TOTAL_ZOOM_POSITION_INQ:
   1952          
   1953          							if(stUART4.RX_BUF[1] == TCM_TOTAL_ZOOM_POSITION_INQ)
   1954          							{
   1955          								u_Data_P1 = stUART4.RX_BUF[2];
   1956          								u_Data_P2 = stUART4.RX_BUF[3];
   1957          
   1958          								u16_Convert_Data = u_Data_P1 << 8;
   1959          								u16_Convert_Data |= u_Data_P2;
   1960          
   1961          								stTSM.ZOOM_DATA = u16_Convert_Data;
   1962          
   1963          								stTSM.ZOOM_CNT = stTSM.ZOOM_DATA;
   1964          
   1965          								stUART4.RX_REQUEST_CMD = 0x00;
   1966          							}
   1967          							
   1968          							break;
   1969          
   1970          						case TCM_GET_FW_UPDATE_INQ:
   1971          
   1972          							if(stUART4.RX_BUF[1] == TCM_GET_FW_UPDATE_INQ)
   1973          							{
   1974          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[2];
   1975          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[3];
   1976          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   1977          									
   1978          								stUART4.RX_REQUEST_CMD = 0x00;
   1979          							}
   1980          							
   1981          							break;
   1982          
   1983          						case TCM_GET_ENGINE_VERSION:
   1984          
   1985          							if(stUART4.RX_BUF[1] == TCM_GET_ENGINE_VERSION)
   1986          							{
   1987          								stINFO.THERMAL_ENGINE_VER_HW = stUART4.RX_BUF[2];
   1988          								stINFO.THERMAL_ENGINE_VER_FW = stUART4.RX_BUF[3];
   1989          
   1990          								stUART4.RX_REQUEST_CMD = 0x00;
   1991          							}
   1992          							
   1993          							break;		
   1994          
   1995          						case TCM_FOCUS_POSITION_INQ:
   1996          
   1997          							if(stUART4.RX_BUF[1] == TCM_FOCUS_POSITION_INQ)
   1998          							{
   1999          								u_Data_P1 = stUART4.RX_BUF[2];
   2000          								u_Data_P2 = stUART4.RX_BUF[3];
   2001          
   2002          								u16_Convert_Data = u_Data_P1 << 8;
   2003          								u16_Convert_Data |= u_Data_P2;
   2004          
   2005          								stTSM.FOCUS_CNT = u16_Convert_Data;
   2006          
   2007          								stUART4.RX_REQUEST_CMD = 0x00;
   2008          							}
   2009          
   2010          							break;	
   2011          
   2012          						default:
   2013          							break;
   2014          					}
   2015          						
   2016          					break;
   2017          
   2018          				case 0x07:
   2019          					switch(stUART4.RX_REQUEST_CMD)
   2020          					{
   2021          						case TCM_COOLER_RUNTIME_INQ:
   2022          
   2023          								stINFO.THERMAL_ENGINE_TIME_DAY= 0x0000;
   2024          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[2]<<8);
   2025          								stINFO.THERMAL_ENGINE_TIME_DAY |= (stUART4.RX_BUF[3]&0xFF);
   2026          
   2027          								stINFO.THERMAL_ENGINE_TIME_HOUR = 0x0000;
   2028          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[4]<<8);
   2029          								stINFO.THERMAL_ENGINE_TIME_HOUR |= (stUART4.RX_BUF[5]&0xFF);
   2030          
   2031          								stINFO.THERMAL_ENGINE_DAY_HIGH = stUART4.RX_BUF[2];
   2032          								stINFO.THERMAL_ENGINE_DAY_LOW = stUART4.RX_BUF[3];
   2033          								stINFO.THERMAL_ENGINE_HOUR_HIGH = stUART4.RX_BUF[4];
   2034          								stINFO.THERMAL_ENGINE_HOUR_LOW = stUART4.RX_BUF[5];								
   2035          								
   2036          								stINFO.TCM640_ENGINE_RUNTIME_COMPLETE = TRUE;
   2037          								
   2038          								stUART4.RX_REQUEST_CMD = 0x00;	
   2039          
   2040          							break;
   2041          					}
   2042          					break;
   2043          					
   2044          				case 0x08:
   2045          					
   2046          					switch(stUART4.RX_REQUEST_CMD)
   2047          					{
   2048          						case TSM_CALL_FIRMWARE_VERSION:
   2049          
   2050          							if(stUART4.RX_BUF[1] == TSM_CALL_FIRMWARE_VERSION)
   2051          							{
   2052          								// Camera Information Data Check
   2053          								stINFO.THERMAL_DAY = stUART4.RX_BUF[4];
   2054          								stINFO.THERMAL_MONTH = stUART4.RX_BUF[5];
   2055          								stINFO.THERMAL_YEAR = stUART4.RX_BUF[6];
   2056          								
   2057          								stUART4.RX_REQUEST_CMD = 0x00;
   2058          							}
   2059          							
   2060          							break;
   2061          
   2062          						default:
   2063          							break;
   2064          					}
   2065          							
   2066          					break;
   2067          					
   2068          				case 0x23:
   2069          					
   2070          					//stTEMP.TEMPERATURE_READ_COMPLETE = 0x01;	// 온도 체크 데이터 수신 완료.
   2071          					
   2072          					break;
   2073          
   2074          				default:
   2075          					break;
   2076          			}
   2077          		
   2078          			for(i=0;i<stUART4.RX_BUF_MAX;i++)
   2079          			{
   2080          				//stTEMP.RX_BUF[i] = stUART4.RX_BUF[i];
   2081          				stUART4.RX_BUF[i] = 0x00;
   2082          			}
   2083          
   2084          			stUART4.RX_ENABLE = 0x00;
   2085          			stUART4.RX_CNT = 0x00;
   2086          			stUART4.RX_BUF_MAX = 0x00;
   2087          			stUART4.RX_PARITY = 0x01;
   2088          			
   2089          		}
   2090          		else if((stUART4.RX_CNT == stUART4.RX_BUF_MAX) && (u_Buf != 0xAF))
   2091          		{
   2092          			for(i=0;i<stUART4.RX_CNT;i++)
   2093          			{
   2094          				stUART4.RX_BUF[i] = 0x00;
   2095          			}
   2096          
   2097          			stUART4.RX_ENABLE = 0x00;
   2098          			stUART4.RX_CNT = 0x00;
   2099          			stUART4.RX_BUF_MAX = 0x00;
   2100          			stUART4.RX_REQUEST_ERROR = 0x01;			
   2101          		}
   2102          	}
   2103          
   2104          	if((stUART4.RX_ENABLE == 0x00) && (u_Buf == 0xA0))
   2105          	{
   2106          		stUART4.RX_BUF[0] = u_Buf;
   2107          		stUART4.RX_CNT++;
   2108          		stUART4.RX_ENABLE = 0x01;
   2109          	}
   2110          }
