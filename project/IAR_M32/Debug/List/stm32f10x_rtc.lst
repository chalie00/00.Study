    187          /**
    188            * @brief  Gets the RTC divider value.
    189            * @param  None
    190            * @retval RTC Divider value.
    191            */
    192          uint32_t RTC_GetDivider(void)
    193          {
    194            uint32_t tmp = 0x00;
    195            tmp = ((uint32_t)RTC->DIVH & (uint32_t)0x000F) << 16;
    196            tmp |= RTC->DIVL;
    197            return tmp;
    198          }
