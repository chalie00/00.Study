    206          void RTC_WaitForLastTask(void)
    207          {
    208            /* Loop until RTOFF flag is set */
    209            while ((RTC->CRL & RTC_FLAG_RTOFF) == (uint16_t)RESET)
    210            {
    211            }
    212          }
